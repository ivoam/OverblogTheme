		}
		
		/* Index Posts */
		.index .bx-window {
			padding-bottom: 0px;
			padding-top: 0px;
		}
		
		.index .bx-wrapper,
		.index .slider .posts {
			position: relative;
			margin-top: 20px;
			width: 100%;
		}

		.index .slider .posts {
			height: 100px;
			overflow:hidden;
		}

		.index .bx-window .posts {
			background: transparent;
			padding: 0;
			-webkit-border-radius: none;
			   -moz-border-radius: none;
			        border-radius: none;
		}

		/* Single Posts */
		.sections .posts .item-content h1.title,
		.single .posts .item-content h1.title {
			font-size: 1.8em;
			margin-bottom: 10px;
		}
		.sections .posts .item-content .body,
		.single .posts .item-content .body 
		{
			line-height: 1.4em;
			margin-top: 15px;
			width: 600px;
			margin-left: auto;
			margin-right: auto;
		}
		
		.sections .posts .item-content .body p,
		.single .posts .item-content .body p {
			margin-bottom: 10px;
		}

		h1,
		h2,
		h3 {
			color: {{ Custom('name_color')|default('rgba(255,255,255,0.9)') }};
			font-weight: bold;
			margin-bottom: 10px;
		}

		h1 {
			font-size: 1.4em;
		}

		h2 {
			font-size: 1.3em;
		}

		h3 {
			font-size: 1.2em;
		}

		.ob-text h2,
		.ob-text h3,
		.ob-text h4 {
			margin-bottom: 5px;
		}

		.ob-text ul,
		.ob-text ol,
		.ob-text p {
			margin-bottom: 10px;
		}

		.ob-text strong {
			font-weight: bold;
		}

		.ob-text ul li {
			list-style-type:disc;
			margin-left: 30px;
		}
		
		.ob-text ol li {
			list-style-type:decimal;
			margin-left: 30px;
		}

		.ob-text ol li p,
		.ob-text ul li p {
			margin-bottom: 0;
		}

		.ob-desc {
			font-style: italic;
		}

		/* Sections */
		.ob-section {
			clear: both;
			width: 600px;
			margin-left: auto;
			margin-right: auto;
		}
		
		.ob-section-text,
		.ob-section-video,
		.ob-section-link,
		.ob-section-quote,
		.ob-section-html {
			margin-bottom: 20px;
		}

		.sections .posts .item-content .body .ob-section-images,
		.ob-section-images {
			margin-bottom: 30px;
		}

		.sections .posts .item-content .body .ob-desc,
		.ob-desc {
			font-style: italic;
			margin-top: 5px;
		}

		/* Text */
		.sections .posts .item-content .body .ob-section-text,
		.ob-section-text {
			text-align: justify;
		}

		.sections .posts .item-content .body .ob-section-text img,
		.ob-section-text img {
			margin-top: 5px;
		}
		
		.sections .posts .item-content .body .ob-section-text pre,
		.ob-section-text pre {
			margin-top: 10px;
			margin-bottom: 10px;
		}

		/* Images */
		.ob-section-images {
			width: 600px;
			margin-left: auto;
			margin-right: auto;
		}

		/* Link */
		.ob-section-link .ob-title {
			font-size: 16px;
			margin-bottom: 4px;
		}

		.ob-section-link .ob-title a {
			font-size: 14px;
			font-weight: bold;
		}

		.ob-section-link .ob-link.ob-pull-left {
			float: left;
		}

		.ob-section-link .ob-media-left {
			max-width: 100%;
		}
		.ob-section-link .ob-snippet {
			color: {{ Custom('biography_color')|default('rgba(255,255,255,0.9)') }};
		}

		.ob-section-link .ob-ctn-image img {
			-webkit-border-radius: {{ Custom('block_radius')|default('10') }}px;
			   -moz-border-radius: {{ Custom('block_radius')|default('10') }}px;
			        border-radius: {{ Custom('block_radius')|default('10') }}px;
		}
		.ob-section-link .ob-desc {
			clear: none;
		}

		/* Map */
		.ob-section-video,
		.ob-section-video iframe,
		.ob-section-video object,
		.ob-section-map {
			width: 600px;
			margin-left: auto;
			margin-right: auto;
		}
		
		/* Audio */
		.sections .posts .item-content .body .obsoundplayer .obsoundplayername,
		.obsoundplayer .obsoundplayername {
		width: 235px;
		}
		/* end Sections */

		/* Comments */
		.single .posts .item-comments {
			margin-top: 20px;
		}
		.single .posts .item-comments iframe,
		.single .posts .item-comments .fb-comments,
		.single .posts .item-comments .fb-comments iframe {
			margin-left: auto;
			margin-right: auto;
			width: 600px;
			display: block;
		}
		
		/* Contact form */
		.special input, .special textarea {
			color: {{ Custom('biography_color')|default('rgba(255,255,255,0.9)') }};
			border: none;
			background-color: #{% if Custom('block_variant') == 'light' %}b2b2b2{% else %}4d4d4d{% endif %};
			background: rgba({% if Custom('block_variant') == 'light' %}0,0,0{% else %}255,255,255{% endif %},0.30);
			padding: 4px 4px 4px 4px;
			-webkit-border-radius: 5px;
			   -moz-border-radius: 5px;
			        border-radius: 5px;
		}
		.special input {
			min-height: 20px;
		}
		
		/* Navigation */
		.navigation {
			margin-top: 20px;
			text-align: center;
		}
		.navigation .back {
			display: block;
			text-align: center;
		}

		.navigation,
		.navigation a {
			font-weight: bold;
		}
		
		.navigation .prev,
		.navigation .next {
			margin-bottom: 20px;
		}
		.navigation a {
			font-size: 12px;
		}
		.navigation .prev {
			float: left;
		}
		.navigation .next {
			float: right;
		}

		.footer {
			clear: both;
			margin-left: {{ Custom('block_left_margin')|default('2%') }};
			margin-right: {{ Custom('block_right_margin')|default('0px') }};
			color: {{ Custom('links_color')|default('whiteSmoke') }};
		}

		.footer p {
			{% if Custom('block_align') == 'right' %}
			text-align: right;
			{% elseif Custom('block_align') == 'center' %}
			text-align: center;
			{% else %}
			text-align: left;
			{% endif %}
		}

		.footer p,
		.footer a {
			color: {{ Custom('links_color')|default('whiteSmoke') }};
		}
		.footer span {
			display: inline-block;
			margin-left: 4px;
			margin-right: 4px;
		}
	</style>
	
	<!-- End CSS -->

	<!-- shortcut:[Javascript] -->
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js" type="text/javascript"></script>
	
	{% if Custom('background_repeat') == 'strech' %}
	<!-- Backstretch -->
	<script src="http://assets.over-blog-kiwi.com/themes/37/js/jquery.backstretch.min.js" type="text/javascript"></script>
	{% endif %}

	{% if isSingle %}
	<!-- Fancybox -->
	<script src="http://assets.over-blog-kiwi.com/themes/jquery/fancybox/jquery.fancybox-1.3.4.pack.js" type="text/javascript"></script>
	<link rel="stylesheet" type="text/css" href="http://assets.over-blog-kiwi.com/themes/jquery/fancybox/jquery.fancybox-1.3.4.css" media="screen" />
	{% endif %}
	
	{% if isIndex %}
	<!-- Favicon fetcher -->
	<script src="http://assets.over-blog-kiwi.com/themes/37/js/jquery.favicons.js" type="text/javascript"></script>
	
	<!-- bxSlider -->
	<script src="http://assets.over-blog-kiwi.com/themes/37/js/jquery.bxSlider.min.js" type="text/javascript"></script>
	{% endif %}

	<script src="http://assets.over-blog-kiwi.com/themes/37/js/modernizr.js" type="text/javascript"></script>

	<!--[if lt IE 9]>
		<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
	<![endif]-->
	<!-- End Javascript -->
</head>
<body>
	<div id="fb-root"></div>
	<!-- shortcut:[Main] -->
	<div class="main {% if isSingle %}single{% elseif isIndex %}index{% elseif isSearch %}search{% elseif isTag %}tag{% endif %}{% if isIndex and Custom('nb_sections') == 'Slider' %} slider{% else %} sections{% endif %}">
		{% if isIndex %}
		<section class="profile">
			{% if Custom('bio_photo_type') is not empty %}
			<div class="portait">
				{% if Custom('bio_photo_type') == 'Uploaded photo' and Custom('bio_photo') is not empty %}
				<img src="{{ Custom('bio_photo')|resize(Custom('bio_photo_size')|default('200'))|crop }}" title="{{ Custom('bio_name') }}" alt="{{ Custom('bio_name') }}" />
				{% elseif Blog.Owner.Avatar is not empty %}
				<img src="{{ Blog.Owner.Avatar|resize(Custom('bio_photo_size')|default('200'))|crop }}" title="{{ Custom('bio_name') }}" alt="{{ Custom('bio_name') }}" />
				{% elseif Blog.Owner.Avatar is empty %}
				<img src="{{ 'http://assets.over-blog-kiwi.com/themes/37/images/avatar.png'|resize(Custom('bio_photo_size')|default('200'))|crop }}" title="{{ Custom('bio_name')|default(Blog.Owner.Nickname) }}" alt="{{ Custom('bio_name')|default(Blog.Owner.Nickname) }}" />
				{% endif %}
			</div>
			{% endif %}

			<h1>{{ Custom('bio_name')|default(Blog.Owner.Nickname) }}</h1>
			<h2>{{ Custom('bio_headline')|default(Blog.Description) }}</h2>

			<div class="about">
				{{ Custom('bio_desc')|default(Blog.Owner.Bio) }}
			</div>

			<ul class="social">
				{% if Custom('linkedin_account') is not empty %}
				<li class="linkedin"><a href="http://www.linkedin.com/in/{{ Custom('linkedin_account') }}" title="{{ Lang.Get('See the profile on ') }} {{ Lang.Get('Linkedin') }}" target="_blank"></a></li>
				{% endif %}
				{% if Custom('twitter_account') is not empty %}
				<li class="twitter"><a href="http://twitter.com/#!/{{ Custom('twitter_account') }}" title="{{ Lang.Get('See the profile on') }} {{ Lang.Get('Twitter') }}" target="_blank"></a></li>
				{% endif %}
				{% if Custom('facebook_account') is not empty %}
				<li class="facebook"><a href="http://www.facebook.com/{{ Custom('facebook_account') }}" title="{{ Lang.Get('See the profile on') }} {{ Lang.Get('Facebook') }}" target="_blank"></a></li>
				{% endif %}
				{% if Custom('foursquare_account') is not empty %}
				<li class="foursquare"><a href="http://foursquare.com/{{ Custom('foursquare_account') }}" title="{{ Lang.Get('See the profile on ') }} {{ Lang.Get('Foursquare') }}" target="_blank"></a></li>
				{% endif %}
				{% if Custom('flickr_account') is not empty %}
				<li class="flickr"><a href="http://www.flickr.com/people/{{ Custom('flickr_account') }}" title="{{ Lang.Get('See the profile on ') }} {{ Lang.Get('Flickr') }}" target="_blank"></a></li>
				{% endif %}
				{% if Custom('youtube_account') is not empty %}
				<li class="youtube"><a href="http://www.youtube.com/user/{{ Custom('youtube_account') }}" title="{{ Lang.Get('See the profile on ') }} {{ Lang.Get('Youtube') }}" target="_blank"></a></li>
				{% endif %}
			</ul>
			<ul class="blogroll">
				{% if Custom('blogroll_1') is not empty %}
				<li><a href="{{ Custom('blogroll_1') }}" title="{{ Custom('blogroll_1') }}" target="_blank">{{ Custom('blogroll_1') }}</a></li>
				{% endif %}
				{% if Custom('blogroll_2') is not empty %}
				<li><a href="{{ Custom('blogroll_2') }}" title="{{ Custom('blogroll_2') }}" target="_blank">{{ Custom('blogroll_2') }}</a></li>
				{% endif %}
				{% if Custom('blogroll_3') is not empty %}
				<li><a href="{{ Custom('blogroll_3') }}" title="{{ Custom('blogroll_3') }}" target="_blank">{{ Custom('blogroll_3') }}</a></li>
				{% endif %}
				{% if Custom('blogroll_4') is not empty %}
				<li><a href="{{ Custom('blogroll_4') }}" title="{{ Custom('blogroll_4') }}" target="_blank">{{ Custom('blogroll_4') }}</a></li>
				{% endif %}
				{% if Custom('blogroll_5') is not empty %}
				<li><a href="{{ Custom('blogroll_5') }}" title="{{ Custom('blogroll_5') }}" target="_blank">{{ Custom('blogroll_5') }}</a></li>
				{% endif %}
			</ul>
			<div class="likeboxes">
				<h3>{{ Custom('sharing_title')|default('Share me!') }}</h3>
				<div class="share share-facebook">
					<div class="fb-like" data-href="{{ Blog.Url }}" data-send="false" data-layout="button_count" data-width="100" data-show-faces="false"></div>
				</div>
				<div class="share share-twitter">
					<a href="https://twitter.com/share" class="twitter-share-button" data-text="{{ Blog.Title }}" data-url="{{ Blog.Url }}" data-lang="{{ Blog.Lang }}" {% if Custom('twitter_account') is not empty %}data-via="{{Custom('twitter_account')}}{% endif %}">Tweet</a>
				</div>
				<div class="share share-google">
					<g:plusone size="medium"></g:plusone>
					<script type="text/javascript">
			  	(function() {
			    	var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
			    	po.src = 'https://apis.google.com/js/plusone.js';
			    	var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
			  	})();
					</script>
				</div>
			</div>
			<div class="clear"></div>
		</section>
		{% endif %}
		<section class="posts">
			
			{% if isSpecial %}
				<div class="post special">
						<div class="item-content">
							<header>
								<hgroup>
									<h1 class="title">
										{{ Lang.Get('Contact') }}
									</h1>
								</hgroup>
							</header>
							<div class="body">
								{{ SpecialContent }}
							</div>
							<div class="clear"></div>
						</div>
				</div>
				<nav class="navigation">
					<p class="back"><a href="/">{{ Lang.Get('Back to home') }}</a></p>
				</nav>
			{% else %}
				{% if isSearch %}
				<h2 class="list-title">{{ Lang.Get('Search result for:') }} {{ Search.Keyword|title }}</h2>
				{% elseif isTag and Tag is not empty %}
				<h2 class="list-title">{{ Lang.Get('Tag:') }} {{ Tag.Title|title }}</h2>
				{% endif %}
				{% list Posts %}
				<!-- Post item -->
				<div class="post">
						{% if isSingle == false and Post.Cover is not empty and Custom('nb_sections') == "Slider" %}
						<div class="item-cover">
							<a href="{{Post.Permalink}}" title="{{Post.Title}}" class="border-img">
							<img src="{{ Post.Cover|resize(80)|crop }}" alt="{{Post.Title}}" title="{{Post.Title}}" />
							</a>
						</div>
						{% endif %}
						<div class="item-content">
							<header>
								<hgroup>
									{% if isSingle %}
									<h1 class="title">
										{% if Post.Title is not empty %}{{Post.Title}}{% else %}{% list Post.Sources %}{% if loop.first %}{{ Lang.Get('A post from') }} {{ Lang.Get(Source) }}
										{% endif %}{% endlist %}{% endif %}
									</h1>
									{% else %}
									<h3 class="title">
										<a href="{{Post.Permalink}}" title="{{Post.Title}}">{% if Post.Title is not empty %}{{Post.Title}}{% else %}{% list Post.Sources %}{% if loop.first %}{{ Lang.Get('A post from') }} {{ Lang.Get(Source) }}
										{% endif %}{% endlist %}{% endif %}</a>
									</h3>
									{% endif %}
									
									<div class="meta">
										<span class="author">{% if Post.Author is not empty %}{{ Post.Author }}{% endif %}</span>
										— <time pubdate>{{ Post.Date|date("M d, Y") }}</time>
										— {% list Post.Tags %}{% if loop.index != 1 %},&nbsp;{% endif %}<a href="{{ Tag.Url }}"  alt="{{ Tag.Title }}">{{ Tag.Title }}</a>{% endlist %}
									</div>

								</hgroup>
							</header>
							<div class="body">
								{% if isSingle or Custom('nb_sections') == "All" %}
								{{ Post.Body }}
								{% elseif Custom('nb_sections') == "Slider" %}
								{{Post.Snippet|truncate(125, true, '...')}} <a href="{{Post.Permalink}}" alt="{{ Lang.Get('Read more') }}">{{ Lang.Get('Read more') }}</a>
								{% else %}
								{{Post.Body(Custom('nb_sections'))}} <a href="{{Post.Permalink}}" alt="{{ Lang.Get('Read more') }}">{{ Lang.Get('Read more') }}</a>
								{% endif %}
							</div>
							{% if isSingle %}
							<div class="likeboxes clear">
								<div class="share share-facebook">
									<div class="fb-like" data-href="{{ Post.Permalink }}" data-send="true" data-layout="button_count" data-width="150" data-show-faces="false" data-font="lucida grande" data-colorscheme="{% if Custom('block_variant') == 'light' %}light{% else %}dark{% endif %}"></div>
								</div>
								<div class="share share-twitter">
									<a href="https://twitter.com/share" class="twitter-share-button"  data-text="{% if Post.Title is not empty %}{{ Post.Title }}{% elseif Post.Snippet is not empty %}{{ Post.Snippet}}{% else %}{{ Blog.Title }}{% endif %}" data-url="{{ Post.Permalink }}" data-lang="{{ Blog.Lang }}"{% if Custom('twitter_account') is not empty %} data-via="{{Custom('twitter_account')}}"{% endif %}></a>
								</div>
								<div class="share share-google">
									<g:plusone size="medium"></g:plusone>
								</div>
							</div>
							
							<div class="item-comments clear">
								{% if Custom("comment") == "Facebook" %}
								<div class="fb-comments" data-href="{{ Post.Permalink }}" data-num-posts="2" data-width="600"></div>
								{% elseif Custom("comment") == "Disqus" and Custom('comment_disqus') is not empty %}
									<div id="disqus_thread"></div>
									<script type="text/javascript">
									var disqus_shortname = '{{ Custom('comment_disqus') }}';

									(function() {
									var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
									dsq.src = 'http://' + disqus_shortname + '.disqus.com/embed.js';
									(document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
									})();
									</script>
									<noscript>Please enable JavaScript to view the <a href="http://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
								{% else %}
									{{Post.Comments}}
								{% endif %}
							</div>
							{% endif %}
						</div>
				</div>
				<!-- End Post item -->
				{% else %}
				<div class="post">
					<div class="item-content">
						<header>
							<hgroup>
								<h3 class="title">
									{% if (isSearch or isTag) and loop.length == 0 %}
									{{ Lang.Get('No result for: ')}}{{Search|default(Tag.Title)}}
									{% else %}
									{{ Lang.Get('No more post.')}}
									{% endif %}
								</h3>
							</hgroup>
						</header>
					</div>
				</div>
				{% endlist %}
				
				<div class="clear"></div>
				
				
				<!-- Pagination -->
				<nav class="navigation">
				{% if Navigation.PreviousPage is not empty or Navigation.PreviousPost is not empty %}
				
					{% if Navigation.PreviousPage is not empty %}
					<a href="{{Navigation.PreviousPage}}" class="prev">{{ Lang.Get('Previous page') }}</a>
					{% endif %}

					{% if Navigation.PreviousPost is not empty %}
					<a href="{{Navigation.PreviousPost.Permalink}}" class="prev">{{ Lang.Get('Previous post') }}</a>
					{% endif %}
				{% endif %}

				{% if Navigation.NextPage is not empty or Navigation.NextPost is not empty %}
					{% if Navigation.NextPage is not empty %}
					<a href="{{Navigation.NextPage}}" class="next">{{ Lang.Get('Next page') }}</a>
					{% endif %}

					{% if Navigation.NextPost is not empty %}
					<a href="{{Navigation.NextPost.Permalink}}" class="next">{{ Lang.Get('Next post') }}</a>
					{% endif %}
				{% endif %}
				
					<a href="/" class="back">{{ Lang.Get('Back to home') }}</a>
				</nav>
				<!-- End Pagination -->
			{% endif %}
		</section>
	</div>
	
	<!-- shortcut:[Footer] -->
	<footer class="footer clear">
		<p>
		{% if Custom('page_1_title') and Custom('page_1_link') %}
		<span><a href="{{ Custom('page_1_link') }}" alt="{{ Custom('page_1_title') }}">{{ Custom('page_1_title') }}</a></span> - 
		{% endif %}
		{% if Custom('page_2_title') and Custom('page_2_link') %}
		<span><a href="{{ Custom('page_2_link') }}" alt="{{ Custom('page_2_title') }}">{{ Custom('page_2_title') }}</a></span> - 
		{% endif %}
		<span>{{Custom('wording_footer')|default(Lang.Get('Theme:') ~ ' Resume &copy; 2012') }}</span>
		</p>
	</footer>
	<!--End Footer-->

	<!-- Twitter Script -->
	<script type="text/javascript">!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="//platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs); }}(document,"script","twitter-wjs");</script>

	<!-- Facebook Script -->
	<script type="text/javascript">(function(d, s, id) {
		var js, fjs = d.getElementsByTagName(s)[0];
		if (d.getElementById(id)) return;
		js = d.createElement(s); js.id = id;
		js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId={{ Custom('facebook_appid')|default('127607930619601') }}";
		fjs.parentNode.insertBefore(js, fjs);
		}(document, 'script', 'facebook-jssdk'));
	</script>

	<!-- Google plus -->
	<script type="text/javascript">
	(function() {
  	var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
  	po.src = 'https://apis.google.com/js/plusone.js';
  	var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
	})();
	</script>

	<!-- Scripts -->
	<script type="text/javascript">
		$(document).ready(function() {
			
			{% if Custom('background_repeat') == 'strech' %}
			//  Backstrech
			$.backstretch(
				$('html').css('background-image').replace('url(','').replace(')','')
			);
			{% endif %}
			
			{% if isIndex %}
			// Favicon on blogroll
			$('.blogroll li').favicons({insert: 'insertBefore'});
			$('.blogroll li a').each(function()
			{
				$(this).html($(this).html().replace('http://','').replace('www.', ''));
			});
			{% endif %}

			{% if isIndex and Custom('nb_sections') == 'Slider' %}
			$(".posts").bxSlider({
				controls: false,
				mode: 'vertical',
				infiniteLoop: true,
				auto: true,
				autoHover: true,
				pause: 4000
			});
			{% endif %}
			
			{% if isSingle %}
			$(".ob-section-images a").attr("rel", "group1");
			$(".ob-section-images a").fancybox({
				'overlayShow'	: false,
				'transitionIn'	: 'elastic',
				'transitionOut'	: 'elastic',
				'type'	: 'image'
			});
			{% endif %}
		});
	
	
		$(window).load(function() {
				$('.item-cover .border-img').each(function()
				{
					var prev = $(this).prev('img');
					$(this).css('width', prev.css('width'));
					$(this).css('height', prev.css('height'));
					$(this).show();
				});
						// enhance video
				$('.single .item-content .body .ob-section-video iframe').each(function(){
					$(this).css('width', 600);
				});
				$('.single .item-content .body .ob-section-images a').click(function(){
					 event.preventDefault();
				});
		});
	</script>

