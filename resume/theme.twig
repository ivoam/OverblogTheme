		 /**
		 * Theme
		 */
		html {
			{% if Custom('background_type') != 'No' %}
			/* Copyright : http://www.flickr.com/photos/mikebaird/4178332772/ */
			background: url('http://assets.over-blog-kiwi.com/themes/37/images/default_background.jpg') no-repeat bottom right fixed;
			
			{% if Custom('background_custom') is not empty %}
			background: url({{ Custom('background_custom') }}) no-repeat center center fixed;
			{% endif %}
			{% if Custom('background_repeat') == 'fixed' %}
			background-repeat: {{ Custom('background_repeat') }};
			{% elseif Custom('background_repeat') == 'strech' or Custom('background_repeat') is empty %}
			-webkit-background-size: cover;
			   -moz-background-size: cover;
			     -o-background-size: cover;
			        background-size: cover;
			{% endif %}

			{% if Custom('background_position') is not empty %}
			background-position: {{ Custom('background_position')|default('left right') }};
			{% endif %}
			{% endif %}
			
