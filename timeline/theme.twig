		<script type="text/javascript">
			// FancyBox
			$(".ob-section-images a, .ob-link-img").attr("rel", "group1");
			$(".ob-section-images a, .ob-link-img").fancybox({
				'overlayShow'	: true,
				'overlayColor'  : '#000',
				'transitionIn'	: 'elastic',
				'transitionOut'	: 'elastic',
				'type'			: 'image'
			});


			// Placeholder Script
			$('input[placeholder]').each(function(){
			    var input = $(this);
			    $(input).val(input.attr('placeholder'));
			    $(input).focus(function(){
			         if (input.val() == input.attr('placeholder')) {
			             input.val('');
			         }
			    });
			    $(input).blur(function(){
			        if (input.val() == '' || input.val() == input.attr('placeholder')) {
			            input.val(input.attr('placeholder'));
			        }
			    });
			});

			// Search form hack
			$('form.search').submit(function(e){
				e.preventDefault();
				window.location = $(this).attr('action') + $(this).find('input').attr('value');
				return false;
			});


		{% if Custom('background_repeat') == 'strech' and Custom('background_image') is not empty %}
			// Backstretch
			$(document).ready(function(){
				//  Backstrech
				$.backstretch(
					'{{ Custom('background_image') }}'
				);
				$('#wrapper-container').css('background', 'none');
			});
		{% endif %}

			var Timeline = new Array(),
				elsStart;

			Timeline.EnhancePictures = function (elements) {
				$(elements).find('.ob-section-text img, .ob-section-images img').each(function(){
					$(this).attr(
						'src',
						$(this).attr('src').replace(/\/[0-9]+x[0-9]+(\-ct)?/, "/408x306-ct")
					);
				});
			},
			Timeline.ItemDispatcher = function(elements)
			{
				$(elements).hide();

				for (var i = 0, len = elements.length; i < len; i++)
				{
					if ($(".items.left").height() <= $(".items.right").height())
					{
						$(elements[i]).appendTo(".items.left");
					}
					else
					{
						$(elements[i]).appendTo(".items.right");
					}
					$(elements[i]).show();
				}
			}
			$(document).ready(function(){
				elsStart = $('.item');
				Timeline.EnhancePictures(elsStart);
				Timeline.ItemDispatcher(elsStart);

				jQuery.ias({
					'container' 	: ".items-wrapper",
					'item' 			: ".item",
					'pagination' 	: ".load-more",
					'next'			: ".load-more a",
					'tresholdMargin': -250,
					'loader' 		: "<div class='loader'><img src='http://assets.over-blog-kiwi.com/themes/13/images/loader.gif' alt='' title='' /></div>",
					onRenderComplete: function(items){
						Timeline.EnhancePictures(items);

						Timeline.ItemDispatcher(items);

						if (typeof OB != "undefined") { OB.restart && OB.restart(); }

						if (typeof FB != "undefined") { FB.XFBML.parse(document.body); }

						if (typeof twttr != "undefined") { twttr.widgets.load(); }

						if (typeof gapi != "undefined") { gapi.plusone.go(); }

						if (typeof DISQUSWIDGETS != "undefined" ){ DISQUSWIDGETS && DISQUSWIDGETS.getCount(); }

						$(".ob-section-images a, .ob-link-img").attr("rel", "fancybox");
						$("a[rel='fancybox']").fancybox({
							'overlayShow'	: true,
							'overlayColor'  : '#000',
							'transitionIn'	: 'elastic',
							'transitionOut'	: 'elastic',
							'type'			: 'image'
						});
					}
				});
			});
			$(window).load(function(){
				Timeline.ItemDispatcher(elsStart);
			});
		</script>
	</body>
