                                <div class="post-share">
                                    {{ Post.RepostButton('nocount') }}

                                    <div class="share">
                                        <a class="share-link" href="#" title="{{ Lang.Get('Share this post') }}">
                                            {% if Post.GetfirstSource == 'OB' or not isSocial %}
                                                {{ Lang.Get('Share this post') }}
                                            {% endif %}
                                        </a>
                                        <ul class="share-buttons">
                                            <li>
                                                <a class="gplus" href="https://plus.google.com/share?url={{ Post.Permalink }}">Google +</a>
                                            </li>
                                            <li>
                                                <a class="twt" href="https://twitter.com/share?url={{ Post.Permalink }}&text={% if Post.Title is not empty %}{{ Post.Title }}{% elseif Post.Snippet is not empty %}{{ Post.Snippet}}{% else %}{{ Blog.Title }}{% endif %}{% if Custom('twitter_username') is not empty %}&via={{ Custom('twitter_username') }}{% endif %}">Twitter</a>
                                            </li>
                                            <li>
                                                <a class="fb" href="https://www.facebook.com/sharer/sharer.php?u={{ Post.Permalink }}&t={% if Post.Title is not empty %}{{ Post.Title }}{% elseif Post.Snippet is not empty %}{{ Post.Snippet}}{% else %}{{ Blog.Title }}{% endif %}">Facebook</a>
                                            </li>
                                            {% if Post.Cover is not empty %}
                                                <li>
                                                    <a class="pint" href="//pinterest.com/pin/create/button/?url={{ Post.Permalink }}&media={{ Post.Cover }}&description={% if Post.Snippet is not empty %}{{ Post.Snippet }}{% else %}{{ Post.Title }}{% endif %}">Pinterest</a>
                                                </li>
                                            {% endif %}
                                        </ul>
                                    </div>
                                </div>
								<!-- Pagination -->
								{% if isSingle and not isPage and (Navigation.PreviousPost is not empty or Navigation.NextPost is not empty) %}
									<nav class="navigation-post clearfix">
										{% if Navigation.PreviousPost is not empty %}
											<a href="{{ Navigation.PreviousPost.Permalink }}" class="prev-post">&larr; {{ Lang.Get('Previous post') }}</a>
										{% endif %}

										{% if Navigation.NextPost is not empty %}
											<a href="{{ Navigation.NextPost.Permalink }}" class="next-post">{{ Lang.Get('Next post') }} &rarr;</a>
										{% endif %}
										<div class="clearboth"></div>
									</nav>
								{% endif %}
								<!-- End Pagination -->
                                {% if isSingle and not isPage and Post.ShowComments is not empty %}
                                    <div class="comments" id="comments">
                                        <h3>{{ Lang.Get('Comment this post') }}</h3>
                                        {% if Custom("comment") == "Facebook" %}
                                            <div class="fb-comments" data-href="{{ Post.Permalink }}" data-num-posts="4" data-width="460"></div>
                                        {% elseif Custom("comment") == "Disqus" and Custom('comment_disqus') is not empty %}
