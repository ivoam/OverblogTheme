                                            <noscript>Please enable JavaScript to view the <a href="http://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
                                        {% else %}
                                            {{ Post.Comments }}
                                        {% endif %}
                                    </div>
                                {% endif %}
                            </footer>
                        </article>
                    {% else %}
                        <!-- No post found -->
                        <article class="no-result">
                            <p>{{ Lang.Get('No post found') }}.</p>
                        </article>
                    {% endlist %}

                    {% if Navigation.NextPage is not empty %}
                        <div class="navigation{% if isSocial %} social-navigation{% endif%}">
                            <a class="next{% if isSocial %} social-next{% endif%}" href="{{ Navigation.NextPage }}">{{ Lang.Get('Next page') }} &raquo;</a>
                        </div>
                    {% endif %}
                </div>
            {% elseif isSpecial %}
                <div class="main">
                    <!-- Contact page -->
                    <article class="special">
	                    {{ SpecialContent }}
                    </article>
                </div>
            {% endif %}

            {% if not isSocial %}
                <!-- Social -->
                <div class="social">
                    {% list Blog.Posts(Custom('displayArticlesLimit'), ['OB'], false) %}
