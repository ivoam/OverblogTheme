                                            {% endif %}
                                        </ul>
                                    </div>
                                </div>
                            </footer>
                        </article>
                    {% endlist %}

                    <!-- Next page -->
                    <div class="social-navigation">
                        <a class="social-next" href="/social/not/ob/2">{{ Lang.Get('Next page') }} &raquo;</a>
                    </div>
                </div>
            {% endif %}
        </section>

        <!-- Blog footer -->
        <footer class="blog-footer">
            <div class="footer-content">
                <p class="credits">
                     Copyright 2013 - <a href="{{ Blog.Url }}" title="{{ Blog.Description }}">{{ Blog.Title }}</a>. {{ Lang.Get('All rights reserved') }}.
                </p>
                <p class="powered">
                    {{ Lang.Get('Proudly powered by') }} <a href="http://www.over-blog.com" target="_blank">OverBlog</a> - <a href="http://fr.over-blog-kiwi.com/terms-of-use" target="_blank">{{ Lang.Get('Terms of use') }}</a>
                </p>
            </div>
        </footer>

        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.1/jquery.min.js"></script>
            <script src="http://assets.over-blog-kiwi.com/themes/jquery/masonry/jquery.masonry.min.js"></script>
            <script src="http://assets.over-blog-kiwi.com/themes/jquery/infinite-ajax-scroll/jquery.ias.min.js"></script>
        {% if not isSpecial %}
            <script src="http://assets.over-blog-kiwi.com/themes/jquery/fancybox2.1.2/jquery.fancybox.pack.js"></script>
        {% endif %}

		<script>
            {% if Custom('google_analytics') is not empty %}
                // Google Analytics
                window.google_analytics_uacct = '{{Custom('google_analytics')}}';
                var _gaq = _gaq || [];
                    _gaq.push(['_setAccount', '{{Custom('google_analytics')}}']);
                    _gaq.push(['_trackPageview']);
                    _gaq.push(['_trackPageLoadTime']);

                (function() {
                    var ga = document.createElement('script');
                        ga.type = 'text/javascript';
                        ga.async = true;
                        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
                    var s = document.getElementsByTagName('script')[0];
                        s.parentNode.insertBefore(ga, s);
                })();
            {% endif %}

            {% if Custom('comment') == 'Disqus' and Custom('comment_disqus') is not empty %}
                // Disqus comments
                var disqus_shortname = "{{ Custom('comment_disqus') }}";
                (function() {
                    var dsq = document.createElement('script');
                        dsq.type = 'text/javascript';
                        dsq.async = true;
                        dsq.src = 'http://' + disqus_shortname + '.disqus.com/embed.js';
                    (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
                })();

                // Disqus comments counter
                var disqus_shortname = "{{ Custom('comment_disqus') }}";
                (function () {
                    var s = document.createElement('script');
                        s.type = 'text/javascript';
						s.async = true;
                        s.src = 'http://' + disqus_shortname + '.disqus.com/count.js';
                    (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(s);
                }());
            {% elseif Custom('comment') == 'Facebook' %}
                // Facebook comments
                (function(d, s, id) {
                    var js,
                        fjs = d.getElementsByTagName(s)[0];
                    if (d.getElementById(id)) return;
                        js = d.createElement(s);
                        js.id = id;
                        js.src = "//connect.facebook.net/{{ Blog.Lang}}_{{ Blog.Lang|upper }}/all.js#xfbml=1&amp;appId={{ Custom('facebook_appid')|default('284865384904712')}}";
                    fjs.parentNode.insertBefore(js, fjs);
                }(document, 'script', 'facebook-jssdk'));
            {% endif %}

            {% if isSingle %}
                // Disable tweets render
                TwitterDisabled = function(elements) {
                    $(elements).find(".twitter-tweet").removeClass("twitter-tweet");
                },

                //Twitter render
                !function(d,s,id){
                    var js, fjs = d.getElementsByTagName(s)[0];
                    if(!d.getElementById(id)){
                        js = d.createElement(s);
                        js.id = id;
                        js.src = "https://platform.twitter.com/widgets.js";
                        fjs.parentNode.insertBefore(js,fjs);
                    }
                }(document,"script","twitter-wjs");
            {% endif %}

            // Twitter controls
            var TwitterControls = function(elements) {
                $(elements).find(".ob-tweet").each(function() {
                    var datatwitter = $(this).find("a[data-twitter]").attr("data-twitter");

                    $(this).parent().parent().find(".tweet-reply").attr("href", "https://twitter.com/intent/tweet?in_reply_to=" + datatwitter);
                    $(this).parent().parent().find(".tweet-retweet").attr("href", "https://twitter.com/intent/retweet?tweet_id=" + datatwitter);
                    $(this).parent().parent().find(".tweet-favorite").attr("href", "https://twitter.com/intent/favorite?tweet_id=" + datatwitter);
                });
            },

            // Social popup
            SocialPopup = function(elements) {
                $(elements).find(".tweet-reply, .tweet-retweet, .tweet-favorite, .share-buttons a").click(function() {
                    var popup = window.open($(this).prop("href"), '',"width=650, height=400, scrollbars=0, left=" +((screen.width - 650)/2) + ", top=" + ((screen.height - 400)/2) );
                    if (window.focus) {
                        popup.focus();
                    }
                    return false;
                });
            }

            $(document).ready(function(){
                {% if isSingle %}
                    // Fancybox
                    $(".ob-link-img").attr("rel", "fancybox").fancybox({
                        type: "image"
                    });

                    TwitterDisabled($(".social .social-post"));
                {% endif %}

                SocialPopup($(".post, .social-post"));

                // Masonry
                var container = $(".social");

                TwitterControls($(".main"));
                TwitterControls($(".social"));

                container.imagesLoaded(function(){
                    container.masonry({
                        itemSelector : ".social .social-post",
                        columnWidth  : 240,
                        isAnimated   : false
                    });
                });

                // Load more with Masonry
                {% if not isSingle or not isSpecial %}
                    jQuery.ias({
                        container        : ".main",
                        item             : ".post",
                        pagination       : ".navigation",
                        next             : ".next",
                        loader           : "<img src='http://assets.over-blog-kiwi.com/themes/14/images/ajax_loader.gif' alt='' title='' width='40' /><p>{{ Lang.Get('Loading new posts') }}...</p>",
                        history          : false,
                        onRenderComplete : function() {
                            $('.navigation').removeClass('social-navigation');
                            $('.next').removeClass('social-next');

                            // OB callback
                            if (typeof OB != "undefined") {
                                OB.restart && OB.restart();
                            }
                            {% if Custom('comment') == 'Facebook' %}
                                // FB callback
                                if (typeof FB != "undefined") {
                                    FB.XFBML.parse(document.body);
                                }
                            {% elseif Custom('comment') == 'Disqus' %}
								// Disqus callback
								if (typeof DISQUSWIDGETS != "undefined" ){
									DISQUSWIDGETS && DISQUSWIDGETS.getCount();
								}
							{% endif %}
                        },
                        onLoadItems      : function(items) {
                            SocialPopup(items);
                            TwitterControls(items);
                        }
                    });
                {% endif %}

                jQuery.ias({
                    container        : ".social",
                    item             : ".social-post",
                    pagination       : ".social-navigation",
                    next             : ".social-next",
                    loader           : "<img src='http://assets.over-blog-kiwi.com/themes/14/images/ajax_loader.gif' alt='' title='' width='10' /><p>{{ Lang.Get('Loading new posts') }}...</p>",
                    history          : false,
                    onRenderComplete : function(){
                        $('.social-navigation').removeClass('navigation');
                        $('.social-next').removeClass('next');

                        // OB callback
                        if (typeof OB != "undefined") {
                            OB.restart && OB.restart();
                        }
                    },
                    onLoadItems       : function(items) {
                        var newElems = $(items);
                        newElems.css({
                            opacity: 0
                        });

                        newElems.imagesLoaded(function() {
                            newElems.animate({
                                opacity: 1
                            });
                            container.masonry("appended", newElems, false);
                        });

                        {% if isSingle %}
                            TwitterDisabled(items);
                        {% endif %}

                        SocialPopup(items);
                        TwitterControls(items);
                    }
                });
            });
        </script>
	</body>
</html>
