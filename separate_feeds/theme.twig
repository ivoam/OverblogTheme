            }
            .ob-section-images .ob-img {
                max-width: 100%;
                width: auto;
            }
            .ob-section-images .ob-row-2-col .ob-img {
                max-width: 240px;
            }
            .ob-section-images .ob-row-3-col .ob-img {
                max-width: 160px;
            }
            .ob-section-images .ob-slideshow {
                height: 380px;
                width: auto;
            }
            .ob-section-video iframe,
