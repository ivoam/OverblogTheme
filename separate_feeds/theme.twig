            .ob-section-video object {
                height: 300px;
                width: 100%;
            }
            .ob-section-map .ob-map,
            .ob-section-map .ob-desc {
                width: auto;
            }
            .ob-section-audio .obsoundplayer {
                height: 70px;
                overflow: hidden;
            }
            .ob-section-audio .obsoundplayer .obsoundplayername {
                width: 120px;
            }
            .ob-section-quote .ob-quote p::before {
                margin: -55px 0 0 -90px;
            }
            .ob-section-quote .ob-quote p::after {
                margin: -40px 0 0 0;
            }
            .ob-section-link .ob-ctn {
                padding: 10px;
