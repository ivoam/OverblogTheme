                font-weight: normal;
            }
            .ob-commentsList .ob-info{
                font-size: 12px;
            }
            .ob-commentsList .ob-avatar{
                -webkit-box-sizing: content-box;
                -moz-box-sizing: content-box;
                box-sizing: content-box;
            }
            /* ----------------------------- */
            /* ==SPECIAL PAGE */
            /* ----------------------------- */
            .special {
