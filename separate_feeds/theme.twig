    <body>
        <!-- Init Facebook script -->
		<div id="fb-root"></div>

        <header class="blog-header">
            <!-- Blog.Avatar -->
            {% if Blog.Avatar %}
                <a href="{{ Blog.Url }}" title="{{ Blog.Title }}">
                    <img class="blog-avatar" src="{{ Blog.Avatar|resize(100,100)|crop }}" title="{{ Blog.Description }}" alt="{{ Blog.Description }}" width="100" height="100" />
                </a>
            {% endif %}

            <!-- Blog title -->
            <h1 class="blog-title">
                <a href="{{ Blog.Url }}"  class="topLien" title="{{ Blog.Description }}">{{ Blog.Title }}</a>
            </h1>
            <!-- Blog description -->
            <strong class="blog-description">
                {{ Blog.Description }}
            </strong>

            <nav class="menu clearfix">
                <ul>
                    <li>
                        <a{% if isIndex %} class="active"{% endif %} href="{{ Blog.Url }}" title="{{ Lang.Get('Home') }}">{{ Lang.Get('Home') }}</a>
                    </li>
                    {% if Blog.ListPages is not empty %}
                    <li>
                        <a class="sub-menu-link{% if isPage %} active{% endif %}" href="#">{{ Lang.Get('Pages') }}</a>
                        <ul class="sub-menu">
                            {% list Blog.ListPages %}
                                <li>
                                    <a href="{{ ListPage.Url }}" title="{{ ListPage.Title }}">{{ ListPage.Title }}</a>
                                </li>
                           {% endlist %}
                        </ul>
                    </li>
                    {% endif %}
                    {% if Blog.PostsTags is not empty and Custom('display_widget_tags') > 0 %}
                        <li>
                            <a class="sub-menu-link{% if isTag %} active{% endif %}" href="#">{{ Lang.Get('Tags') }}</a>
                            <ul class="sub-menu">
                                {% list Blog.PostsTags(Custom('display_tags')) %}
                                    <li>
                                        <a href="{{ PostsTag.Url }}" title="{{ PostsTag.Title }}">{{ PostsTag.Title }}</a>
                                    </li>
                                {% endlist %}
                            </ul>
                        </li>
                    {% endif %}
                    {% if Custom('display_widget_archives') == false %}
                        <li>
                            <a class="sub-menu-link{% if isArchive %} active{% endif %}" href="#">{{ Lang.Get('Archives') }}</a>
                            <ul class="sub-menu">
                                {% list Blog.Archives %}
                                     <li>
                                        <ul>
                                            {% list Archive.Months %}
                                                {% if Month.PostCount > 0 %}
                                                    <li>
                                                        <a href="{{ Month.Url }}">{{ Month.Name }} {{ Archive.Year }}</a>
                                                    </li>
                                                {% endif %}
                                            {% endlist %}
                                        </ul>
                                    </li>
                                {% endlist %}
                            </ul>
                        </li>
                    {% endif %}
                    <li>
                        <a{% if isSpecial %} class="active"{% endif %} href="{{ Blog.Url }}/contact">{{ Lang.Get('Contact') }}</a>
                    </li>
                </ul>

                <!-- Social link -->
                <ul class="social-link">
                    {% if Custom('twitter_username') is not empty %}
                        <li>
                            <a class="twitter" href="http://twitter.com/{{ Custom('twitter_username') }}" title="{{ Lang.Get('Follow me on') }} Twitter" target="_blank"></a>
                        </li>
                    {% endif %}
                    {% if Custom('facebook_username') is not empty %}
                        <li>
                            <a class="facebook" href="http://facebook.com/{{ Custom('facebook_username') }}" title="{{ Lang.Get('Follow me on') }} Facebook" target="_blank"></a>
                        </li>
                    {% endif %}
                    <li>
                        <a class="rss" href="{{ Blog.RssUrl }}" title="{{ Lang.Get('Subscribe to the RSS feed') }}" target="_blank"></a>
                    </li>
                </ul>

                <!-- Search form -->
                {% if Custom('display_widget_search') == false %}
                    <form class="search-form" action="/search" method="post">
                        <input type="text" name="q" placeholder="{{ Lang.Get('Search') }}" value="{% if isSearch %}{{ Search.Keyword }}{% endif %}" />
                        <input type="submit" title="{{ Lang.Get('Search') }}" value="" />
                    </form>
                {% endif %}
            </nav>
        </header>

        <section class="content clearfix">
        	<!-- left content -->
            {% if isIndex %}
                <div class="main">
                    {% list Blog.Posts(Custom('displayArticlesLimit'), ['OB']) %}
                        <article class="post" itemprop="blogPosts" itemscope itemtype="http://schema.org/BlogPosting">
                            <header class="post-header" itemprop="headline">
                                {% if Post.Cover is not empty %}
                                    <!-- Post cover -->
                                    <a class="post-cover" href="{{ Post.Permalink }}"{% if Post.Title is not empty %} title="{{ Post.Title }}"{% endif %}>
                                        <img src="{{ Post.Cover|resize(480,280)|crop }}" alt="" width="480" height="280" />
                                    </a>
                                {% endif %}

                                <div class="post-title">
                                    {% if Post.Title is not empty %}
                                        <!-- Post title -->
                                        <h2>
                                            <a itemprop="name" href="{{ Post.Permalink }}" title="{{ Post.Snippet }}">{{ Post.Title }}</a>
                                        </h2>
                                    {% endif %}

                                    {% if Post.ShowComments is not empty %}
                                        <!-- Comments number -->
										<a class="comments-number" href="{{ Post.Permalink }}{% if Custom('comment') == 'Disqus' and Custom('comment_disqus') is not empty %}#disqus_thread{% else %}#comments{% endif %}">
											{% if Custom('comment') == 'Facebook' %}
												<fb:comments-count href="{{ Post.Permalink }}"></fb:comments-count>
											{% elseif Custom('comment') == 'OverBlog' %}
												{{ Post.CommentCount }}
											{% endif %}
											<span class="comments-wording">{{ Lang.Get('com') }}</span>
										</a>
                                    {% endif %}
                                </div>

                                <!-- Post source -->
                                <span class="post-source {{ Post.GetfirstSource }}"></span>

                                {% if Post.Author is not empty %}
                                    <!-- Post author -->
                                    <span class="post-author" itemprop="author">
                                        {{ Lang.Get('by') }} <span>{{ Post.Author }}</span>
                                    </span>
                                {% endif %}
                            </header>

                            {% if Post.Snippet is not empty %}
                                <!-- Post snippet -->
                                <p class="post-snippet" itemprop="articleBody">
                                    {{ Post.Snippet }}
                                </p>
                            {% endif %}

                            <a class="read-more" href="{{ Post.Permalink }}" title="{{ Lang.Get('Read more') }}">{{ Lang.Get('Read more') }}</a>

                            {% if Post.Tags is not empty %}
                                <!-- Post tags -->
                                <p class="post-tags clearfix" itemprop="keywords">
                                    {% list Post.Tags %}
                                        <a href="{{ Tag.Url }}" rel="tag">
                                            <span>{{ Tag.Title }}</span>
                                        </a>
                                    {% endlist %}
                                </p>
                            {% endif %}

                            <footer class="post-footer">
                                <time class="post-date" itemprop="datePublished" datetime="{{ Post.Date }}">{{ Post.Date|datel(Lang.Get('Default date format'))|title }}</time>

                                <div class="post-share">
                                    {{ Post.RepostButton('nocount') }}

                                    <div class="share">
                                        <a class="share-link" href="#">{{ Lang.Get('Share this post') }}</a>
                                        <ul class="share-buttons">
                                            <li>
                                                <a class="gplus" href="https://plus.google.com/share?url={{ Post.Permalink }}">Google +</a>
                                            </li>
                                            <li>
                                                <a class="twt" href="https://twitter.com/share?url={{ Post.Permalink }}&text={% if Post.Title is not empty %}{{ Post.Title }}{% elseif Post.Snippet is not empty %}{{ Post.Snippet}}{% else %}{{ Blog.Title }}{% endif %}{% if Custom('twitter_username') is not empty %}&via={{ Custom('twitter_username') }}{% endif %}">Twitter</a>
                                            </li>
                                            <li>
                                                <a class="fb" href="https://www.facebook.com/sharer/sharer.php?u={{ Post.Permalink }}&t={% if Post.Title is not empty %}{{ Post.Title }}{% elseif Post.Snippet is not empty %}{{ Post.Snippet}}{% else %}{{ Blog.Title }}{% endif %}">Facebook</a>
                                            </li>
                                            {% if Post.Cover is not empty %}
                                                <li>
                                                    <a class="pint" href="//pinterest.com/pin/create/button/?url={{ Post.Permalink }}&media={{ Post.Cover }}&description={% if Post.Snippet is not empty %}{{ Post.Snippet }}{% else %}{{ Post.Title }}{% endif %}">Pinterest</a>
                                                </li>
                                            {% endif %}
                                        </ul>
                                    </div>
                                </div>
                            </footer>
                        </article>
                    {% endlist %}
                    <div class="navigation">
                        <a class="next" href="/social/ob/2">Plus d'OB</a>
                    </div>
                </div>
            {% elseif isSingle or isSocial or isSearch or isTag or isArchive %}
                <div class="main{% if isSocial %} social{% endif %}">
                    {% list Posts %}
                        <article class="post{% if isSocial %} social-post{% endif %}" itemprop="blogPosts" itemscope itemtype="http://schema.org/BlogPosting">
