                -webkit-box-shadow: 0 0.3125em 0.875em -0.3125em rgba(0, 0, 0, 0.5);
                   -moz-box-shadow: 0 0.3125em 0.875em -0.3125em rgba(0, 0, 0, 0.5);
                    -ms-box-shadow: 0 0.3125em 0.875em -0.3125em rgba(0, 0, 0, 0.5);
                     -o-box-shadow: 0 0.3125em 0.875em -0.3125em rgba(0, 0, 0, 0.5);
                        box-shadow: 0 0.3125em 0.875em -0.3125em rgba(0, 0, 0, 0.5);
                width: 227px;
            }
            .widgets .profile,
            .widgets .pages,
            .widgets .archives,
            .widgets .social {
                padding: 13px;
            }
            .widgets h3 {
                margin-bottom: 10px;
            }
            .widgets a {
                -webkit-transition: all 200ms ease;
                   -moz-transition: all 200ms ease;
                     -o-transition: all 200ms ease;
                        transition: all 200ms ease;
            }
            /* PROFILE */
            .widgets .avatar {
                display: block;
            }
            .widgets .avatar img {
                width: 100%;
            }
            .widgets h3 {
                color: {{ Custom('titles_font_color')|default('#222') }};
            }
            .widgets .profile p {
                color: {{ Custom('text_font_color')|default('#888') }};
            }

            /* PAGES */
            .widgets .list-pages,
            .widgets .list-pages li {
                list-style: none;
                padding: 0;
            }
            .widgets .list-pages li:last-child {
                margin: 0;
            }
            .widgets .list-pages li a {
                display: block;
                font-size: 0.9em;
                padding: 4px 0;
            }
            .widgets .list-pages li a:hover {
                background: #F6F6F6;
                color: #222;
            }

            /* ARCHIVES */
            .widgets .archives ul,
            .widgets .archives li {
                list-style: none;
                padding: 0;
            }
            .arch_year .year {
                background: #ECECEC;
                color: #222;
                display: block;
                font-weight: bold;
                margin-bottom: 5px;
                padding: 4px 0;
                text-align: center;
            }
            .widgets .arch_year .year:hover {
                background: #E3E3E3;
            }
            .widgets .arch_month .month {
                margin-bottom: 6px;
                padding: 3px 4px 3px;
            }
            .widgets .arch_month .month:hover {
                background: #F6F6F6;
            }
            .widgets .arch_month .month a {
                display: block;
                font-size: 0.9em;
            }
            .widgets .arch_month .month a span {
                float: right;
                font-size: 0.8em;
                font-weight: bold;
            }
            .widgets .arch_month .month:hover a {
                color: #222;
            }

            /* SOCIAL */
            .widgets .social {
                background: #ECECEC;
            }
            .widgets .widget + .social {
                border-top: 1px solid #E0E0E0;
                -webkit-box-shadow: 0 1px 0 0 #FFF inset;
                   -moz-box-shadow: 0 1px 0 0 #FFF inset;
                    -ms-box-shadow: 0 1px 0 0 #FFF inset;
                     -o-box-shadow: 0 1px 0 0 #FFF inset;
                        box-shadow: 0 1px 0 0 #FFF inset;
                margin-top: 10px;
            }
            .widgets .social ul {
                padding-left: 0;
            }
            .widgets .social li {
                float: left;
                list-style: none;
            }
            .widgets .social li + li {
                margin-left: 9px;
            }
            .widgets .social a {
                background: url(http://assets.over-blog-kiwi.com/themes/42/images/sprite.png) no-repeat center -500px;
                background-color: #CCC;
                color: #FFF;
                display: block;
                font-size: 0.6em;
                font-weight: bold;
                padding: 55px 0 5px;
                text-align: center;
                text-decoration: none;
                -webkit-transition: background-color 250ms ease;
                   -moz-transition: background-color 250ms ease;
                    -ms-transition: background-color 250ms ease;
                     -o-transition: background-color 250ms ease;
                        transition: background-color 250ms ease;
                width: 60px;
            }
            .widgets .social .tw a {
                background-position: center -567px;
            }
            .widgets .social .rss a {
                background-position: center -637px;
            }
            .widgets .social a:hover {
                background-color: #999;
            }

            /* ----------------------------- */
            /* ==Sections */
            /* ----------------------------- */
            .ob-repost {
                border: 1px solid #E2E2E2;
                color: #222;
                font-size: 1.3em;
                font-weight: bold;
                padding: .5em .8em;
                text-align: center;
            }
            .ob-repost .ob-link {
                text-decoration: underline;
            }
            .ob-section {
                width: auto;
            }
            /* DESCRIPTION */
            .ob-desc {
                color: #888;
                font-size: 1.3em;
                font-style: italic;
                font-weight: bold;
                margin-top: 5px;
            }
            /* IMAGES */
            .ob-section-images img {
                margin-bottom: 13px;
                max-width: 100%;
            }
            .ob-section-images .ob-row-1-col {
                margin-bottom: 13px;
                text-align: center;
            }
            .ob-section-images .ob-row-1-col:last-child {
                margin-bottom: 0;
            }
            .ob-section-images .ob-row-2-col img {
                width: 50%;
            }
            .ob-section-images .ob-row-3-col img {
                width: 194px;
            }
            .ob-section-images .ob-slideshow {
                max-width: 100%;
            }
            .ob-section-images .ob-slideshow img {
                width: 100%;
            }
            /* VIDEO */
            .ob-section-video iframe,
            .ob-section-video embed,
            .ob-section-video object {
                height: 337px;
                width: 100%;
            }
            /* TWEETS */
            .ob-section-html .twitter-tweet-rendered,
            .ob-section-html .twitter-tweet-rendered .twt-border {
                max-width: 100% !important;
                width: 100% !important;
            }
            /* TEXT */
            .ob-section ul,
            .ob-section ol {
                display: inline-block;
                margin-bottom: 25px;
            }
            /* AUDIO */
            .ob-section-audio .obsoundplayer {
                height: auto;
                overflow: hidden;
            }
            div.obsoundplayer span.obsoundplayername {
                font-size: 1.3em;
                width: 160px;
            }
            /* FILE */
            .ob-section-file .ob-ctn {
                background: #F1F1F1;
                border-color: #DDD;
                display: block;
                max-width: 100%;
            }
            .ob-section-file .ob-link {
                color: #222;
                font-size: 1.4em;
                max-width: 100%;
            }
            .ob-section-file .ob-link:hover {
                text-decoration: underline;
            }
            /* QUOTE */
            .ob-section-quote {
                background: #F1F1F1;
                border-color: #DDD;
                margin-bottom: 20px;
            }
            .ob-section-quote .ob-author,
            .ob-section-quote .ob-source {
                color: #666;
                max-width: 525px;
                overflow: hidden;
                text-overflow: ellipsis;
                -webkit-transition: color 200ms ease;
                   -moz-transition: color 200ms ease;
                     -o-transition: color 200ms ease;
                        transition: color 200ms ease;
                white-space: nowrap;
            }
            .ob-section-quote .ob-author .ob-link,
            .ob-section-quote .ob-source .ob-link {
                color: #666;
            }
            .ob-section-quote .ob-quote p {
                color: #222;
                margin-bottom: 40px;
            }
            .ob-section-quote .ob-quote p:before {
                color: #DCDCDC;
                margin: -64px 0 0 -80px;
            }
            .ob-section-quote .ob-quote p:after {
                color: #DCDCDC;
                margin: -14px 0 0 0;
            }
            /* LINK */
            .ob-section-link .ob-media-left {
                margin: 0;
            }
            .ob-section-link .ob-title,
            .ob-section-link .ob-snippet {
                padding-left: 20px;
            }
            .ob-section-link .ob-title .ob-link {
                color: #222;
                font-family: "Lato";
            }
            .ob-section-link .ob-snippet {
                font-family: "Lato";
            }
            .ob-section-link .ob-url {
                background: #DCDCDC;
                color: gray;
                max-width: 405px;
            }
            .ob-section-link .ob-url .ob-link {
                font-family: "Lato";
            }
            /* MAP */
            .ob-section-map .ob-map,
            .ob-section-map .ob-desc {
                width: 100%;
            }
            .ob-section-map img{
				max-width: none;
			}

            /* ----------------------------- */
            /* ==NAVIGATION */
            /* ----------------------------- */
            .navigation {
                margin: 20px 0;
                overflow: hidden;
                padding: 0 20px;
            }
            .navigation a {
                color: #888;
                display: inline-block;
                font-size: 1.2em;
                font-weight: bold;
                margin-bottom: 15px;
                text-decoration: none;
            }
            .navigation a:hover {text-decoration: underline;}
            .navigation .prev {float: left;}
            .navigation .prev:before {
                background: url(http://assets.over-blog-kiwi.com/themes/42/images/sprite.png) no-repeat center -477px;
                content: '';
                display: inline-block;
                height: 15px;
                width: 15px;
                margin-right: 5px;
                vertical-align: top;
            }
            .navigation .next {float: right;}
            .navigation .next:after {
                background: url(http://assets.over-blog-kiwi.com/themes/42/images/sprite.png) no-repeat center -447px;
                content: '';
                display: inline-block;
                height: 15px;
                width: 15px;
                margin-left: 5px;
                vertical-align: middle;
            }


            /* ----------------------------- */
            /* ==SPECIAL PAGE */
            /* ----------------------------- */
            .special .contact {
                background: #F9F9F9;
                border: 1px solid #CCC;
                -webkit-box-shadow: 0 0.3125em 0.875em -0.3125em rgba(0, 0, 0, 0.5);
                   -moz-box-shadow: 0 0.3125em 0.875em -0.3125em rgba(0, 0, 0, 0.5);
                    -ms-box-shadow: 0 0.3125em 0.875em -0.3125em rgba(0, 0, 0, 0.5);
                     -o-box-shadow: 0 0.3125em 0.875em -0.3125em rgba(0, 0, 0, 0.5);
                        box-shadow: 0 0.3125em 0.875em -0.3125em rgba(0, 0, 0, 0.5);
                float: left;
                margin: 8px 8px 8px 251px;
                width: 720px;
            }
            .special .widgets + .contact {
                margin: 8px;
            }
            .special .inner {
                padding: 13px;
            }
            .special h2 {
                color: {{ Custom('titles_font_color')|default('#222') }};
            }
            .special .ob-label {
                font-size: 1.4em;
                margin-left: 85px;
                width: 15%;
            }
            .special .ob-input-text,
            .special .ob-input-email,
            .special .ob-input-textarea,
            .special .ob-input-submit {
                background: #F5F5F5;
                border: 1px solid #DDD;
                border-bottom-color: #EEE;
                border-radius: 3px;
                color: #222;
                font-size: 12px;
                padding: 9px;
            }
            .special .ob-input-submit {
                cursor: pointer;
                font-weight: bold;
                margin-left: 27%;
                text-transform: capitalize;
            }
            .special .ob-succes {
                font-size: 1.4em;
                text-align: center;
                text-transform: uppercase;
            }
            .special .ob-terms {
                display: block;
                font-size: 1.1em;
                font-weight: bold;
                margin: 60px 0 10px;
                text-align: center;
                text-decoration: none;
            }
            .special .ob-terms:hover {
                text-decoration: underline;
            }

            /* ----------------------------- */
            /* ==LOADER */
            /* ----------------------------- */
            .ias_loader,
            .scroll-to-top,
            .load-more {
                background-color: #F9F9F9;
                border: 1px solid #DDD;
                -webkit-box-shadow: 0 0.3125em 0.875em -0.3125em rgba(0, 0, 0, .5);
                        box-shadow: 0 0.3125em 0.875em -0.3125em rgba(0, 0, 0, .5);
            }
            .ias_loader {
                bottom: -40px;
                left: 50%;
                margin-left: -100px;
                position: absolute;
                text-align: center;
                max-width: 400px;
                padding-right:10px;
            }
            .ias_loader img {
                display: inline-block;
                margin: 10px;
                width: 20px;
            }
            .ias_loader p {
                display: inline-block;
                font-size: 1.4em;
                line-height: 2.4em;
            }

            /* ----------------------------- */
            /* ==SCROLL TO TOP */
            /* ----------------------------- */
            .withsharebar .scroll-to-top {
                bottom: 45px;
                position: fixed;
                right: 20px;
            }
            .scroll-to-top {
                bottom: 5px;
                position: fixed;
                right: 20px;
            }
            .load-more{
                margin-left: 8px;
            }
            .withsharebar  .load-more{
                position: absolute;
                margin-top: 35px;
            }
            .scroll-to-top a,
            .load-more a {
                color: {{ Custom('titles_font_color')|default('#222') }};
                display: inline-block;
                font-size: 16px;
                padding: 10px 10px 10px;
                text-decoration: none;
            }
            .scroll-to-top a:hover,
            .load-more a:hover {
                color: {{ Custom('links_font_color')|default('#668FBA') }};
            }
            .scroll-to-top a:active {
                padding-bottom: 8px;
            }
        </style>
    </head>
    <body>
        <div id="fb-root"></div>

        <div class="global">
            <!-- header -->
            <header class="header">
                <!-- banner -->
                <div class="banner">
                    {% if Blog.Avatar is not empty %}
                        <a class="avatar" href="{{ Blog.Url }}" title="{{ Blog.Title }}">
                            <img src="{{ Blog.Avatar|resize(100)|crop }}" alt="" width="100" height="100" />
                        </a>
                    {% endif %}

                    <!--blog title -->
                    <h1>
                        <a href="{{ Blog.Url }}" title="{{ Blog.Description }}">{{ Blog.Title }}</a>
                    </h1>

                    {% if Blog.Description is not empty %}
                        <p>{{ Blog.Description }}</p>
                    {% endif %}

                    <form class="search" action="/search" method="post">
                        <input type="text" placeholder="{{ Lang.Get('Search') }}" {% if isSearch %}value="{{ Search.Keyword }}"{% endif %} name="q" />
                        <input type="submit" title="{{ Lang.Get('Search') }}" value="{{ Lang.Get('Search') }}" />
                    </form>
                </div>

                <!-- menu -->
                <nav class="menu">
                    <ul>
                        <li class="{% if isIndex %} active{% endif %}">
                            <a  href="{{ Blog.Url }}">{{ Lang.Get('Home') }}</a>
                        </li>
                        {% if Custom('category_number') > 0 %}
                            {% list Blog.PostsTags(Custom('category_number')) %}
                                <li class="{% if Tag.Title is not empty and PostsTag.Title == Tag.Title %} active{% endif %}">
                                    <a  href="{{ PostsTag.Url }}" title="{{ PostsTag.Title }}">{{ PostsTag.Title }}</a>
                                </li>
                            {% endlist %}
                        {% endif %}
                        <li class="{% if isSpecial %} active {% endif %}last">
                            <a href="/contact">{{ Lang.Get('Contact me') }}</a>
                        </li>
                    </ul>
                </nav>
            </header>

            <!-- main -->
            <section class="main{% if isSpecial %} special clearfix{% elseif isSingle %} single clearfix{% else %} index{% endif %}">
                {% if Custom('display_widget_bio') == false or Custom('display_widget_pages') == false or Custom('display_widget_archives') == false or Custom('display_widget_follow') == false %}
                    <!-- widgets -->
                    <article class="{% if not isSingle and not isSpecial %}masonry-item {% endif %}widgets">
                        {% if Custom('display_widget_bio') == false %}
                            <!-- blog owner about -->
                            <div class="widget profile">
                                {% if Blog.Owner.Avatar is not empty %}
                                    <img class="avatar" src="{{ Blog.Owner.Avatar|resize(199,237)|crop }}" alt="" width="199" height="237" />
                                {% endif %}
                                {% if Blog.Owner.Nickname is not empty %}
                                    <h3>{{ Blog.Owner.Nickname }}</h3>
                                {% endif %}
                                {% if Blog.Owner.Bio is not empty %}
                                    <p>{{ Blog.Owner.Bio }}</p>
                                {% endif %}
                            </div>
                        {% endif %}

                        {% if Custom('display_widget_pages') == false and Blog.ListPages is not empty %}
                            <div class="widget pages">
                                <h3>{{ Lang.Get('Pages') }}</h3>
                                <ul class="list-pages">
                                    {% list Blog.ListPages %}
                                        <li>
                                            <a  href="{{ ListPage.Url }}" title="{{ ListPage.Title }}">{{ ListPage.Title }}</a>
                                        </li>
                                    {% endlist %}
                                </ul>
                            </div>
                        {% endif %}

                        {% if Custom('display_widget_archives') == false and Blog.Archives is not empty %}
                        <!-- blog archives -->
                            <div class="widget archives">
                                <h3>{{ Lang.Get('Archives') }}</h3>
                                <ul id="archivelist">
                                    {% list Blog.Archives %}
                                         <li class="arch_year">
                                            <a href="#" class="year" data-year="{{ Archive.Year }}">{{ Archive.Year }}</a>
                                            <ul id="arch_{{ Archive.Year }}" class="arch_month">
                                                {% list Archive.Months%}
                                                    {% if Month.PostCount > 0 %}
                                                        <li class="month">
                                                            <a href="{{ Month.Url }}">
                                                                {{ Lang.Get(Month.Name) }} <span>{{ Month.PostCount }}</span>
                                                            </a>
                                                        </li>
                                                    {% endif %}
                                                {% endlist %}
                                            </ul>
                                        </li>
                                    {% endlist %}
                                </ul>
                            </div>
                        {% endif %}

                        {% if Custom('display_widget_follow') == false %}
                            <!-- follow blog owner -->
                            <div class="widget social">
                                <ul class="clearfix">
                                    {% if Custom('facebook_username') is not empty %}
                                        <li class="fb">
                                            <a href="//facebook.com/{{ Custom('facebook_username') }}" target="_blank" >FACEBOOK</a>
                                        </li>
                                    {% endif %}
                                    {% if Custom('twitter_username') is not empty %}
                                        <li class="tw">
                                            <a href="//twitter.com/{{ Custom('twitter_username') }}" target="_blank">TWITTER</a>
                                        </li>
                                    {% endif %}
                                    <li class="rss">
                                        <a href="{{ Blog.RssUrl }}" target="_blank" >RSS</a>
                                    </li>
                                </ul>
                            </div>
                        {% endif %}
                    </article>
                {% endif %}

                {% if isSpecial %}
                    <article class="contact">
                        <div class="inner">
                            {{ SpecialContent }}
                        </div>
                    </article>
                {% else %}
                    {% list Posts %}
                        <article class="post{% if not isSingle %} masonry-item{% endif %}">
                            <header>
                                {% if not isSingle and Custom('post_cover') is not empty or not isSingle and Post.Cover is not empty %}
                                    <!-- post cover -->
                                    <a class="cover permalink" href="{{ Post.Permalink }}" {% if Post.Snippet is not empty %}title="{{ Post.Snippet }}"{% endif %}>
                                        <img src="{% if Post.Cover is not empty %}{{ Post.Cover }}{% else %}{{ Custom('post_cover') }}{% endif %}" alt="" />
                                    </a>
                                {% endif %}
                                <!-- post date -->
                                <time class="date" datetime="{{ Post.Date }}">{{ Post.Date|datel(Lang.Get('Default date format'))|title }}</time>

                                {% if isSingle and Post.Author is not empty %}
                                    <!-- post author -->
                                    <p class="author">
                                        {{ Lang.Get('Published by') }} <span>{{ Post.Author }}</span>
                                    </p>
                                {% endif %}

                                <!-- post title -->
                                <h2 class="title">
                                    <a class="permalink" href="{{ Post.Permalink }}" {% if Post.Snippet is not empty %}title="{{ Post.Snippet }}"{% endif %}>
                                        {% if Post.Title is not empty %}
                                            {{ Post.Title }}
                                        {% else %}
                                            {{ Lang.Get('Published from') }}
                                            {% list Post.Sources %}
                                                {{ Lang.Get(Source) }} {% if not loop.last %}{{ Lang.Get('and') }} {% endif %}
                                            {% endlist %}
                                        {% endif %}
                                    </a>
                                </h2>
                            </header>

                            {% if isSingle %}
                                <!-- post body -->
                                <div class="body">
                                    {{ Post.Body }}
                                </div>
                            {% else %}
                                {% if Post.getFirstSource == 'TW' %}
                                    <!-- tweet on home page -->
                                    <div class="tweet">
                                        {{ Post.Body(1) }}
                                        <div class="tweet-actions">
                                            <a class="tweet-reply" href=""></a>
                                            <a class="tweet-retweet" href=""></a>
                                            <a class="tweet-favorite" href=""></a>
                                        </div>
                                    </div>
                                {% else %}
                                    <!-- post snippet -->
                                    <p class="snippet">
                                        {{ Post.Snippet }}
                                    </p>
                                {% endif %}
                            {% endif %}

                            {% if Post.Tags is not empty %}
                                <!-- post tag -->
                                <div class="tag">
                                    <p>
                                        {% list Post.Tags %}
                                            <a href="{{ Tag.url }}">#{{ Tag.Title }}</a>{% if loop.last is empty %}, {% endif %}
                                        {% endlist %}
                                    </p>
                                </div>
                            {% endif %}

                            {% if not isPage and Navigation.PreviousPost is not empty or Navigation.NextPost is not empty %}
                                <!-- Pagination -->
                                <nav class="navigation">
                                     {% if Navigation.PreviousPost is not empty %}
                                        <a href="{{ Navigation.PreviousPost.Permalink }}" class="prev">
                                            {% if Navigation.PreviousPost.Title is not empty %}
                                                {{ Navigation.PreviousPost.Title }}
                                            {% else %}
                                                {{ Lang.Get('Previous post') }}
                                            {% endif %}
                                       </a>
                                    {% endif %}

                                    {% if Navigation.NextPost is not empty %}
                                        <a href="{{ Navigation.NextPost.Permalink }}" class="next">
                                            {% if Navigation.NextPost.Title is not empty %}
                                                {{ Navigation.NextPost.Title }}
                                            {% else %}
                                                {{ Lang.Get('Next post') }}
                                            {% endif %}
                                        </a>
                                    {% endif %}
                                </nav>
                            {% endif %}

                            {% if isSingle and not isPage and Post.ShowComments is not empty or not isSingle and Custom('share_service') != 'None' %}
                                <footer class="footer">
                            {% endif %}
                                {% if isSingle and not isPage and Post.ShowComments is not empty %}
                                    <!-- post comments -->
                                    <div class="item-comments" id="comments">
                                        <h3>{{ Lang.Get('Comment this post') }}</h3>
                                        {% if Custom("comment") == "Facebook" %}
                                            <div class="fb-comments" data-href="{{ Post.Permalink }}" data-num-posts="2" data-width="582"></div>
                                        {% elseif Custom("comment") == "Disqus" and Custom('comment_disqus') is not empty %}
                                            <div id="disqus_thread"></div>
                                            <noscript>Please enable JavaScript to view the <a href="http://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
                                        {% else %}
                                            {{ Post.Comments }}
                                        {% endif %}
                                    </div>
                                {% endif %}
                                {% if not isSingle and Custom('share_service') != 'None' %}
                                    <!-- post share buttons on home page -->
                                    <div class="share">
                                        {% if Custom('share_service') == 'Google +' %}
                                            <!-- google + share -->
                                            <a class="gplus" href="https://plus.google.com/share?url={{ Post.Permalink }}"></a>
                                        {% elseif Custom('share_service') == 'Twitter' %}
                                            <!-- twitter share -->
                                            <a class="twt" href="https://twitter.com/share?url={{ Post.Permalink }}&text={% if Post.Title is not empty %}{{ Post.Title }}{% elseif Post.Snippet is not empty %}{{ Post.Snippet}}{% else %}{{ Blog.Title }}{% endif %}{% if Custom('twitter_username') is not empty %}&via={{ Custom('twitter_username') }}{% endif %}" target="_blank"></a>
                                        {% elseif Custom('share_service') == 'Facebook' %}
                                            <!-- facebook share -->
                                            <a class="fb" href="https://www.facebook.com/sharer/sharer.php?u={{ Post.Permalink }}&t={% if Post.Title is not empty %}{{ Post.Title }}{% elseif Post.Snippet is not empty %}{{ Post.Snippet}}{% else %}{{ Blog.Title }}{% endif %}" target="_blank"></a>
                                        {% elseif Custom('share_service') == 'Pinterest' and Post.Cover is not empty %}
                                            <a class="pint" href="//pinterest.com/pin/create/button/?url={{ Post.Permalink }}&media={{ Post.Cover }}&description={% if Post.Snippet is not empty %}{{ Post.Snippet }}{% else %}{{ Post.Title }}{% endif %}"></a>
                                        {% elseif Custom('share_service') == 'All' or Custom('share_service') is empty %}
                                            <!-- google + share -->
                                            <a class="gplus" href="https://plus.google.com/share?url={{ Post.Permalink }}"></a>

                                            <!-- twitter share -->
                                            <a class="twt" href="https://twitter.com/share?url={{ Post.Permalink }}&text={% if Post.Title is not empty %}{{ Post.Title }}{% elseif Post.Snippet is not empty %}{{ Post.Snippet}}{% else %}{{ Blog.Title }}{% endif %}{% if Custom('twitter_username') is not empty %}&via={{ Custom('twitter_username') }}{% endif %}" target="_blank"></a>

                                            <!-- facebook share -->
                                            <a class="fb" href="https://www.facebook.com/sharer/sharer.php?u={{ Post.Permalink }}&t={% if Post.Title is not empty %}{{ Post.Title }}{% elseif Post.Snippet is not empty %}{{ Post.Snippet}}{% else %}{{ Blog.Title }}{% endif %}" target="_blank"></a>
                                            {% if Post.Cover is not empty %}
                                                <a class="pint" href="//pinterest.com/pin/create/button/?url={{ Post.Permalink }}&media={{ Post.Cover }}&description={% if Post.Snippet is not empty %}{{ Post.Snippet }}{% else %}{{ Post.Title }}{% endif %}"></a>
                                            {% endif %}
                                        {% endif %}
                                    </div>
                                {% endif %}
                            {% if isSingle and not isPage and Post.ShowComments is not empty or not isSingle and Custom('share_service') != 'None' %}
                                </footer>
                            {% endif %}
                        </article>

                        {% if isSingle and Custom('share_service') != 'None' %}
                            <aside class="share-bar" role="complementary">
                                {% if Custom('share_service') == 'Google +' %}
                                    <div class="gplus">
                                        <iframe allowtransparency="true" frameborder="0" scrolling="no" src="https://plusone.google.com/u/0/_/+1/fastbutton?url={{ Post.Permalink }}&amp;size=tall&amp;annotation=bubble&amp;lang={{ Blog.Lang }}" style="border:none; overflow:hidden; width: 50px; height: 60px;" align="left"></iframe>
                                    </div>
                                {% elseif Custom('share_service') == 'Twitter' %}
                                    <div class="twt">
                                        <iframe allowtransparency="true" frameborder="0" scrolling="no" src="http://platform.twitter.com/widgets/tweet_button.html?url={{ Post.PermaLink }}{% if Custom('twitter_username') is not empty %}&via={{ Custom('twitter_username')}}{% endif %}&text={% if Post.Title is not empty %}{{ Post.Title }}{% elseif Post.Snippet is not empty %}{{ Post.Snippet}}{% else %}{{ Blog.Title }}{% endif %}&lang={{ Blog.Lang }}&count=vertical" style="border:none; overflow:hidden; height: 62px;"></iframe>
                                    </div>
                                {% elseif Custom('share_service') == 'Facebook' %}
                                    <div class="facebook">
                                        <iframe allowtransparency="true" frameborder="0" scrolling="no" src="//www.facebook.com/plugins/like.php?href={{ Post.Permalink }}&amp;send=false&amp;layout=box_count&amp;width=59&amp;show_faces=false&amp;action=like&amp;colorscheme=light&amp;font&amp;height=90"  style="border:none; overflow:hidden; width:59px; height:61px;"></iframe>
                                    </div>
                                {% elseif Custom('share_service') == 'Pinterest' and Post.Cover is not empty %}
                                    <div class="pinterest">
                                        <iframe scrolling="no" frameborder="0" src="http://pinit-cdn.pinterest.com/pinit.html?url={{ Post.Permalink }}&media={{ Post.Cover }}&description={% if Post.Snippet is not empty %}{{ Post.Snippet }}{% else %}{{ Post.Title }}{% endif %}&layout=vertical" style="border:none; overflow:hidden; width: 43px; height: 58px;"></iframe>
                                    </div>
                                {% elseif Custom('share_service') == 'All' or Custom('share_service') is empty %}
                                    <div class="gplus">
                                        <iframe allowtransparency="true" frameborder="0" scrolling="no" src="https://plusone.google.com/u/0/_/+1/fastbutton?url={{ Post.Permalink }}&amp;size=tall&amp;annotation=bubble&amp;lang={{ Blog.Lang }}" style="border:none; overflow:hidden; width: 50px; height: 60px;" align="left"></iframe>
                                    </div>
                                    <div class="twt">
                                        <iframe allowtransparency="true" frameborder="0" scrolling="no" src="http://platform.twitter.com/widgets/tweet_button.html?url={{ Post.PermaLink }}{% if Custom('twitter_username') is not empty %}&via={{ Custom('twitter_username')}}{% endif %}&text={% if Post.Title is not empty %}{{ Post.Title }}{% elseif Post.Snippet is not empty %}{{ Post.Snippet}}{% else %}{{ Blog.Title }}{% endif %}&lang={{ Blog.Lang }}&count=vertical" style="border:none; overflow:hidden; height: 62px;"></iframe>
                                    </div>
                                    <div class="facebook">
                                        <iframe allowtransparency="true" frameborder="0" scrolling="no" src="//www.facebook.com/plugins/like.php?&href={{ Post.Permalink }}&amp;send=false&amp;layout=box_count&amp;show_faces=false&amp;action=like&amp;colorscheme=light&amp;font&amp;height=90"  style="border:none; overflow:hidden; width:100%; height:61px;"></iframe>
                                    </div>
                                    {% if Post.Cover is not empty %}
                                        <div class="pinterest">
                                            <iframe scrolling="no" frameborder="0" src="http://pinit-cdn.pinterest.com/pinit.html?url={{ Post.Permalink }}&media={{ Post.Cover }}&description={% if Post.Snippet is not empty %}{{ Post.Snippet }}{% else %}{{ Post.Title }}{% endif %}&layout=vertical" style="border:none; overflow:hidden; width: 43px; height: 58px;"></iframe>
                                        </div>
                                    {% endif %}
                                {% endif %}
                                <div class="ob-share">
                                    {{ Post.RepostButton('top') }}
                                </div>
                            </aside>
                        {% endif %}
                    {% else %}
                        <!-- query without results -->
                        <article class="no-post">
                            <p>{{ Lang.Get('No post found') }}.</p>
                        </article>
                    {% endlist %}
                {% endif %}
            </section>
            {% if Navigation.NextPage %}
                <p class="load-more">
                    <a href="{{ Navigation.NextPage }}">{{ Lang.Get('Load more') }}</a>
                </p>
            {% endif %}
        </div>

        <!-- Scripts -->
        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.1/jquery.min.js"></script>
        {% if not isSingle and not isSpecial %}
            <script src="http://assets.over-blog-kiwi.com//themes/jquery/masonry/jquery.masonry.min.js"></script>
            <script src="http://assets.over-blog-kiwi.com/t/jquery/infinite-ajax-scroll/jquery-ias.min.js"></script>
        {% endif %}
        <script src="http://assets.over-blog-kiwi.com/themes/jquery/fancybox2.1.2/jquery.fancybox.pack.js"></script>
        {% if Custom('background') is not empty %}
            <script src="http://assets.over-blog-kiwi.com/themes/13/js/jquery.backstretch.min.js" type="text/javascript"></script>
        {% endif %}

        <script>
            {% if Custom('google_analytics') is not empty %}
                // Google Analytics
                window.google_analytics_uacct = '{{Custom('google_analytics')}}';
                var _gaq = _gaq || [];
                    _gaq.push(['_setAccount', '{{Custom('google_analytics')}}']);
                    _gaq.push(['_trackPageview']);
                    _gaq.push(['_trackPageLoadTime']);

                (function() {
                    var ga = document.createElement('script');
                        ga.type = 'text/javascript';
                        ga.async = true;
                        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
                    var s = document.getElementsByTagName('script')[0];
                        s.parentNode.insertBefore(ga, s);
                })();
            {% endif %}

            {% if not isSingle and not isSpecial %}
                var ias = new Array();
                ias.counter = 1,
                ias.MAXPAGE = 10;

                // Twitter controls
                var TwitterControls = function(elements) {
                    $(elements).find(".ob-tweet").each(function() {
                        var datatwitter = $(this).find("a[data-twitter]").attr("data-twitter");

                        $(this).parent().parent().find(".tweet-reply").attr("href", "https://twitter.com/intent/tweet?in_reply_to=" + datatwitter);
                        $(this).parent().parent().find(".tweet-retweet").attr("href", "https://twitter.com/intent/retweet?tweet_id=" + datatwitter);
                        $(this).parent().parent().find(".tweet-favorite").attr("href", "https://twitter.com/intent/favorite?tweet_id=" + datatwitter);
                    });
                },

                // Disable tweets render
                TwitterDisabled = function(elements, twitter) {
                    $(elements).find(".twitter-tweet").removeClass("twitter-tweet");
                    if(twitter){
                        //Twitter render
                        !function(d,s,id){
                            var js, fjs = d.getElementsByTagName(s)[0];
                            if(!d.getElementById(id)){
                                js = d.createElement(s);
                                js.id = id;
                                js.src = "https://platform.twitter.com/widgets.js";
                                fjs.parentNode.insertBefore(js,fjs);
                            }
                        }(document,"script","twitter-wjs");
                    }
                },

                // Social popup
                SocialPopup = function(elements) {
                    $(elements).find(".tweet-reply, .tweet-retweet, .tweet-favorite, .share a").click(function() {
                        var popup = window.open($(this).prop("href"), '',"width=650, height=400, scrollbars=0, left=" +((screen.width - 650)/2) + ", top=" + ((screen.height - 400)/2) );
                        if (window.focus){
                            popup.focus();
                        }
                        return false;
                    });
                },

                {% if Custom('post_opening') == 'Pop-in' %}
                    // Opening of the posts with fancybox
                    ItemFancybox = function (selector) {

                        var ItemPermalink,
                            shareBar,
                            url,
                            location;

                        $(selector).fancybox({
                            autoHeight  : true,
                            autoSize    : false,
                            closeEffect : "elastic",
                            fitToView   : false,
                            margin      : [80, 20, 20, 20],
                            openEffect  : "none",
                            padding     : 0,
                            scrolling   : "no",
                            topRatio    : 0,
                            type        : "ajax",
                            width       : 610,
                            helpers : {
                                overlay : {
                                    showEarly : false
                                },
                                title : null
                            },
                            afterLoad: function() {
                                shareBar = $(".share-bar", this.content);
                                this.content = $(".post", this.content);
                            },
                            afterShow: function() {
                                if(!$.browser.msie || $.browser.msie && $.browser.version > 9.0 ){
                                    url = $(this.element).attr('href');
                                    location = window.location.pathname;
                                    history.pushState("", "", url);
                                }

                                // OB callback
                                if (typeof OB != "undefined") { OB.restart && OB.restart(); }

                                {% if Custom('comment') == "Facebook" %}
                                    // FB callback
                                    if (typeof FB != "undefined") { FB.XFBML.parse(document.body); }
                                {% elseif Custom('comment') == 'Disqus' %}
                                    var disqus_shortname = '{{ Custom('comment_disqus') }}';
                                    (function() {
                                        var dsq = document.createElement('script');
                                            dsq.type = 'text/javascript';
                                            dsq.async = true;
                                            dsq.src = 'http://' + disqus_shortname + '.disqus.com/embed.js';
                                        (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
                                    })();
                                {% endif %}

                                //Twitter render
                                if (typeof twttr != "undefined") {
                                    twttr.widgets.load();
                                }
                                else {
                                    !function(d,s,id){
                                        var js, fjs = d.getElementsByTagName(s)[0];
                                        if(!d.getElementById(id)){
                                            js = d.createElement(s);
                                            js.id = id;
                                            js.src = "https://platform.twitter.com/widgets.js";
                                            fjs.parentNode.insertBefore(js,fjs);
                                        }
                                    }(document,"script","twitter-wjs");
                                }

                                // Insert share bar
                                $(".fancybox-inner").append(shareBar);

                                // Disable image link
                                $('.ob-link-img').click(function() {
                                    return false;
                                });

                                $('.ob-link-img').hover(function() {
                                    $(this).css('cursor','default');
                                });
                            },
                            beforeClose : function() {
                                if(!$.browser.msie || $.browser.msie && $.browser.version > 9.0 ){
                                    history.pushState("", "", location);
                                }

                                // kill Disqus
                                if(typeof(DISQUS) === 'object' ){
                                    DISQUS = false;
                                }
                            }
                        });
                    },
                {% endif %}

                //Force scrollbar when content don't have one
                ForceScrollBar = function() {
                    if($(window).scrollTop() + $(window).height() == $(document).height()){
                        $('html').height($(document).height()+10);
                    }
                };

            {% elseif isSingle %}
                //Twitter render
                !function(d,s,id){
                    var js, fjs = d.getElementsByTagName(s)[0];
                    if(!d.getElementById(id)){
                        js = d.createElement(s);
                        js.id = id;
                        js.src = "https://platform.twitter.com/widgets.js";
                        fjs.parentNode.insertBefore(js,fjs);
                    }
                }(document,"script","twitter-wjs");
            {% endif %}

            // Comments
            {% if Custom('comment') == 'Facebook' %}
                // Facebook comments
                (function(d, s, id) {
                    var js, fjs = d.getElementsByTagName(s)[0];
                    if (d.getElementById(id)) return;
                        js = d.createElement(s);
                        js.id = id;
                        js.src = "//connect.facebook.net/{{ Blog.Lang }}_{{ Blog.Lang|upper }}/all.js#xfbml=1&appId={{ Custom('facebook_appid')|default('284865384904712')}}";
                        fjs.parentNode.insertBefore(js, fjs);
                }(document, 'script', 'facebook-jssdk'));
            {% elseif isSingle and Custom('comment') == 'Disqus' %}
                // Disqus comments
                var disqus_shortname = '{{ Custom('comment_disqus') }}';
                (function() {
                    var dsq = document.createElement('script');
                        dsq.type = 'text/javascript';
                        dsq.async = true;
                        dsq.src = 'http://' + disqus_shortname + '.disqus.com/embed.js';
                    (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
                })();
            {% endif %}

            $(document).ready(function(){
                {% if isSingle %}
                    // Fancybox on single pages via url
                    $(".ob-link-img").attr("rel", "fancybox").fancybox({
                        type: "image"
                    });

                {% elseif not isSpecial %}
                    var container = $(".main");

                    // Masonry
                    container.imagesLoaded(function(){
                        container.masonry({
                            itemSelector : ".masonry-item",
                            columnWidth  : 245,
                            isAnimated   : false
                        });
                    });

                    TwitterControls($(".masonry-item"));

                    TwitterDisabled($(".masonry-item"));

                    SocialPopup($(".masonry-item"));

                    {% if Custom('post_opening') == 'Pop-in' %}
                        ItemFancybox($(".permalink"));
                    {% endif %}

                    ForceScrollBar();

                    // Load more with Masonry
                    jQuery.ias({
                        container      : ".main",
                        item           : ".masonry-item",
                        pagination     : ".load-more",
                        next           : ".load-more a",
                        loader         : "<img src='http://assets.over-blog-kiwi.com/themes/14/images/ajax_loader.gif' alt='' title='' /><p>{{ Lang.Get('Loading new posts') }}...</p>",
                        history        : false,
                        tresholdMargin : -200,
                        triggerPageTreshold: ias.MAXPAGE+1,
                        beforePageChange: function(){
                            if (ias.counter >= ias.MAXPAGE) {
                                $('.load-more').show();
                                return false;
                            }
                        },
                        onRenderComplete: function(items){
                            ias.counter++;
                        },
                        onLoadItems    : function(items) {
                            var newElems;

                            {% if Custom('display_widget_bio') == false or Custom('display_widget_pages') == false or Custom('display_widget_archives') == false or Custom('display_widget_follow') == false %}
                                items.shift();
                            {% endif %}

                            newElems = $(items);
                            newElems.css({ opacity: 0 });

                            newElems.imagesLoaded(function() {
                                newElems.animate({ opacity: 1 });
                                container.masonry( "appended", newElems, false );
                            });

                            TwitterControls(items);

                            TwitterDisabled(items);

                            SocialPopup(items);

                            ForceScrollBar();
                        }
                    });

                {% endif %}

                // Placeholder Script
                if($.browser.msie && $.browser.version <= 9.0 ){
                    $('input[placeholder]').each(function(){
                        var input = $(this);
                        $(input).val(input.attr('placeholder'));
                        $(input).focus(function(){
                             if (input.val() == input.attr('placeholder')) {
                                 input.val('');
                             }
                        });
                        $(input).blur(function(){
                            if (input.val() == '' || input.val() == input.attr('placeholder')) {
                                input.val(input.attr('placeholder'));
                            }
                        });
                    });
                }

                // Scroll top button
                $(window).scroll(function() {
                    if($(window).scrollTop() <= 0) {
                        $('.scroll-to-top').slideUp("250");
                    }
                    else {
                        if($('.scroll-to-top').length == 0) {
                            $('body').append(
                                '<div class="scroll-to-top">'+
                                    "<a href='#'>{{ Lang.Get('Go top') }}</a>"+
                                '</div>');
                        }
                        $('.scroll-to-top').slideDown("250");
                    }
                });

                $('.scroll-to-top a').live('click', function(event){
                    event.preventDefault();
                    $('html,body').animate({scrollTop: 0}, 'slow');
                });

                {% if Custom('background') is not empty %}
                    // Backstretch
                    if($.browser.msie && $.browser.version <= 8.0 ){
                        $.backstretch('{{ Custom('background') }}');
                        $('body').css('background', 'none');
                    }
                {% endif %}

                {% if Custom('display_widget_archives') == false %}
                    // Archives
                    // Hide years without months
                    $(".arch_year").each(function() {
                        if ($(this).find(".arch_month li").length == 0) {
                            $(this).hide();
                        }
                    });

                    $(".arch_month").hide();
                    $('.arch_year > a').click(function() {
                        {% if isSingle or isSpecial %}
                            $(this).siblings(".arch_month").slideToggle(1);
                        {% else %}
                            $(this).siblings(".arch_month").slideToggle(1, function(){
                                container.masonry('reload')
                            });
                        {% endif %}
                        return false;
                    });
                {% endif %}
            });
        </script>
    </body>
</html>
