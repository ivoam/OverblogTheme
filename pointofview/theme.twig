							{% if loop.first is not empty %}
							<span id="post-tags">{{ Lang.Get('Labels') }} :
							{% endif %} 
							{% endlist %}
							{% list Post.Tags %} <a href="{{ Tag.Url }}">{{ Tag.Title }}</a>{% if loop.last is empty %}, {% endif %}{% endlist %}</span>
						</div>
						<div id="post-body">
							{{ Post.Body }}						
						</div>
						{% if Custom ('share_button') is not empty %}
