            .ob-section-video object {
                max-height: 346px;
                width: 100%;
            }

            /* Section HTML */
            .ob-section-html iframe,
            .ob-section-html object,
            .ob-section-html embed {
                max-width: 100%
            }


            .ob-section-audio .obsoundplayer .obsoundplayername {
                height: 31px;
                overflow: hidden;
                width: 140px;
            }
            .ob-desc {
                font-style: italic;
                font-weight: bold;
            }
            input[type="submit"],
            input[type="reset"] {
                cursor: pointer;
                text-transform: uppercase;
            }
            .container_12 .post {
                float: left;
                width: 600px;
            }
            .container_12 .post-sidebar {
                float: right;
                margin-left: 60px;
                width: 300px;
            }

            .footer {
                font-size: 12px;
                margin: 30px auto;
                overflow: hidden;
            }
            .footer .right {
                float: right;
            }
            .footer .right a {
                -webkit-transition: color 300ms ease-out;
                   -moz-transition: color 300ms ease-out;
                    -ms-transition: color 300ms ease-out;
                     -o-transition: color 300ms ease-out;
                        transition: color 300ms ease-out;
            }
            .footer .right a:hover {
                color: #ED8B00;
            }
        </style>
        <!--[if IE]>
            <style>#header h1 a:hover{font-size: 75px; }</style>
        <![endif]-->
        <!--[if lt IE 9]>
            <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->

    </head>
    <body>
        <div id="fb-root"></div>
        <div class="container">
            <!-- shortcut:[Header content] -->
            <header class="header">
                <h1 class="blog-title">
                    <a href="{{ Blog.Url }}" title="{{ Blog.Description }}">{{ Blog.Title }}</a>
                </h1>

                <p class="description">
                    <strong>{{ Blog.Description }}</strong>
                </p>
            </header>

            {% if Custom('custom_hide_widget_search_form') == false or Custom('custom_tagline_text') is not empty %}
                <div class="headline">
                    <!-- Search form -->
                    {% if Custom('custom_hide_widget_search_form') == false %}
                        <form class="search" action="/search" method="post">
                            <fieldset>
                                <input type="text" name="q" placeholder="{{ Lang.Get('Search our blog...') }}" value="{{ Search }}" />
                                <input type="submit" name="go" id="go" value="{{ Lang.Get('Search') }}" />
                            </fieldset>
                        </form>
                    {% endif %}
                    {% if Custom('custom_tagline_text') is not empty %}
                        <p id="tagline2">{{ Custom('custom_tagline_text') }}</p>
                    {% endif %}
                    <br class="clear" />
                </div>
            {% endif %}

            <!-- menu -->
            <nav class="menu">
                <ul class="nav">
                    <!-- Index -->
                    <li {% if isIndex %} class="active" {% endif %}>
                        <a {% if isIndex %} {% endif %} href="{{ Blog.Url }}">{{ Lang.Get('Home') }}</a>
                    </li>

                    {% if Blog.PostsTags is not empty %}
                        <!-- Tags -->
                        <li>
                            <a href="#">{{ Lang.Get('Tags') }}</a>
                            <ul>
                                {% list Blog.PostsTags(Custom('tag_number')|default('10')) %}
                                    <li {% if isTag and Tag.Title == PostsTag.Title %} class="active" {% endif %}>
                                        <a href="{{ PostsTag.Url }}">{{ PostsTag.Title }}</a>
                                    </li>
                                {% endlist %}
                            </ul>
                        </li>
                    {% endif %}

                    <!-- Pages -->
                    {% list Blog.ListPages %}
                        {% if loop.first %}
                            <li>
                                <a href="#">{{ Lang.Get('Pages') }}</a>
                                <ul>
                        {% endif %}
                                    <li>
                                        <a href="{{ ListPage.Url }}">{{ ListPage.Title }}</a>
                                    </li>
                        {% if loop.last %}
                                </ul>
                            </li>
                        {% endif %}
                    {% endlist %}
                    <li class="last">
                        <a href="{{ Blog.url }}/contact">{{ Lang.Get('Contact') }}</a>
                    </li>
                </ul>
            </nav>

            {% if isSpecial or isSingle %}
                <div class="box">
                    <div class="container_12">
                        <div class="content">
                            {% if isSpecial %}
                                {{ SpecialContent }}
                            {% else %}
                                <h2 class="title">
                                    {% if Post.Title is not empty %}
                                        {{ Post.Title }}
                                    {% else %}
                                        {{ Lang.Get('Published from') }}
                                        {% list Post.Sources %}
                                            {{ Lang.Get(Source) }} {% if not loop.last %}{{ Lang.Get('and') }} {% endif %}
                                        {% endlist %}
                                    {% endif %}
                                </h2>
                                <p class="meta">
                                    {% if Post.Author and Post.Author is not empty %}
                                        {{ Lang.Get('Posted by') }} {{ Post.Author }} {{ Lang.Get('on') }}
                                    {% else %}
                                        {{ Lang.Get('Posted on') }}
                                    {% endif %}
                                    {{ Post.Date|datel(Lang.Get('Default date format')) | title }}, {{ Post.Date|date("H:ia") }}
                                </p>
                                {% list Post.Tags %}
                                    {% if loop.index == 1 %}
                                        <p class="meta">
                                            <strong>{{ Lang.Get('Tags:') }}</strong>
                                    {% endif %}
                                    <a href="{{ Tag.Url }}" title="{{ Tag.Title }}">#{{ Tag.Title }}</a>{% if not loop.last %}, {% endif %}
                                    {% if loop.length > 0 and loop.last %}
                                         </p>
                                    {% endif %}
                                {% endlist %}

                                <div class="entry">
                                    {{ Post.Body }}

                                    <div class="ob-section m-social-networks clear">
                                        <h6>{% if isPage %}{{ Lang.Get('Share this page') }}{% else %}{{ Lang.Get('Share this post') }}{% endif %}</h6>
                                        {% if Custom('share_service') != 'None' %}
                                            {% if Custom('share_service') == 'Google +' %}
                                                <div class="google">
                                                    <div class="g-plusone" data-size="medium" data-href="{{ Post.Permalink }}"></div>
                                                </div>
                                            {% elseif Custom('share_service') == 'Twitter' %}
                                                <div class="twitter">
                                                    <a href="https://twitter.com/share" class="twitter-share-button"  data-text="{% if Post.Title is not empty %}{{ Post.Title }}{% elseif Post.Snippet is not empty %}{{ Post.Snippet}}{% else %}{{ Blog.Title }}{% endif %}" data-url="{{ Post.Permalink }}" data-lang="{{ Blog.Lang }}"{% if Custom('twitter_username') is not empty %} data-via="{{Custom('twitter_username')}}"{% endif %}></a>
                                                </div>
                                            {% elseif Custom('share_service') == 'Facebook' %}
                                                <div class="facebook">
                                                    <div class="fb-like" data-href="{{ Post.Permalink }}" data-send="false" data-layout="button_count" data-width="90" data-show-faces="false"></div>
                                                </div>
                                            {% elseif Custom('share_service') == 'Pinterest' %}
                                                <div class="pinterest">
                                                    <a href="http://pinterest.com/pin/create/button/?url={{ Post.Permalink }}&media={{ Post.Cover }}&description={{ Post.Snippet }}" class="pin-it-button" count-layout="horizontal">
                                                        <img border="0" src="//assets.pinterest.com/images/PinExt.png" title="Pin It" />
                                                    </a>
                                                </div>
                                            {% elseif Custom('share_service') == 'All' or Custom('share_service') is empty %}
                                                <div class="google">
                                                    <div class="g-plusone" data-size="medium" data-href="{{ Post.Permalink }}"></div>
                                                </div>
                                                <div class="twitter">
                                                    <a href="https://twitter.com/share" class="twitter-share-button"  data-text="{% if Post.Title is not empty %}{{ Post.Title }}{% elseif Post.Snippet is not empty %}{{ Post.Snippet}}{% else %}{{ Blog.Title }}{% endif %}" data-url="{{ Post.Permalink }}" data-lang="{{ Blog.Lang }}"{% if Custom('twitter_username') is not empty %} data-via="{{Custom('twitter_username')}}"{% endif %}></a>
                                                </div>
                                                <div class="facebook">
                                                    <div class="fb-like" data-href="{{ Post.Permalink }}" data-send="false" data-layout="button_count" data-width="90" data-show-faces="false"></div>
                                                </div>
                                                <div class="pinterest">
                                                    <a href="http://pinterest.com/pin/create/button/?url={{ Post.Permalink }}&media={{ Post.Cover }}&description={{ Post.Snippet }}" class="pin-it-button" count-layout="horizontal" always-show-count="true">
                                                        <img src="//assets.pinterest.com/images/PinExt.png" title="Pin It" />
                                                    </a>
                                                </div>
                                            {% endif %}
                                        {% endif %}
                                        <div class="ob-share">
                                            {{ Post.RepostButton }}
                                        </div>
                                    </div>
                                </div>
                                {% if Navigation.PreviousPost is not empty or Navigation.NextPost is not empty %}
                                    <!-- Pagination -->
                                    <div id="pagination">
                                         {% if Navigation.PreviousPost is not empty %}
                                            <a href="{{ Navigation.PreviousPost.Permalink }}" class="btn prev" title="{{ Lang.Get('Go to') }} {% if Navigation.PreviousPost.Title is not empty %}&quot;{{ Navigation.PreviousPost.Title|truncate(30) }}&quot;{% else %}{{ Lang.Get('previous post') }}{% endif %}">{{ Lang.Get('Previous') }}</a>
                                        {% endif %}

                                        {% if Navigation.NextPost is not empty %}
                                            <a href="{{ Navigation.NextPost.Permalink }}" class="btn next" title="{{ Lang.Get('Go to') }} {% if Navigation.NextPost.Title is not empty %}&quot;{{ Navigation.NextPost.Title|truncate(30) }}&quot;{% else %}{{ Lang.Get('next post') }}{% endif %}">{{ Lang.Get('Next') }}</a>
                                        {% endif %}
                                        <div class="clear"></div>
                                    </div>
                                {% endif %}
                                {% if not isPage and Post.ShowComments is not empty %}
                                    <div class="bloc-comment" id="comments">
                                        <!-- Comments -->
                                        <div class="item-comments">
                                            {% if Custom("comment") == "Facebook" %}
                                                <div class="fb-comments" data-href="{{ Post.Permalink }}" data-num-posts="10" data-width="600" data-colorscheme="{{ Custom('facebook_comments_theme_color')|default('Light')|lower }}"></div>
                                            {% elseif Custom("comment") == "Disqus" and Custom('comment_disqus') is not empty %}
                                                <div id="disqus_thread"></div>
                                                <script type="text/javascript">
                                                    var disqus_shortname = '{{ Custom('comment_disqus') }}';
                                                    (function() {
                                                        var dsq = document.createElement('script');  dsq.type = 'text/javascript';  dsq.async = true;
                                                        dsq.src = 'http://' + disqus_shortname + '.disqus.com/embed.js';
                                                        (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
                                                    })();
                                                </script>
                                                <noscript>Please enable JavaScript to view the <a href="http://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
                                            {% else %}
                                                <h6>{{ Lang.Get('Comment this post') }}</h6>
                                                {{ Post.Comments }}
                                            {% endif %}
                                        </div>
                                    </div>
                                {% endif %}
                            {% endif %}
                        </div>
                        <div class="sidebar">
                            {% if Custom('custom_hide_widget_about_blog') == false or Custom('custom_hide_widget_biography') == false %}
                                <div id="featured">
                                    <ul>
                                        {% if Custom('custom_hide_widget_about_blog') == false %}
                                            <li>
                                                <h3>{{ Lang.Get('About this blog') }}</h3>
                                                {% if Blog.Avatar is not empty %}
                                                    <p class="imgholder">
                                                        <a href="{{ Blog.Avatar }}" class="avatar" title="{{ Blog.Title }}">
                                                            <img src="{{ Blog.Avatar | resize(210,90) | crop }}" width="210" height="90" alt="{{ Blog.Title }}" />
                                                        </a>
                                                    </p>
                                                {% endif %}
                                                <p>{{ Blog.Description }}</p>
                                            </li>
                                        {% endif %}
                                        {% if Custom('custom_hide_widget_biography') == false %}
                                            <li>
                                                <h3>
                                                    {% if Blog.Owner.Nickname is not empty %}
                                                        {{ Blog.Owner.Nickname }}
                                                    {% else %}
                                                        {{ Lang.Get('About me') }}
                                                    {% endif %}
                                                </h3>
                                                {% if Blog.Owner.Avatar is not empty %}
                                                    <p class="imgholder">
                                                        <a href="{{ Blog.Owner.Avatar }}" class="avatar" title="{{ Blog.OWner.Nickname }}">
                                                            <img src="{{ Blog.Owner.Avatar | resize(210,90) | crop }}" width="210" height="90" alt="{{ Blog.Title }}" />
                                                        </a>
                                                    </p>
                                                {% endif %}
                                                <p>{{ Blog.Owner.Bio }}</p>
                                                {% if not isSpecial %}
                                                    <p class="readmore">
                                                        <a href="{{ Blog.Url }}/contact">{{ Lang.Get('Contact me') }} »</a>
                                                    </p>
                                                {% endif %}
                                            </li>
                                        {% endif %}
                                    </ul>
                                </div>
                            {% endif %}

                            {% if Custom('custom_hide_widget_follow_us') == false %}
                                <div class="subnav">
                                    <h3>{{ Lang.Get('Follow us') }}</h3>
                                    <ul>
                                        {% if Custom('facebook_username') is not empty %}
                                            <li><a href="http://facebook.com/{{ Custom('facebook_username') }}" target="_blank">Facebook</a></li>
                                        {% endif %}
                                        {% if Custom('twitter_username') is not empty %}
                                            <li><a href="http://twitter.com/{{ Custom('twitter_username') }}" target="_blank">Twitter</a></li>
                                        {% endif %}
                                        {% if Custom('flickr_username') is not empty %}
                                            <li><a href="http://flickr.com/{{ Custom('flickr_username') }}" target="_blank">Flickr</a></li>
                                        {% endif %}
                                        <li><a href="{{ Blog.RssUrl }}" target="_blank">RSS</a></li>
                                    </ul>
                                </div>
                            {% endif %}
                        </div>
                        <div class="clear"></div>
                    </div>
                </div>
            {% else %}
                {% if isIndex %}
                    <!-- shortcut:[Slider] -->
                    <div class="slideshow">
                        <div id="mySlides">
                            {% if Custom('pinned_tag') is not empty %}
                                {% list Blog.TaggedPosts(Custom('pinned_tag')) %}
                                    <div class="m-slides">
                                        {% if TaggedPost.Cover is not empty %}
                                            <img src="{{ TaggedPost.Cover|resize(960, 426)|crop }}" alt="{{ TaggedPost.Title }}" />
                                        {% elseif Custom('custom_home_slider_default_image') is not empty %}
                                            <img src="{{ Custom('custom_home_slider_default_image')|resize(960, 426)|crop }}" alt="{{ TaggedPost.Title }}" />
                                        {% else %}
                                            <img src="http://assets.over-blog-kiwi.com/themes/32/images/slider3.jpg" alt="{{ TaggedPost.Title }}" />
                                        {% endif %}
                                        <span>
                                            {% if TaggedPost.Title is not empty %}
                                                <b class="slideheading">{{ TaggedPost.Title }}</b>
                                            {% endif %}
                                            {{ TaggedPost.Snippet }}
                                            <a href="{{ TaggedPost.Permalink }}" title="{{ TaggedPost.Title }}" class="readmore">{{ Lang.Get('Read More!') }}</a>
                                        </span>
                                    </div>
                                {% endlist %}
                            {% else %}
                                {% list Blog.Posts(10) %}
                                    <div class="m-slides">
                                        {% if Post.Cover is not empty %}
                                            <img src="{{ Post.Cover|resize(960, 426)|crop }}" alt="{{ Post.Title }}" />
                                        {% elseif Custom('custom_home_slider_default_image') is not empty %}
                                            <img src="{{ Custom('custom_home_slider_default_image')|resize(960, 426)|crop }}" alt="{{ Post.Title }}" />
                                        {% else %}
                                            <img src="http://assets.over-blog-kiwi.com/themes/32/images/slider3.jpg" alt="{{ Post.Title }}" />
                                        {% endif %}
                                        <span>
                                            {% if Post.Title is not empty %}
                                                <b class="slideheading">{{ Post.Title }}</b>
                                            {% endif %}
                                            {{ Post.Snippet }}
                                            <a href="{{ Post.Permalink }}" title="{{ Post.Title }}" class="readmore">{{ Lang.Get('Read More!') }}</a>
                                        </span>
                                    </div>
                                {% endlist %}
                            {% endif %}
                        </div>

                        <section class="jFlowPrev">
                            <div></div>
                        </section>
                        <section class="jFlowNext">
                            <div></div>
                        </section>

                        <div id="myController">
                            {% if Custom('pinned_tag') is not empty %}
                                {% list Blog.TaggedPosts(Custom('pinned_tag')) %}
                                    <span class="jFlowControl"></span>
                                {% endlist %}
                            {% else %}
                                {% list Blog.Posts(10) %}
                                    <span class="jFlowControl"></span>
                                {% endlist %}
                            {% endif %}
                        </div>
                    </div>

                    {% if Custom('custom_hide_widget_biography') == false %}
                        </div>
                            <div class="callout about">
                                <div class="calloutcontainer">
                                    {% if Blog.Owner.Nickname is not empty and Blog.Owner.Bio is not empty %}
                                        <div class="container bio">
                                            <p class="title">{{ Lang.Get('About me') }}</p>
                                            <a title="{{ Lang.Get('Contact') }} {{ Blog.Owner.Nickname }}" href="/contact" class="more">{{ Lang.Get('Contact') }}</a>
                                            <p class="m-bio">
                                                {% if Blog.Owner.Avatar is not empty %}
                                                    <a href="{{ Blog.Owner.Avatar }}" class="avatar ob-pull-left ob-media-left" title="{{ Blog.Owner.Nickname }}">
                                                        <img class="m-avatar-img" src="{{ Blog.Owner.Avatar|resize(100,80)|crop }}" alt="" width="100" height="80" />
                                                    </a>
                                                {% endif %}
                                                {{ Blog.Owner.Bio }}
                                            </p>
                                        </div>
                                    {% endif %}
                                    <div class="clear"></div>
                                    <div class="calloutoverlay"></div>
                                    <div class="calloutoverlaybottom"></div>
                                </div>
                            </div>
                        <div class="container">
                    {% endif %}
                {% endif %}
                <div id="home_featured2">
                    {% if isSearch %}
                        <h2>{{ Lang.Get('Search results for') }} “{{ Search.Keyword }}”</h2>
                    {% elseif isTag %}
                        <h2>{{ Lang.Get('Post(s) with') }} #{{ Tag.Title }} tag</h2>
                    {% endif %}
                    <ul>
                        {% list Posts %}
                            <li>
                                <div class="imgholder">
                                    <a title="{{ Lang.Get('More about') }} {% if Post.Title is not empty %}&quot; {{ Post.Title }}&quot; {% else %}{{ Lang.Get('this post') }}{% endif %}" href="{{ Post.PermaLink }}">
                                        {% if Post.Cover is not empty %}
                                            <img class="ob-media ob-img ob-pull-left ob-media-left" src="{{ Post.Cover|resize(275,145)|crop }}" alt="" width="275" height="145" />
                                        {% elseif Custom('custom_default_post_thumbnail') is not empty %}
                                            <img class="ob-media ob-img ob-pull-left ob-media-left" src="{{ Custom('custom_default_post_thumbnail')|resize(275, 145)|crop }}" alt="" width="275" height="145" />
                                        {% else %}
                                            <img class="ob-media ob-img ob-pull-left ob-media-left" src="http://assets.over-blog-kiwi.com/themes/32/images/thumb.jpg" width="275" height="145" alt="" />
                                        {% endif %}
                                    </a>
                                </div>
                                <h2 class="post-title">
                                    <a title="{{ Post.Snippet }}" href="{{ Post.PermaLink }}">
                                        {% if Post.Title is not empty %}
                                            {{ Post.Title }}
                                        {% else %}
                                            {{ Lang.Get('Published from') }}
                                            {% list Post.Sources %}
                                                {{ Lang.Get(Source) }} {% if not loop.last %}{{ Lang.Get('and') }}{% endif %}
                                            {% endlist %}
                                        {% endif %}
                                    </a>
                                </h2>
                                <p class="entry">
                                    {{ Post.Snippet }}
                                </p>
                            </li>
                            {% if loop.index % 3 == 0 %}
                                <div class="clear"></div>
                            {% endif %}
                        {% else %}
                            <p>{{ Lang.Get('No result for this expression') }}</p>
                        {% endlist %}
                    </ul>
                    <!-- Pagination -->
                    {% if Navigation.PreviousPage is not empty or Navigation.NextPage is not empty %}
                        <div id="pagination">
                            {% if Navigation.PreviousPage is not empty %}
                                <a href="{{ Navigation.PreviousPage }}" class="prev">{{ Lang.Get('Previous') }}</a>
                            {% endif %}

                            {% if Navigation.NextPage is not empty %}
                                <a href="{{ Navigation.NextPage }}" class="next">{{ Lang.Get('Next') }} </a>
                            {% endif %}
                            <div class="clear"></div>
                        </div>
                    {% endif %}
                </div>
            {% endif %}
        </div>
        <div class="callout">
            <div class="calloutcontainer">
                <div class="container">
                    <div class="footbox">
                        <!-- Archives -->
                        {% if Custom('custom_hide_widget_blog_archive') == false %}
                            {% if Blog.Archives is not empty %}
                                <h3>{{ Lang.Get('Blog archives') }}</h3>
                                <ul id="archivelist">
                                    {% list Blog.Archives %}
                                        <li>
                                            <a href="#" class="plustext" onclick="javascript: return switch_arch('{{Archive.Year}}')">{{ Archive.Year }}</a>
                                            <ul id="arch_{{Archive.Year}}" class="arch_month">
                                            {% list Archive.Months %}
                                                {% if Month.PostCount > 0 %}
                                                    <li><a href="{{Month.Url}}">{{ Lang.Get(Month.Name) }}</a> ({{ Month.PostCount }})</li>
                                                {% endif %}
                                            {% endlist %}
                                            </ul>
                                        </li>
                                    {% endlist %}
                                </ul>
                            {% endif %}
                        {% endif %}
                    </div>
                    {% if Custom('custom_hide_widget_follow_us') == false %}
                        <div class="footbox last">
                            <!-- Social networks -->
                            <h3>{{ Lang.Get('We are social!') }}</h3>
                            <div id="social">
                                {% if Custom('facebook_username') is not empty %}
                                    <a href="http://www.facebook.com/{{ Custom('facebook_username') }}" class="s3d facebook" target="_blank">
                                        Facebook <span class="icons facebook"></span>
                                    </a>
                                {% endif %}
                                {% if Custom('twitter_username') is not empty %}
                                    <a href="http://www.twitter.com/{{ Custom('twitter_username') }}" class="s3d twitter" target="_blank">
                                        Twitter <span class="icons twitter"></span>
                                    </a>
                                {% endif %}
                                {% if Custom('flickr_username') is not empty %}
                                    <a href="http://www.flickr.com/{{ Custom('flickr_username') }}" class="s3d flickr" target="_blank">
                                        Flickr <span class="icons flickr"></span>
                                    </a>
                                {% endif %}
                                {% if Custom('youtube_username') is not empty %}
                                    <a href="http://www.youtube.com/user/{{ Custom('youtube_username') }}" class="s3d youtube" target="_blank">
                                        Youtube <span class="icons youtube"></span>
                                    </a>
                                {% endif %}
                                <a href="{{ Blog.RssUrl}}" class="s3d rss" target="_blank">
                                    Rss <span class="icons rss"></span>
                                </a>
                                <div class="clear"></div>
                            </div>
                        </div>
                    {% endif %}

                    <!-- Custom tags group 3 -->
                    {% if Custom('custom_hide_widget_last_post') == false %}
                        <div class="latestgallery">
                            <h3>{{ Lang.Get('Last posts') }}</h3>
                            <ul>
                                {% list Blog.Posts(4) %}
                                    <li>
                                        <a title="{{ Post.Snippet}}" href="{{ Post.PermaLink }}">
                                            {% if Post.Cover is not empty %}
                                                <img  src="{{ Post.Cover|resize(150, 95)|crop }}" alt="{{ Post.Title }}" width="150" height="95" />
                                            {% elseif Custom('custom_default_post_thumbnail') is not empty %}
                                                <img src="{{ Custom('custom_default_post_thumbnail')|resize(150, 95)|crop }}" alt="{{ TaggedPost.Title }}" width="150" height="95" />
                                            {% else %}
                                                <img src="http://assets.over-blog-kiwi.com/themes/32/images/thumb.jpg" width="150" height="95" alt="{{ TaggedPost.Title }}" />
                                            {% endif %}
                                        </a>
                                    </li>
                                {% endlist %}
                            </ul>
                        </div>
                    {% endif %}
                    <div class="clear"></div>
                    <div class="calloutoverlay"></div>
                    <div class="calloutoverlaybottom"></div>
                </div>
            </div>
        </div>
        <footer class="footer">
            <div class="container">
                {% if Custom('custom_footer_left') is not empty %}
                    <p class="left">{{ Custom('custom_footer_left') }}</p>
                {% endif %}
                <p class="right">
                    {{ Lang.Get('Hosted by') }} <a href="http://www.over-blog.com" target="_blank">Overblog</a>
                </p>
            </div>
        </footer>

        <script type="text/javascript">
            {% if Custom('custom_hide_widget_archives') == false %}
                // Archives
                function switch_arch(year) {
                    'use strict';
                    var archiveYear, archiveMonths, displayStyle;
                    displayStyle = '';
                    archiveYear = document.getElementById('arch_' + year);
                    if (typeof (archiveYear) !== 'undefined') {
                        if (archiveYear.style.display !== 'none') {
                            // Hide years without months
                            archiveMonths = archiveYear.getElementsByTagName('li');
                            if (!archiveMonths.length) {
                                archiveYear.parentNode.style.display = 'none';
                            }
                            // Onclick management
                            displayStyle = 'none';
                        }
                        archiveYear.style.display = displayStyle;
                    }
                    return false;
                }
