        {% else %}
            {% if Custom('ob_style_custom_articles') %}
<link type="text/css" href="{{ Custom('ob_style_custom_articles') }}" rel="stylesheet" />
            {% endif %}
        {% endif %}
    {% else %}
        {% if Custom('ob_style_custom_articles') %}
<link type="text/css" href="{{ Custom('ob_style_custom_articles') }}" rel="stylesheet" />
        {% endif %}
    {% endif %}
{% else %}
    {% if Custom('ob_style_custom_home') %}
<link type="text/css" href="{{ Custom('ob_style_custom_home') }}" rel="stylesheet" />
    {% else %}
        {% if Custom('ob_style_custom_articles') %}
<link type="text/css" href="{{ Custom('ob_style_custom_articles') }}" rel="stylesheet" />
        {% endif %}
    {% endif %}
{% endif %}

