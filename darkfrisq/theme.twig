            .ob-section-quote .ob-quote p {color: white;font-size:1.2em; margin: 40px 70px 0px;}
            .ob-section-quote p:before {margin: -82px 0 0 -87px;}
            .ob-section-quote p:after {margin: -30px 0 0 -0;}
            .ob-section-quote p.ob-author,
