            .style_red .ob-section-quote .ob-link:hover,
            .style_red .ob-section-file .ob-ctn a.ob-link,
            .style_red .ob-section-file .ob-c a.ob-link:visited,
            .style_red .ob-section-link p.ob-title,
            .style_red .ob-section-link p.ob-title .ob-link,
            .style_red .ob-section-quote .ob-quote p:before,
            .style_red .ob-section-quote .ob-quote p:after {color: #d80003;}

            .style_pink .ob-section-link .ob-url .ob-link:hover,
            .style_pink .ob-section-file .ob-ctn a.ob-link:hover,
            .style_pink .ob-section-quote .ob-link:hover,
            .style_pink .ob-section-file .ob-ctn a.ob-link,
            .style_pink .ob-section-file .ob-c a.ob-link:visited,
            .style_pink .ob-section-link p.ob-title,
            .style_pink .ob-section-link p.ob-title .ob-link,
            .style_pink .ob-section-quote .ob-quote p:before,
            .style_pink .ob-section-quote .ob-quote p:after {color: #dd2267;}

            .style_blue .ob-section-link .ob-url .ob-link:hover,
            .style_blue .ob-section-file .ob-ctn a.ob-link:hover,
            .style_blue .ob-section-quote .ob-link:hover,
            .style_blue .ob-section-file .ob-ctn a.ob-link,
            .style_blue .ob-section-file .ob-c a.ob-link:visited,
            .style_blue .ob-section-link p.ob-title,
            .style_blue .ob-section-link p.ob-title .ob-link,
            .style_blue .ob-section-quote .ob-quote p:before,
            .style_blue .ob-section-quote .ob-quote p:after {color: #16aff8;}

            .style_orange .ob-section-link .ob-url .ob-link:hover,
            .style_orange .ob-section-file .ob-ctn a.ob-link:hover,
            .style_orange .ob-section-quote .ob-link:hover,
            .style_orange .ob-section-file .ob-ctn a.ob-link,
            .style_orange .ob-section-file .ob-c a.ob-link:visited,
            .style_orange .ob-section-link p.ob-title,
            .style_orange .ob-section-link p.ob-title .ob-link,
            .style_orange .ob-section-quote .ob-quote p:before,
            .style_orange .ob-section-quote .ob-quote p:after {color: #f97d16;}

            .style_neutral .ob-section-link .ob-url .ob-link:hover,
            .style_neutral .ob-section-file .ob-ctn a.ob-link:hover,
            .style_neutral .ob-section-quote .ob-link:hover,
            .style_neutral .ob-section-file .ob-ctn a.ob-link,
            .style_neutral .ob-section-file .ob-c a.ob-link:visited,
            .style_neutral .ob-section-link p.ob-title,
            .style_neutral .ob-section-link p.ob-title .ob-link,
            .style_neutral .ob-section-quote .ob-quote p:before,
            .style_neutral .ob-section-quote .ob-quote p:after {color: #D8D8D8;}

            .ob-section-link .ob-title .ob-link,
            .ob-section-link .ob-snippet,
            .ob-section-link .ob-url .ob-link {
                font-family: Georgia,serif;
            }

            .ob-section-link .ob-ctn,
            .ob-section-file .ob-ctn,
            .ob-section-quote {
                border: 1px solid #636360;
                background: #3d3e39; background: rgba(255, 255, 255, 0);
            }

            /* Medias */
            .ob-cell,
            .ob-img,
            .ob-media,
            .ob-video,
            .ob-slideshow,
            .ob-section-map .ob-desc,
            .ob-section-images img,
            .yui3-slideshow-content {
                max-width: 570px;
            }
            .ob-video iframe,
            .ob-video object,
            .ob-video embed,
            .ob-section-html iframe,
            .ob-section-html object,
            .ob-section-html embed{
                width: 570px;
                max-height: 370px;
            }
            .ob-section-html iframe.twitter-tweet[style]{
                max-height: none;
                border:none !important;
            }
            .ob-map div {color: #272727;}
            /* Section image */
            .ob-row-1-col img {width: 570px;}
            .ob-row-2-col img {width: 285px;}
            .ob-row-3-col img {width: 190px;}
            .ob-text li:last-child {margin-bottom: 15px;}
            .ob-text li p {margin-bottom: 5px;}
            .ob-text pre {
                width: 570px;
                white-space: pre-wrap; /* CSS 3 */
                white-space: -moz-pre-wrap; /* Mozilla depuis 1999 */
                white-space: -pre-wrap; /* Opera 4 & 6 */
                white-space: -o-pre-wrap; /* Opera 7 */
                word-wrap: break-word; /* IE 5.5+ */
                overflow: auto;/* IE 6 */
            }
            .ob-section-audio .obsoundplayer .obsoundplayername {
                height: 31px;
                overflow: hidden;
                width: 200px;
            }

            .ob-form + a {display: block; margin-top: 80px; text-align: center;}
