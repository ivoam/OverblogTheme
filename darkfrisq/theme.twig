    </head>
    <body>
        <div class="header">
            <div id="ln_0" class="ln">
                <div id="cl_0_0" class="cl">
                    <a href="{{ Blog.Url }}">{{ Blog.Title }}</a>
                </div>
                <div class="clear"></div>
            </div>
            <div id="top">
                <h1>
                    <a href="{{ Blog.Url }}" title="{{ Blog.Description }}" class="topLien">{{ Blog.Title }}</a>
                </h1>
            </div>
        </div>
        <div id="global">
            <div id="ln_1" class="ln">
                <div id="cl_1_0" class="cl">
                    <div class="column_content">
                        <div>
                            {# Pagination #}
                            {% if Navigation.PreviousPage is not empty or Navigation.NextPage is not empty or Navigation.PreviousPost is not empty or Navigation.NextPost is not empty %}
                                {% if isIndex %}
                                    <div class="before_articles">
                                        <div class="pagination clearfix">
                                            {% if Navigation.PreviousPage is not empty %}
                                            <a href="{{ Navigation.PreviousPage }}" class="previous">&laquo; {{ Lang.Get('Previous page') }}</a>
                                            {% elseif Navigation.PreviousPost is not empty %}
                                            <a href="{{ Navigation.PreviousPost.Permalink }}" title="{{ Navigation.PreviousPost.Title }}" class="previous">&laquo; {% if Navigation.PreviousPost.Title is not empty %}{{ Navigation.PreviousPost.Title | truncate(25) }}{% else %}{{ Lang.Get('Previous post') }}{% endif %}</a>
                                            {% endif %}
                                            {% if Navigation.NextPage is not empty %}
                                            <a href="{{ Navigation.NextPage }}" class="next">{{ Lang.Get('Next page') }} &raquo;</a>
                                            {% elseif Navigation.NextPost is not empty %}
                                            <a href="{{ Navigation.NextPost.Permalink }}" title="{{ Navigation.NextPost.Title }}" class="next">{% if Navigation.NextPost.Title is not empty %}{{ Navigation.NextPost.Title | truncate(25) }}{% else %}{{ Lang.Get('Next post') }}{% endif %} &raquo;</a>
                                            {% endif %}
                                        </div>
                                    </div>
                                {% endif %}
                            {% endif %}

                            {# List title #}
                            {% if isSearch %}
                            <h3 class="list-title">{{ Lang.Get ('Search for') }} &ldquo;{{ Search.Keyword }}&rdquo;</h1>
                            {% endif %}
                            {% if isTag %}
                            <h3 class="list-title">{{ Lang.Get('Posts with') }} #<strong>{{ Tag.Title }}</strong></h1>
                            {% endif %}

                            {# Posts #}
                            {% if Posts is empty %}

                            {% if isSearch %}
                            <div class="contenuArticle">
                                <p>
                                    {{ Lang.Get('No result') }} !
                                </p>
                            </div>
                            {% elseif isSpecial %}
                                <div class="special">
                                    {{ SpecialContent }}
                                </div>
                            {% else %}
                            <div class="contenuArticle">
                                <p>
                                    {{ Lang.Get('No post') }}.
                                </p>
                            </div>
                            {% endif %}

                            {% else %}

                            {# shortcut:[Posts list] #}
                            {% list Posts %}

                            <div class="article index_{{ loop.index }} {% if (loop.index % 2) == 0 %}odd{% else %}even{% endif %}">
                                <div class="option beforeArticle">
                                    {% if not isPage %}
                                        <div class="date">
                                            <time datetime="{{ Post.Date }}">{{ Post.Date|datel(Lang.Get('EEEE')) | title }}, {{ Post.Date|datel(Lang.Get('Default date format')) | title }} {{ Post.Date | date('h:m') }}</time>
                                        </div>
                                    {% endif %}
                                </div>
                                <div class="divTitreArticle">
                                    <div class='post-title'>
                                        <h2>
                                            {% if isSingle %}
                                            <a href="{{ Post.Permalink }}" class="titreArticle" title="{{ Post.Snippet }}">{% if Post.Title is not empty %}{{ Post.Title }}{% else %}{{ Lang.Get('From') }} {% list Post.Sources %}{{ Lang.Get(Source) }}{% if not loop.last %}{{ Lang.Get('and') }} {% endif %}{% endlist %}{% endif %}</a>
                                            {% else %}
                                            <a href="{{ Post.Permalink }}" class="titreArticle" title="{{ Lang.Get('More about') }} {% if Post.Title is not empty %}&quot;{{ Post.Title }}&quot;{% else %}{{ Lang.Get('this post') }}{% endif %}">{% if Post.Title is not empty %}{{ Post.Title }}{% else %}{{ Lang.Get('From') }} {% list Post.Sources %}{{ Lang.Get(Source) }}{% if not loop.last %}{{ Lang.Get('and') }} {% endif %}{% endlist %}{% endif %}</a>
                                            {% endif %}
                                        </h2>
                                    </div>
                                </div>
                                <div class="contenuArticle">
                                     {% if isSingle or Custom('display_mode') == 'All' %}
                                        {{ Post.Body }}
                                    {% elseif not isSingle and Custom('display_mode') != 'Summary' and Custom('display_mode') != 'All'  %}
                                        {{ Post.Body(Custom('display_mode')) }}
                                    {% else %}
                                        {% if Post.Cover is not empty %}
                                            <a href="{{ Post.Permalink }}" title="{{ Lang.Get('More about') }} {% if Post.Title is not empty %}&quot;{{ Post.Title }}&quot;{% else %}{{ Lang.Get('this post') }}{% endif %}">
                                                <img src="{{ Post.Cover|resize(570,250)|crop }}" title="" alt="" width="570"/>
                                            </a>
                                        {% elseif Custom('default_post_cover') is not empty %}
                                            <a href="{{ Post.Permalink }}" title="{{ Lang.Get('More about') }} {% if Post.Title is not empty %}&quot;{{ Post.Title }}&quot;{% else %}{{ Lang.Get('this post') }}{% endif %}">
                                                <img src="{{ Custom('default_post_cover')|resize(570,250)|crop }}" title="" alt="" width="570"/>
                                            </a>
                                        {% endif %}
                                        <p>{{ Post.Snippet }}</p>
                                    {% endif %}
                                </div>
                                <div class="clear"></div>
                            </div>
                            <div class="clear"></div>

                            {# Author #}
                            <div class="option afterArticle">
                                <p>
                                {% if Post.Author.Nickname is not empty %}
                                    <span class="publishedBy">{{ Custom('text_publishedby') }} {{ Post.Author.Nickname }}</span>
                                {% endif %}
                                {% if isPage == false and Post.ShowComments is not empty %}
                                    {% if Custom("comment") == "Disqus" and Custom('comment_disqus') is not empty %}
                                        - <a href="{{ Post.Permalink }}#disqus_thread" title="{{ Lang.Get('comments') }}" class="comments"></a>
                                    {% else %}
                                        - <a href="{{ Post.PermaLink }}#comments" title="{{ Lang.Get('comments') }}" class="comments">
                                            {% if Custom("comment") == "Facebook" %}
                                                <fb:comments-count href="{{ Post.Permalink }}"></fb:comments-count>
                                                {{ Lang.Get('comments') }}
                                            {% else %}
                                                {{ Post.CommentCount(Lang.Get('ncomment'), Lang.Get('ncomments')) }}
                                            {% endif %}
                                        </a>
                                    {% endif %}
                                {% endif %}
                                    {% list Post.Tags %}
                                        {% if loop.first == 'true' %}
                                        -
                                        {% endif %}
                                        <a href="{{ Tag.Url }}">{{ Tag.Title }}</a>{% if loop.last is empty %}, {% endif %}
                                    {% endlist %}
                                </p>
                            </div>
                            {% if loop.last  %}
                                {# Pagination #}
                                {% if Navigation.PreviousPage is not empty or Navigation.NextPage is not empty or Navigation.PreviousPost is not empty or Navigation.NextPost is not empty %}
                                    {% if isPage == false %}
                                        <div class="before_articles">
                                            <div class="pagination clearfix">
                                                {% if Navigation.PreviousPage is not empty %}
                                                <a href="{{ Navigation.PreviousPage }}" class="previous">&laquo; {{ Lang.Get('Previous page') }}</a>
                                                {% elseif Navigation.PreviousPost is not empty %}
                                                <a href="{{ Navigation.PreviousPost.Permalink }}" title="{{ Navigation.PreviousPost.Title }}" class="previous">&laquo; {% if Navigation.PreviousPost.Title is not empty %}{{ Navigation.PreviousPost.Title | truncate(25) }}{% else %}{{ Lang.Get('Previous post') }}{% endif %}</a>
                                                {% endif %}
                                                <a href="#" title="{{ Lang.Get('Go top') }}" class="top">{{ Lang.Get('Top') }}</a>
                                                {% if Navigation.NextPage is not empty %}
                                                <a href="{{ Navigation.NextPage }}" class="next">{{ Lang.Get('Next page') }} &raquo;</a>
                                                {% elseif Navigation.NextPost is not empty %}
                                                <a href="{{ Navigation.NextPost.Permalink }}" title="{{ Navigation.NextPost.Title }}" class="next">{% if Navigation.NextPost.Title is not empty %}{{ Navigation.NextPost.Title | truncate(25) }}{% else %}{{ Lang.Get('Next post') }}{% endif %} &raquo;</a>
                                                {% endif %}
                                            </div>
                                        </div>
                                    {% endif %}
                                {% endif %}
                            {% endif %}

                            {# Share #}
                            {% if isSingle %}
                                {% if Custom ('share_service') is not empty %}
                                    <div class="share clearfix">
                                        {{ Post.Share }}
                                    </div>
                                {% endif %}
                            {% endif %}

                            {# Comments #}
                            {% if isSingle and isPage == false and Post.ShowComments is not empty %}
                                <div class="item-comments">
                                    <h4 class="h4commentMessage" id="comments">{{ Lang.Get('Comments') }}</h4>
                                    {% if Custom("comment") == "Facebook" %}
                                    <div class="fb-comments" data-href="{{ Post.Permalink }}" data-num-posts="2" data-width="570"></div>
                                    {% elseif Custom("comment") == "Disqus" and Custom('comment_disqus') is not empty %}
                                        <div id="disqus_thread"></div>
                                        <script type="text/javascript">
                                        var disqus_shortname = '{{ Custom('comment_disqus') }}';

                                        (function() {
                                        var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
                                        dsq.src = 'http://' + disqus_shortname + '.disqus.com/embed.js';
                                        (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
                                        })();
                                        </script>
                                        <noscript>Please enable JavaScript to view the <a href="http://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
                                    {% else %}
                                        {{Post.Comments}}
                                    {% endif %}
                                </div>
                                {% endif %}
                            {% endlist %} {# Posts #}

                            {% endif %}

                         </div>


                    </div>
                </div>
                <div id="cl_1_1" class="cl">
                    <div class="column_content">
                        {# Blog profile #}
                        {% if (Blog.Avatar or Blog.Description) and Custom("display_widget_profile") != true %}
                        <div class="box profile" >
                            <div class="box-content clearfix">
                                {% if Blog.Avatar is not empty %}
                                <h3 class="avatar">
                                    <a href="{{ Blog.Url }}"><img src="{{ Blog.Avatar | resize(100)|crop }}" alt="{{ Blog.Title }}" /></a>
                                </h3>
                                {% endif %}
                                {% if Blog.Description is not empty %}
                                <p>
                                    {{ Blog.Description }}
                                </p>
                                {% endif %}
                            </div>
                        </div>
                        {% endif %}

                        {# SOCIAL #}
                        {% if Custom('display_widget_social') != true %}
                        <div class="box social" >
                            <div class="box-titre"><h3><span>{{ Lang.Get('Follow us') }}</span></h3></div>
                            <div class="box-content">
                                <ul>
                                    {% if Custom('facebook_username') is not empty %}
                                    <li>
                                        <a href="http://facebook.com/{{ Custom('facebook_username') }}" title="{{ Lang.Get('Follow us on') }} Facebook" >Facebook</a>
                                    </li>
                                    {% endif %}
                                    {% if Custom('twitter_username') is not empty %}
                                    <li>
                                        <a href="http://twitter.com/{{ Custom('twitter_username') }}" title="{{ Lang.Get('Follow us on') }} Twitter">Twitter</a>
                                    </li>
                                    {% endif %}
                                    <li>
                                        <a href="{{ Blog.RssUrl }}" title="{{ Lang.Get('Follow us with') }} RSS">{{ Lang.Get('Flux RSS') }}</a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        {% endif %}

                        {# Search #}
                        {% if Custom('display_widget_search') != true %}
                        <div class="box search" >
                            <div class="box-titre">
                                <h3><span>{{ Lang.Get('Search') }}</span></h3>
                            </div>
                            <div class="box-content">
                                <form action="/search" method="post">
                                    <div class="ob-form-field">
                                        <label class="ob-label visuallyhidden" for="fSearch">{{ Lang.Get('Search') }}</label>
                                        <input type="text" id="fSearch" name="q" placeholder="{{ Lang.Get('Search') }}..." value="{{ Search }}" class="ob-input ob-input-text" />
                                    </div>
                                        <button class="ob-input ob-input-submit" type="submit">{{ Lang.Get('Search') }}!</button>
                                </form>
                            </div>
                        </div>
                        {% endif %}

                        {# Subscribe Form #}
                        {% if Custom('custom_hide_widget_subscribe') == false %}
                            <div class="box subscribe">
                                <div class="box-titre">
                                    <h3><span>{{ Lang.Get('Subscribe') }}</span></h3>
                                </div>
                                <div class="box-content">
                                    <p>{{ Lang.Get('Subscribe to be notified about future posts.') }}</p>
                                    {% email_notification %}
                                </div>
                            </div>
                        {% endif %}

                        {# Navigation #}
                        <div class="box navigation" >
                            <div class="box-titre">
                                <h3><span>{{ Lang.Get('Navigation') }}</span></h3>
                            </div>
                            <div class="box-content">
                                <ul>
                                    <li><a href="{{ Blog.Url }}" title="{{ Lang.Get('Go Home') }}">{{ Lang.Get('Home') }}</a></li>
                                    <li><a href="/contact" title="{{ Lang.Get('Contact us') }}">{{ Lang.Get('Contact') }}</a></li>
                                </ul>
                            </div>
                        </div>

                        {# Pages #}
                        {% if Custom('display_widget_pages') != true and Blog.Pages is not empty %}
                        <div class="box pages" >
                            <div class="box-titre">
                                <h3><span>{{ Lang.Get('Pages') }}</span></h3>
                            </div>
                            <div class="box-content">
                                <ul>
                                    {% list Blog.Pages %}
                                    <li>
                                        <a  href="{{ Page.Url }}" title="{{ Page.Title }}">{{ Page.Title }}</a>
                                    </li>
                                    {% endlist %}
                                </ul>
                            </div>
                        </div>
                        {% endif %}

                        {# Tags #}
                        {% if Custom('display_widget_tags') != true %}
                        <div class="box tags" >
                            <div class="box-titre">
                                <h3><span>{{ Lang.Get('Tags') }}</span></h3>
                            </div>
                            <div class="box-content">
                                <ul>
                                    {% list Blog.PostsTags(10) %}
                                    <li>
                                        <a  href="{{ PostsTag.Url }}"
                                            title="{{ PostsTag.Title }}">
                                            {{ PostsTag.Title }}
                                        </a>
                                        ({{ PostsTag.PostCount }})
                                    </li>
                                    {% endlist %}
                                </ul>
                            </div>
                        </div>
                        {% endif %}

                        {% if Custom('custom_hide_widget_blog_archive') == false and Blog.Archives is not empty %}
                            <div class="box archives">
                                <div class="box-titre">
                                    <h3><span>{{ Lang.Get('Archives') }}</span></h3>
                                </div>
                                <div class="box-content">
                                    <ul id="archivelist">
                                        {% list Blog.Archives %}
                                             <li class="arch_year">
                                                <a href="#" class="year" data-year="{{ Archive.Year }}">{{ Archive.Year }}</a>
                                                <ul id="arch_{{ Archive.Year }}" class="arch_month">
                                                    {% list Archive.Months%}
                                                        {% if Month.PostCount > 0 %}
                                                            <li class="month">
                                                                <a href="{{ Month.Url }}">
                                                                    {{ Lang.Get(Month.Name) }} <span>{{ Month.PostCount }}</span>
                                                                </a>
                                                            </li>
                                                        {% endif %}
                                                    {% endlist %}
                                                </ul>
                                            </li>
                                        {% endlist %}
                                    </ul>
                                </div>
                            </div>
                        {% endif %}

                    </div>
                </div>
                <div class="clear"></div>
            </div>
            <div id="ln_2" class="ln">
                <div id="cl_2_0" class="cl">
                    <div class="column_content">
                        <p>{{ Lang.Get('Hosted by') }} <a href="http://www.over-blog.com" target="_blank">Overblog</a></p>
                    </div>
                </div>
                <div class="clear"></div>
            </div>
        </div>
        <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
        <script src="http://assets.over-blog-kiwi.com/themes/jquery/fancybox2.1.2/jquery.fancybox.pack.js"></script>
        <script type="text/javascript">
            $(document).ready(function() {
                $(".ob-row-1-col a,.ob-row-2-col a,.ob-row-3-col a,.ob-link-img").attr("rel", "fancybox");
                $("a[rel=fancybox]").fancybox({
                    'overlayShow'   : false,
                    'transitionIn'  : 'fadin',
                    'transitionOut' : 'fadin',
                    'type'          : 'image'
                });
            });
        </script>
        {% if isSingle %}
            <!-- Disqus comments count -->
            {% if Custom("comment") == "Disqus" and Custom('comment_disqus') is not empty %}
            <script type="text/javascript">
                var disqus_shortname = "{{ Custom('comment_disqus') }}";
                (function () {
                    var s = document.createElement('script'); s.async = true;
                    s.type = 'text/javascript';
                    s.src = 'http://' + disqus_shortname + '.disqus.com/count.js';
                    (document.getElementsByTagName('HEAD')[0] || document.getElementsByTagName('BODY')[0]).appendChild(s);
                }());
            </script>
            {% endif %}

            <!-- Facebook Script -->
            <div id="fb-root"></div>
            <script type="text/javascript">
                {% if Custom('share_service') == 'Facebook' or Custom('share_service') == 'All' or Custom('comment') == 'facebook' %}
                    (function(d, s, id) {
                        var js, fjs = d.getElementsByTagName(s)[0];
                        if (d.getElementById(id)) return;
                        js = d.createElement(s); js.id = id;
                        js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId={{ Custom('facebook_appid')|default('127607930619601')}}";
                        fjs.parentNode.insertBefore(js, fjs);
                    }(document, 'script', 'facebook-jssdk'));
                {% endif %}
            </script>


            <!-- Google + button -->
            <script type="text/javascript">
              window.___gcfg = {lang: '{{ Blog.Lang }}'};

              (function() {
                var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
                po.src = 'https://apis.google.com/js/plusone.js';
                var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
              })();
            </script>
        {% endif %}
        <script type="text/javascript">
            //Twitter render
            $(document).ready(function(){
                !function(d,s,id){
                    var js, fjs = d.getElementsByTagName(s)[0];
                    if(!d.getElementById(id)){
                        js = d.createElement(s);
                        js.id = id;
                        js.src = "https://platform.twitter.com/widgets.js";
                        fjs.parentNode.insertBefore(js,fjs);
                    }
                }(document,"script","twitter-wjs");
            });

            {% if Custom('display_widget_archives') == false %}
                // Archives
                // Hide years without months
                $(".arch_year").each(function() {
                    if ($(this).find(".arch_month li").length == 0) {
                        $(this).hide();
                    }
                });

                $(".arch_month").hide();
                $('.arch_year > a').click(function(e) {
                    $(this).siblings(".arch_month").slideToggle(1);
                    e.preventDefault();
                });
            {% endif %}
        </script>
        {% if Custom('google_analytics') is not empty %}
            <!-- Google Analytics -->
            <script type="text/javascript">
                window.google_analytics_uacct = '{{Custom('google_analytics')}}';
                var _gaq = _gaq || [];
                _gaq.push(['_setAccount', '{{Custom('google_analytics')}}']);
                _gaq.push(['_trackPageview']);
                _gaq.push(['_trackPageLoadTime']);

                (function() {
                    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
                    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
                    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
                })();
            </script>
        {% endif %}
    </body>
</html>
