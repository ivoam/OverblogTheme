            .ob-section-link p.ob-title {margin-left: 20px; max-height: 36px; overflow: hidden;}
            .ob-section-link p.ob-snippet {margin-bottom: 35px; margin-left: 20px; max-height: 26px; overflow: hidden;font-size:1.2em;}
            .ob-section-link p.ob-url {background: #63645f; background: rgba(255, 255, 255, 0.2);margin: 0; max-height: 24px; max-width: 360px; padding-bottom: 0\9;}
            .ob-section-link .ob-desc {margin-top: 5px;}

            /* Sections color variations */
