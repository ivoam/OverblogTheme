    <body>
        <!-- Init Facebook script -->
        <div id="fb-root"></div>
        <div id="global">
            <div id="ln_0" class="ln">
                <div id="cl_0_0" class="cl">
                    <div class="column_content">
                        <div class="header">
                            <div id="top">
                                <h1 class="title">
                                    <a href="{{ Blog.Url }}"  class="topLien" title="{{ Blog.Description }}">{{ Blog.Title }}</a>
                                </h1>
                                <p class="description">{{ Blog.Description }}</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="clear"></div>
            </div>
            <div id="ln_1" class="ln">
                <div id="cl_1_0" class="cl">
                    <div class="column_content">
                        <div>
                            <!-- Title -->
                            {% if isTag or isSearch %}
                                <p class="list-title">
                                    {% if isTag %}
                                        {{ Lang.Get('Posts with') }} <strong>#{{ Tag.Title }}</strong> {{ Lang.Get('tag') }}
                                    {% else %}
                                        {{ Lang.Get('Search results for') }} &ldquo;<strong>{{ Search.Keyword }}</strong>&rdquo;
                                    {% endif %}
                                </p>
                            {% endif %}

                            <!-- Posts -->
                            {% if Posts is empty %}
                                {% if isSearch %}
                                    <div class="article">
                                        <div class="contenuArticle">
                                            <p>{{ Lang.Get('No result for this expression') }} !</p>
                                        </div>
                                    </div>
                                {% elseif isSpecial %}
                                    <div class="article">
                                        <div class="contenuArticle special">
                                            {{ SpecialContent }}
                                        </div>
                                    </div>
                                {% else %}
                                    <div class="contenuArticle">
                                        <p>{{ Lang.Get('No post') }}…</p>
                                    </div>
                                {% endif %}
                            {% else %}
                                <!-- list posts -->
                                {% list Posts %}
                                    <article>
                                        <div class="article">
                                            <div class="option beforeArticle">
                                                <div class="date">
                                                    <span class="publishedBy">
                                                        {{ Lang.Get('Published by') }} {% if Post.Author is not empty %}{{ Post.Author }} {% else %}{{ Blog.Owner.Nickname }}{% endif %}
                                                    </span>
                                                    {{ Lang.Get('on') }} <time datetime="{{ Post.Date }}">{{ Post.Date|datel (Lang.Get('Default date format'))|title }}</time>
                                                </div>
                                                <span class="tags">
                                                    {% list Post.Tags %}
                                                        {% if loop.first is not empty %}
                                                            <span>{{ Lang.Get('Tagged on') }} : </span>
                                                        {% endif %}
                                                            <a href="{{ Tag.Url }}">#{{ Tag.Title }}</a>{% if loop.last is empty %}, {% endif %}
                                                    {% endlist %}
                                                </span>
                                            </div>
                                            <div class="divTitreArticle">
                                                <h2 class="post-title">
                                                    <a href="{{ Post.Permalink }}" class="titreArticle" title="{{ Post.Snippet }}">
                                                        {% if Post.Title is not empty %}
                                                            {{ Post.Title }}
                                                        {% else %}
                                                            {{ Lang.Get('Published from') }}
                                                            {% list Post.Sources %}
                                                                {{ Lang.Get(Source) }} {% if not loop.last %}{{ Lang.Get('and') }} {% endif %}
                                                            {% endlist %}
                                                        {% endif %}
                                                    </a>
                                                </h2>
                                            </div>
                                            <div class="contenuArticle">
                                                {% if isSingle or Custom('display_mode') == 'All' %}
                                                    {{ Post.Body }}
                                                {% elseif not isSingle and Custom('display_mode') != 'Summary' and Custom('display_mode') != 'All'  %}
                                                    {{ Post.Body(Custom('display_mode')) }}
                                                {% else %}
                                                    {% if Post.Cover is not empty or Custom('post_cover') is not empty %}
                                                        <a href="{{ Post.Permalink }}" title="{{ Post.Snippet }}">
                                                            <img src="{% if Post.Cover is not empty %}{{ Post.Cover|resize(600,350)|crop }}{% else %}{{ Custom('post_cover')|resize(600,350)|crop }}{% endif %}" alt="" title="" width="600" height="350" />
                                                        </a>
                                                    {% endif %}
                                                    <p>{{ Post.Snippet }}</p>
                                                {% endif %}
                                            </div>
                                            <div class="clear"></div>
                                        </div>
                                        <div class="clear"></div>

                                        <!-- Share buttons + comments -->
                                        <div class="afterArticle">
                                            {% if Custom('share_service') != 'None' %}
                                                <div class="share">
                                                   {{ Post.Share }}
                                                </div>
                                            {% endif %}
                                            {% if isSingle == false and Post.ShowComments is not empty %}
                                                {% if Custom("comment") == "Disqus" and Custom('comment_disqus') is not empty %}
                                                    <a class="comment-number comments" href="{{ Post.Permalink }}#disqus_thread" title="{{ Lang.Get('comments') }}"></a>
                                                {% else %}
                                                    <a class="comment-number comments" href="{{ Post.PermaLink }}#comments" title="{{ Lang.Get('comments') }}">
                                                        {% if Custom("comment") == "Facebook" %}
                                                            <fb:comments-count href="{{ Post.Permalink }}"></fb:comments-count>
                                                            {{ Lang.Get('comment(s)') }}
                                                        {% else %}
                                                            {{ Post.CommentCount(Lang.Get('ncomment'), Lang.Get('ncomments')) }}
                                                        {% endif %}
                                                   </a>
                                                {% endif %}
                                            {% endif %}
                                            <div class="clear"></div>

                                            <!-- Pagination -->
                                            {% if Navigation.PreviousPost is not empty or Navigation.NextPost is not empty %}
                                                {% if isPage == false %}
                                                    <div class="pagination clearfix">
                                                        {% if Navigation.PreviousPost is not empty %}
                                                            <a href="{{ Navigation.PreviousPost.Permalink }}" title="{{ Navigation.PreviousPost.Title }}" class="previous">← {% if Navigation.PreviousPost.Title is not empty %}{{ Navigation.PreviousPost.Title|truncate(20) }}{% else %}{{ Lang.Get('Previous post') }}{% endif %}</a>
                                                        {% endif %}
                                                        {% if Navigation.NextPost is not empty %}
                                                            <a href="{{ Navigation.NextPost.Permalink }}" title="{{ Navigation.NextPost.Title }}" class="next">{% if Navigation.NextPost.Title is not empty %}{{ Navigation.NextPost.Title|truncate(20) }}{% else %}{{ Lang.Get('Next post') }}{% endif %} →</a>
                                                        {% endif %}
                                                    </div>
                                                {% endif %}
                                            {% endif %}

                                            <!-- Comments -->
                                            {% if isSingle and isPage == false and Post.ShowComments is not empty %}
                                                <div id="comments" class="item-comments">
                                                    {% if Custom("comment") == "Facebook" %}
                                                        <h3>{{ Lang.Get('Comment this post') }}</h3>
                                                        <div class="fb-comments" data-href="{{ Post.Permalink }}" data-num-posts="2" data-width="600" data-colorscheme="{{ Custom('facebook_comments_color') }}"></div>
                                                    {% elseif Custom("comment") == "Disqus" and Custom('comment_disqus') is not empty %}
                                                        <div id="disqus_thread"></div>
                                                        <script type="text/javascript">
                                                            var disqus_shortname = '{{ Custom('comment_disqus') }}';
                                                            (function() {
                                                                var dsq = document.createElement('script');
                                                                dsq.type = 'text/javascript';
                                                                dsq.async = true;
                                                                dsq.src = 'http://' + disqus_shortname + '.disqus.com/embed.js';
                                                                (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
                                                            })();
                                                        </script>
                                                        <noscript>Please enable JavaScript to view the <a href="http://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
                                                    {% else %}
                                                        <h3>{{ Lang.Get('Comment this post') }}</h3>
                                                        {{ Post.Comments }}
                                                    {% endif %}
                                                </div>
                                            {% endif %}
                                        </div>
                                    </article>
                                {% endlist %}
                            {% endif %}
                        </div>

                        <!-- Pagination -->
                        {% if Navigation.PreviousPage is not empty or Navigation.NextPage is not empty %}
                            {% if isPage == false %}
                                <div class="before_articles">
                                    <div class="pagination clearfix">
                                        {% if Navigation.PreviousPage is not empty %}
                                            <a href="{{ Navigation.PreviousPage }}" class="previous">← {{ Lang.Get('Previous page') }}</a>
                                        {% endif %}
                                        {% if Navigation.NextPage is not empty %}
                                            <a href="{{ Navigation.NextPage }}" class="next">{{ Lang.Get('Next page') }} →</a>
                                        {% endif %}
                                    </div>
                                </div>
                            {% endif %}
                        {% endif %}
                    </div>
                </div>
                <div id="cl_1_1" class="cl">
                    <div class="column_content">
                        <!-- Blog description -->
                        {% if Custom('custom_hide_widget_biography') == false %}
                            <div class="box profile" >
                                <div class="box-titre">
                                    <h3>{{ Lang.Get('About me') }}</h3>
                                </div>
                                <div class="box-content clearfix">
                                    {% if Blog.Avatar or Blog.Owner.Avatar is not empty %}
                                        <p class="avatar">
                                            <a href="{{ Blog.Owner.Avatar }}" title="{% if Blog.Owner.Nickname is not empty %}{{ Blog.Owner.Nickname }}{% else %}{{ Blog.Title }}{% endif %}">
                                                <img src="{% if Blog.Owner.Avatar is not empty %}{{ Blog.Owner.Avatar|resize(100) }}{% else %}{{ Blog.Avatar|resize(100) }}{% endif %}" alt="{{ Blog.Title }}" width="100" />
                                            </a>
                                        </p>
                                    {% endif %}
                                    {% if Blog.Owner.Bio is not empty %}
                                        <p>“{{ Blog.Owner.Bio }}”</p>
                                    {% endif %}
                                    {% if Blog.Owner.Nickname is not empty %}
                                        <p class="blog-owner-nickname">{{ Lang.Get('Written by') }} {{ Blog.Owner.Nickname }}</p>
                                    {% endif %}
                                </div>
                            </div>
                        {% endif %}

                        <!-- Search -->
                        {% if Custom('custom_hide_widget_search_form') == false %}
                            <div class="box search" >
                                <div class="box-titre">
                                    <h3>
                                        <span>{{ Lang.Get('Search') }}</span>
                                    </h3>
                                </div>
                                <div class="box-content">
                                    <form class="search" action="/search" method="post">
                                        <div>
                                            <input type="text" name="q" placeholder="{{ Lang.Get('Search') }}" value="{{ Search.Keyword }}" />
                                            <button type="submit">{{ Lang.Get('OK') }}</button>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        {% endif %}

                         <div class="box subscribe">
                            <div class="box-titre">
                                <h3><span>{{ Lang.Get('Subscribe') }}</span></h3>
                            </div>
                            <div class="box-content">
                                <p>{{ Lang.Get('Subscribe to be notified about future posts.') }}</p>
                                {% email_notification %}
                            </div>
                        </div>

                        <!-- Navigation -->
                        <div class="box navigation" >
                            <div class="box-titre">
                                <h3>
                                    <span>{{ Lang.Get('Pages') }}</span>
                                </h3>
                            </div>
                            <div class="box-content">
                                <ul>
                                    <li>
                                        <a href="{{ Blog.Url }}">{{ Lang.Get('Home') }}</a>
                                    </li>
                                        {% list Blog.ListPages %}
                                            <li>
                                                <a  href="{{ ListPage.Url }}" title="{{ ListPage.Title }}">{{ ListPage.Title }}</a>
                                            </li>
                                       {% endlist %}
                                    <li>
                                        <a href="{{ Blog.Url }}/contact">{{ Lang.Get('Contact') }}</a>
                                    </li>
                                </ul>
                            </div>
                        </div>


                        {% if Custom('custom_display_widget_last_posts') == false %}
                            <!-- Last posts -->
                            <div class="box last" >
                                <div class="box-titre">
                                    <h3>
                                        <span>{{ Lang.Get('Last Posts') }}</span>
                                    </h3>
                                </div>
                                <div class="box-content">
                                    <ul class="clearfix">
                                        {% list Blog.Posts(6) %}
                                            {% if Post.Cover is not empty or Custom('post_cover') is not empty %}
                                                <li class="left">
                                                    <h3>
                                                        <a href="{{ Post.Permalink }}">
                                                            <img src="{% if Post.Cover is not empty %}{{ Post.Cover|resize(126,90)|crop }}{% else %}{{ Custom('post_cover')|resize(126,90)|crop }}{% endif %}" alt="{{Post.Title}}" width="126" height="90" />
                                                        </a>
                                                    </h3>
                                                </li>
                                            {% endif %}
                                        {% endlist %}
                                    </ul>
                                </div>
                            </div>
                        {% endif %}


                        {% if Custom('tag_number') > 0 %}
                            <!-- Tags -->
                            <div class="box tags" >
                                <div class="box-titre">
                                    <h3>
                                        <span>{{ Lang.Get('Tags') }}</span>
                                    </h3>
                                </div>
                                <div class="box-content">
                                    <ul>
                                        {% list Blog.PostsTags(Custom('tag_number')) %}
                                            <li>
                                                <a href="{{ PostsTag.Url }}" title="{{ PostsTag.Title }}">#{{ PostsTag.Title }}</a> <span class="number">{{ PostsTag.PostCount }}</span>
                                            </li>
                                        {% endlist %}
                                    </ul>
                                </div>
                            </div>
                        {% endif %}


                        {% if Custom('custom_hide_widget_follow_us') == false %}
                             <!-- Follow us -->
                            <div class="box follow" >
                                <div class="box-titre">
                                    <h3>
                                        <span>{{ Lang.Get('Follow us') }}</span>
                                    </h3>
                                </div>
                                <div class="box-content">
                                    <ul>
                                        {% if Custom('facebook_username') is not empty %}
                                            <li class="facebook-follow">
                                                <a href="http://facebook.com/{{ Custom('facebook_username') }}" title="{{ Lang.Get('Follow us on') }} Facebook" target="_blank">Facebook</a>
                                            </li>
                                        {% endif %}
                                        {% if Custom('twitter_username') is not empty %}
                                            <li class="twitter-follow">
                                                <a href="http://twitter.com/{{ Custom('twitter_username') }}" title="{{ Lang.Get('Follow us on') }} Twitter" target="_blank">Twitter</a>
                                            </li>
                                        {% endif %}
                                        <li class="rss-follow">
                                            <a href="{{ Blog.RssUrl }}" title="{{ Lang.Get('Follow us with') }} RSS" target="_blank">{{ Lang.Get('Flux RSS') }}</a>
                                        </li>                  
                                    </ul>
                                     <div class="clear"></div>
                                </div>
                            </div>
                        {% endif %}

                        {% if Custom('custom_hide_widget_blog_archives') == false %}
                            <div class="box archives" >
                                <div class="box-titre">
                                    <h3>
                                        <span>{{ Lang.Get('Archives') }}</span>
                                    </h3>
                                </div>
                                <div class="box-content">
                                    <ul id="archivelist">
                                        {% list Blog.Archives %}
                                             <li class="arch_year">
                                                <a href="#" class="plustext" onclick="javascript:switch_arch('{{ Archive.Year }}')">{{ Archive.Year }}</a>
                                                <ul id="arch_{{ Archive.Year }}" class="arch_month" {% if loop.index == 1 %}style="display:block;"{% endif %}>
                                                    {% list Archive.Months %}
                                                        {% if Month.PostCount > 0 %}
                                                            <li><a href="{{ Month.Url }}">{{ Lang.Get(Month.Name) }}</a><span class="number">{{ Month.PostCount }}</span></li>
                                                        {% endif %}
                                                    {% endlist %}
                                                </ul>

                                            </li>
                                        {% endlist %}
                                    </ul>
                                </div>
                            </div>
                        {% endif %}

                        <p class="credits">
                            {{ Lang.Get('Hosted by') }} <a href="http://www.over-blog.com" target="_blank">Overblog</a>
                        </p>
                    </div>
                </div>
                <div class="clear"></div>
            </div>
        </div>

        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
        <script src="http://assets.over-blog-kiwi.com/themes/jquery/fancybox/jquery.fancybox-1.3.4.pack.js"></script>

        <script>
            $(document).ready(function() {
                // Fancybox
                $(".ob-section-images a, .ob-link-img").attr("rel", "fancybox");
                $("a[rel=fancybox], .avatar a").fancybox({
                    'overlayShow'   : true,
                    'transitionIn'  : 'fadin',
                    'transitionOut' : 'fadin',
                    'type'          : 'image'
                });

                {% if Custom('custom_hide_widget_search_form') == false %}
                    // Placholder Script
                    (function() {
                        // HTML5 placeholder fallback
                        function isPlaceholderSupported() {
                            'use strict';
                            var o = document.createElement('input');
                            return 'placeholder' in o;
                        }
                        function initPlaceHolder(el){
                            if(jQuery.trim(el.val()) === '' || jQuery.trim(el.val()) === el.attr('placeholder')){
                                el.val(el.attr('placeholder'));
                                el.css({'color': '#999'});
                            }
                        }
                        function togglePlaceholder(el, submitMode) {
                            'use strict';
                            var v = jQuery.trim(el.val()), p = el.attr('placeholder');
                            if (v === '' && !submitMode) {
                                el.val(p);
                                el.css({'color': '#999'});
                            } else if (v === p) {
                                el.val('');
                            }
                        }
                        function emulatePlaceholder(form) {
                            'use strict';
                            if (!isPlaceholderSupported()) {
                                jQuery('input[placeholder]', form).each(function () {
                                    initPlaceHolder(jQuery(this));
                                }).focus(function () {
                                    togglePlaceholder(jQuery(this), false);
                                }).blur(function () {
                                    initPlaceHolder(jQuery(this));
                                });
                                form.submit(function () {
                                    jQuery(this).find('input[placeholder]').each(function () {
                                        togglePlaceholder(jQuery(this), true);
                                    });
                                });
                            }
                        }
                    })();
                {% endif %}
            });

            {% if Custom('custom_hide_widget_blog_archives') == false %}
                // Archives
                function switch_arch(year) {
                    'use strict';
                    var archiveYear,
                        archiveMonths,
                        displayStyle;
                        displayStyle = '';
                        archiveYear = document.getElementById('arch_' + year);
                    if (typeof (archiveYear) !== 'undefined') {
                        if (archiveYear.style.display !== 'none') {

                            // Hide years without months
                            archiveMonths = archiveYear.getElementsByTagName('li');
                            if (!archiveMonths.length) {
                                archiveYear.parentNode.style.display = 'none';
                            }
                            // Onclick management
                            displayStyle = 'none';
                        }
                        archiveYear.style.display = displayStyle;
                    }
                    return false;
                }

                $('.plustext').click(function() {
                    return false;
                });

                {% list Blog.Archives %}
                    {% if Archive.Year != "now"|date("Y") %}
                        switch_arch('{{ Archive.Year }}');
                    {% endif %}
                {% endlist %}
            {% endif %}

            {% if Custom("comment") == "Disqus" and Custom('comment_disqus') is not empty %}
                // Disqus comments count -->
                var disqus_shortname = "{{ Custom('comment_disqus') }}";
                (function () {
                    var s = document.createElement('script');
                        s.async = true;
                        s.type = 'text/javascript';
                        s.src = 'http://' + disqus_shortname + '.disqus.com/count.js';
                    (document.getElementsByTagName('HEAD')[0] || document.getElementsByTagName('BODY')[0]).appendChild(s);
                }());
            
            {% elseif Custom('comment') == 'Facebook' %}
                // Facebook comments + share
                (function(d, s, id) {
                    var js, fjs = d.getElementsByTagName(s)[0];
                    if (d.getElementById(id)) return;
                        js = d.createElement(s);
                        js.id = id;
                        js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId={{ Custom('facebook_appid')|default('284865384904712')}}";
                        fjs.parentNode.insertBefore(js, fjs);
                }(document, 'script', 'facebook-jssdk'));
            {% endif %}
            // Twitter share + tweets
            !function(d,s,id){
                var js, fjs = d.getElementsByTagName(s)[0];
                if(!d.getElementById(id)){
                    js = d.createElement(s);
                    js.id = id;
                    js.src = "//platform.twitter.com/widgets.js";
                    fjs.parentNode.insertBefore(js,fjs);
                }
            }(document,"script","twitter-wjs");

            // Google + button
            window.___gcfg = {lang: '{{ Blog.Lang }}'};
                (function() {
                var po = document.createElement('script');
                    po.type = 'text/javascript';
                    po.async = true;
                    po.src = 'https://apis.google.com/js/plusone.js';
                var s = document.getElementsByTagName('script')[0];
                    s.parentNode.insertBefore(po, s);
            })();

             {% if Custom('google_analytics') is not empty %}
                // Google Analytics
                window.google_analytics_uacct = '{{Custom('google_analytics')}}';
                var _gaq = _gaq || [];
                    _gaq.push(['_setAccount', '{{Custom('google_analytics')}}']);
                    _gaq.push(['_trackPageview']);
                    _gaq.push(['_trackPageLoadTime']);

                (function() {
                    var ga = document.createElement('script');
                        ga.type = 'text/javascript';
                        ga.async = true;
                        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
                    var s = document.getElementsByTagName('script')[0];
                        s.parentNode.insertBefore(ga, s);
                })();
            {% endif %}
        </script>
    </body>
</html>
