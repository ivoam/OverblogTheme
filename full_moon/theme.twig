                /** MOON **/
                {% else %}

                    /* BACKGROUND */
                    body {background-image: url("{{ Custom('background')|default('http://assets.over-blog-kiwi.com/themes/5/images/moon/header-moon.jpg') }}"); background-attachment: fixed; {% if Custom('background') is not empty %} background-position: center center; -webkit-background-size: cover; -moz-background-size: cover; -o-background-size: cover; background-size: cover;{% else %}background-position: center top;{% endif %}background-repeat: no-repeat;}

                    /* TOP */
