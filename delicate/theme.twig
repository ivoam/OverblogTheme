        <![endif]-->
        
        <!-- Custom vars -->
        <meta name="constant:displayArticlesLimit" content="10" />
        
        <meta name="string:custom_menu_1_tag:Menu tag 1" group="Navigation" />
        <meta name="string:custom_menu_1_title:Menu tag 1 custom title" group="Navigation" />
        <meta name="string:custom_menu_2_tag:Menu tag 2" group="Navigation" />
        <meta name="string:custom_menu_2_title:Menu tag 2 custom title" group="Navigation" />
        
        <meta name="tag:custom_tag_pictures_pinned:Tag for pictures in slider" group="Slider" />
        <meta name="image:custom_slider_default_image:Slider default image" default="http://assets.over-blog-kiwi.com/themes/28/images/header/header.jpg" group="Slider" />
        <meta name="boolean:custom_slider_only_home:Show slider only on home page" group="Slider" />
        
        <meta name="color:custom_theme_color:Theme color" default="#0E73B8" group="Design" />
        <meta name="color:custom_bg_color:Background color" default="#EDEEEF" group="Design" />
        <meta name="list:custom_bg_pattern:Background pattern" items="none, carreaux, corde, hachures, metal, misc, motif, rayures" default="none" group="Design" />
        
        <meta name="boolean:custom_hide_widget_search_form:Hide Widget &quot;Search form&quot;" group="Widgets" />
        <meta name="boolean:custom_hide_widget_biography:Hide Widget &quot;Biography&quot;" group="Widgets" />
        <meta name="boolean:custom_hide_widget_categories:Hide Widget &quot;Categories&quot;" group="Widgets" />
        <meta name="boolean:custom_hide_widget_pages:Hide Widget &quot;Pages&quot;" group="Widgets" />
        <meta name="boolean:custom_hide_widget_archives:Hide Widget &quot;Archives&quot;" group="Widgets" />
        
        <meta name="list:comment:Comment service" items="OverBlog, Facebook, Disqus" default="OverBlog" group="Comments" />
        <meta name="string:comment_disqus:Disqus website's shortname (Mandatory)" group="Comments" />
        <meta name="list:custom_facebook_comments_color_theme:Facebook comments color theme" items="light, dark" default="light" group="Comments" />
        
        <meta name="image:custom_logo_picture:Choose your logo (max 400*100px)" group="Settings" />
        <meta name="string:custom_footer_left:Enter your footer text (left)" default="Copyright" group="Settings" />
        <meta name="string:custom_footer_right:Enter your footer text (right)" default="Powered by Overblog" group="Settings" />
        <meta name="string:google_analytics:Enter your Google Analytics ID" group="Settings" />
        <meta name="string:facebook_appid:Enter your FaceBook App ID" group="Settings" />
        <meta name="string:facebook_profile_id:Enter your FaceBook Profile ID" group="Settings" />
        <meta name="string:twitter_account:Enter your Twitter login" group="Settings" />
        <meta name="string:google_webmaster_id:Enter your Google Webmaster Tools ID" group="Settings" />
        <meta name="string:msn_webmaster_id:Enter your MSN Webmaster Tools ID" group="Settings" />
    </head>
    <body class="home blog">
        <div class="content-pad">
            <div id="header">
                <!-- shortcut:[Header content] -->
                <div id="logo">
                    <a href="{{ Blog.Url }}" title="{{ Blog.Description }}">
                        {% if Custom('custom_logo_picture') is not empty %}
                            <img src="{{ Custom('custom_logo_picture')|resize(400,100) }}" alt="{{ Blog.Title }}" title="{{ Blog.Title }}" />
                        {% else %}
                            <h1>{{ Blog.Title }}</h1>
                        {% endif %}
                    </a>
                </div>        
                <div id="top_search">
                    <!-- Search form -->
                    {% if Custom('custom_hide_widget_search_form') == false %}
                        <form method="get" id="searchforma" class="search" action="/search/"> 
                            <input type="text" class="search-input png_crop" title="search" value="{{ Search }}" id="edit-search-theme-form-keys" placeholder="{{ Lang.Get('Search') }}..." name="q" />
                            <input type="submit" title="Search" class="ob-input-submit" name="op" value="{{ Lang.Get('Search') }}"/>
                        </form>
                    {% endif %}
                <div style="clear:both;"></div>
            </div>
        </div>
        <div class="top">
            <div id="menu">
                <ul id="nav-ie" class="topnav fl fr sf-js-enabled sf-shadow">
                    <!-- Index -->
                    <li {% if isIndex %} class="current_page_item" {% endif %}><a {% if isIndex %} {% endif %} href="{{ Blog.Url }}"><span>{{ Lang.Get('Home') }}</span></a></li>
                    <!-- Custom menu 1 -->
                    {% if Custom('custom_menu_1_tag') is not empty %}
                        <li {% if isTag and Tag is not empty and Tag.Title == Custom('custom_menu_1_tag') %} class="current_page_item" {% endif %}>
                            <a href="/tag/{{ Custom('custom_menu_1_tag') }}">
                                <span>
                                    {% if Custom('custom_menu_1_title') is not empty %}
                                        {{ Custom('custom_menu_1_title') }}
                                    {% else %}
                                        {{ Custom('custom_menu_1_tag') }}
                                    {% endif %}
                                </span>
                            </a>
                        </li>
                    {% endif %}
                    <!-- Custom menu 2 -->
                    {% if Custom('custom_menu_2_tag') is not empty %}
                        <li {% if isTag and Tag is not empty and Tag.Title == Custom('custom_menu_2_tag') %} class="current_page_item" {% endif %}>
                            <a href="/tag/{{ Custom('custom_menu_2_tag') }}">
                                <span>
                                    {% if Custom('custom_menu_2_title') is not empty %}
                                        {{ Custom('custom_menu_2_title') }}
                                    {% else %}
                                        {{ Custom('custom_menu_2_tag') }}
                                    {% endif %}
                                </span>
                            </a>
                        </li>
                    {% endif %}
                    <!-- Tags -->
                    <li {% if isTag and Tag is not empty and Tag.Title != Custom('custom_menu_1_tag') and Tag.Title != Custom('custom_menu_2_tag') %} class="current_page_item" {% endif %}>
                        <a href="#"><span>{{ Lang.Get('Tags') }}</span></a>
                        <ul class='children'>
                            {% list Blog.PostsTags(10, 'SORT_POSTS_NUMBER_DESC') %}
                                {% if PostsTag.Title != Custom('custom_menu_1_tag') and PostsTag.Title != Custom('custom_menu_2_tag') %}
                                    <li {% if isTag and Tag is not empty and Tag.Title == PostsTag.Title %} class="current_page_item" {% endif %}><a href="{{ PostsTag.Url }}"><span>{{ PostsTag.Title }}</span></a></li>
                                {% endif %}
                            {% endlist %}
                        </ul>
                    </li>
                    <li><a href="/contact">{{ Lang.Get('Contact') }}</a></li>
                </ul>
            </div>
        </div>
        <div class="clear"></div>
        
        {% if isIndex or Custom('custom_slider_only_home') == false %}
            {% if Custom('custom_tag_pictures_pinned') is not empty %}
                {% list Blog.TaggedPosts(Custom('custom_tag_pictures_pinned'), 1) %}
                    {% if loop.index == 1 %}
                        <div class="head-img">
                            <div class="slideshow-bg module">
                                <div class="slideshow">
                                    {% list Blog.TaggedPosts(Custom('custom_tag_pictures_pinned'), 5) %}
                                        <div>
                                            <div class="tagline">
                                                <a title="{{ Lang.Get('More about') }} {% if TaggedPost.Title is not empty %}&quot;{{ TaggedPost.Title }}&quot;{% else %}{{ Lang.Get('this post') }}{% endif %}" href="{{ TaggedPost.PermaLink }}">
                                                    {% if TaggedPost.Title is not empty %}
                                                        {{ TaggedPost.Title }}
                                                    {% else %}
                                                        {{ Lang.Get('From') }} 
                                                        {% list TaggedPost.Sources %}
                                                            {{ Lang.Get(Source) }}
                                                            {% if not loop.last %}
                                                                 {{ Lang.Get('and') }} 
                                                            {% endif %}
                                                        {% endlist %}
                                                    {% endif %}
                                                </a>
                                            </div>
                                            {% if TaggedPost.Cover is not empty %}
                                                <img src="{{ TaggedPost.Cover|resize(970, 225)|crop }}" title="{{ TaggedPost.Title }}" alt="" />
                                            {% elseif Custom('custom_slider_default_image') is not empty %}
                                                <img src="{{ Custom('custom_slider_default_image')|resize(970, 225)|crop }}" title="{{TaggedPost.Title}}" alt="" />
                                            {% else %}
                                                <img src="http://assets.over-blog-kiwi.com/themes/28/images/header/header.jpg" title="{{TaggedPost.Title}}" alt="" />
                                            {% endif %}
                                        </div>
                                    {% endlist %}
                                </div>
                                <div id="slideshow-nav"></div>
                            </div>
                        </div>
                    {% endif %}
                {% endlist %}
            {% else %}
                <div class="head-img">
                    <div class="slideshow-bg module">
                        <div class="slideshow">
                            {% list Blog.Posts(10) %}
                                <div>
                                    <div class="tagline">
                                        <a title="{{ Lang.Get('More about') }} {% if Post.Title is not empty %}&quot;{{ Post.Title }}&quot;{% else %}{{ Lang.Get('this post') }}{% endif %}" href="{{ Post.PermaLink }}">
                                            {% if Post.Title is not empty %}
                                                {{ Post.Title }}
                                            {% else %}
                                                {{ Lang.Get('From') }} 
                                                {% list Post.Sources %}
                                                    {{ Lang.Get(Source) }}
                                                    {% if not loop.last %}
                                                         {{ Lang.Get('and') }} 
                                                    {% endif %}
                                                {% endlist %}
                                            {% endif %}
                                        </a>
                                    </div>
                                    {% if Post.Cover is not empty %}
                                        <img src="{{ Post.Cover|resize(970, 225)|crop }}" title="{{ Post.Title }}" alt="" />
                                    {% elseif Custom('custom_slider_default_image') is not empty %}
                                        <img src="{{ Custom('custom_slider_default_image')|resize(970, 225)|crop }}" title="{{Post.Title}}" alt="" />
                                    {% else %}
                                        <img src="http://assets.over-blog-kiwi.com/themes/28/images/header/header.jpg" title="{{Post.Title}}" alt="" />
                                    {% endif %}
                                </div>
                            {% endlist %}
                        </div>
                        <div id="slideshow-nav"></div>
                    </div>
                </div>
            {% endif %}
        {% endif %}
        <div id="main">     
            <div class="columns">        
                <div class="narrowcolumn">
                    
                    <!-- shortcut:[Main content] -->
                    {% if isSpecial %}
                        {{SpecialContent}}
                    {% elseif isSingle %}
                        <!-- Single Page -->
                        <div class="post">
                            <div class="title">
                                <h2>
                                    <a title="{{ Lang.Get('More about') }} {% if Post.Title is not empty %}&quot;{{ Post.Title }}&quot;{% else %}{{ Lang.Get('this post') }}{% endif %}" href="{{ Post.PermaLink }}">
                                        {% if Post.Title is not empty %}
                                            {{ Post.Title }}
                                        {% else %}
                                            {{ Lang.Get('From') }} 
                                            {% list Post.Sources %}
                                                {{ Lang.Get(Source) }}
                                                {% if not loop.last %}
                                                     {{ Lang.Get('and') }} 
                                                {% endif %}
                                            {% endlist %}
                                        {% endif %}
                                    </a>
                                </h2>
                                <small>
                                    {{ Post.Date|datel(Lang.Get('Default date format')) | title }}, {{ Post.Date|date("H:ia") }}
                                    {% if Post.Author and Post.Author is not empty %}
                                        | {{ Lang.Get('Posted by') }}
                                        <span class="author">
                                            <span class="author vcard">
                                                {{Post.Author}}
                                            </span>
                                        </span>
                                    {% endif %}
                                    {% list Post.Tags %}
                                        {% if loop.index == 1 %}
                                             {{ Lang.Get('in') }}
                                        {% endif %}
                                        <a href="{{Tag.Url}}" title="{{Tag.Title}}">{{Tag.Title}}</a>
                                        {% if not loop.last %}
                                            - 
                                        {% endif %}
                                    {% endlist %}
                                </small>
                            </div>
                            <div class="entry">
                                <p>{{ Post.Body }}</p>
                                <div class="clear"></div>
                            </div>
                            <p class="postmetadata">    
                                <span class="category">
                                    {% list Post.Tags %}
                                        <a href="{{Tag.Url}}" title="{{Tag.Title}}">{{Tag.Title}}</a>
                                    {% endlist %}
                                </span>
                            </p>
                            <div class="ob-section m-social-networks">
                                <!-- Social networks : share -->
                                <div class="fb-like" data-href="{{ Blog.Url }}{{ Post.PermaLink }}" data-send="true" data-layout="button_count" data-width="120" data-show-faces="true" data-font="lucida grande"></div>
                                <a href="https://twitter.com/share" class="twitter-share-button" data-text="{% if Post.Title is not empty %}{{ Post.Title }}{% elseif Post.Snippet is not empty %}{{ Post.Snippet}}{% else %}{{ Blog.Title }}{% endif %}" data-url="{{ Blog.Url }}{{ Post.PermaLink }}" data-lang="{{ Blog.Lang }}" {% if Custom('twitter_account') is not empty %} data-via="{{Custom('twitter_account')}}" {% endif %}></a>
                            </div>
                            {% if not isPage %}
                                <div class="bloc-comment" id="comments">
                                    <!-- Comments -->
                                    <h3>{{ Lang.Get('Comment this post') }}</h3>
                                    <div class="item-comments">
                                        {% if Custom("comment") == "Facebook" %}
                                            <div class="fb-comments" data-href="{{ Blog.Url }}{{ Post.Permalink }}" data-num-posts="2" data-width="640" data-colorscheme="{{ Custom("custom_facebook_comments_color_theme")|default('light') }}"></div>
                                        {% elseif Custom("comment") == "Disqus" and Custom('comment_disqus') is not empty %}
                                            <div id="disqus_thread"></div>
                                            <script type="text/javascript">
                                                var disqus_shortname = '{{ Custom('comment_disqus') }}';
                                                (function() {
                                                    var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
                                                    dsq.src = 'http://' + disqus_shortname + '.disqus.com/embed.js';
                                                    (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
                                                })();
                                            </script>
                                            <noscript>Please enable JavaScript to view the <a href="http://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
                                        {% else %}
                                            {{ Post.Comments }}
                                        {% endif %}
                                    </div>
                                </div>
                            {% endif %}
                        </div>
                        <div class="return">
                            <a href="{{ Blog.Url }}">{{ Lang.Get('Return to home') }}</a>
                        </div>
                    {% else %}
                        <!-- Other Pages -->
                        {% list Posts %}
                            <div class="post">
                                <div class="title">
                                    <!-- Comments count -->
                                    {% if isIndex %}
                                        {% if Custom("comment") == "Disqus" and Custom('comment_disqus') is not empty %}
                                            <a title="{{ Lang.Get('comments') }}" class="comments" href="{{ Blog.Url }}{{ Post.PermaLink }}#disqus_thread"></a>
                                        {% else %}
                                            <a title="{{ Lang.Get('comments') }}" class="comments" href="{{ Post.PermaLink }}#comments">
                                                {% if Custom("comment") == "Facebook" %}
                                                    <fb:comments-count href={{ Blog.Url }}{{ Post.Permalink }}></fb:comments-count> {{ Lang.Get('comments') }}
                                                {% else %}
                                                    {{ Post.CommentCount(Lang.Get('comment'), Lang.Get('comments')) }}
                                                {% endif %}
                                            </a>
                                        {% endif %}
                                    {% endif %}
                                    <h2>
                                        <a title="{{ Lang.Get('More about') }} {% if Post.Title is not empty %}&quot;{{ Post.Title }}&quot;{% else %}{{ Lang.Get('this post') }}{% endif %}" href="{{ Post.PermaLink }}">
                                            {% if Post.Title is not empty %}
                                                {{ Post.Title }}
                                            {% else %}
                                                {{ Lang.Get('From') }} 
                                                {% list Post.Sources %}
                                                    {{ Lang.Get(Source) }}
                                                    {% if not loop.last %}
                                                         {{ Lang.Get('and') }} 
                                                    {% endif %}
                                                {% endlist %}
                                            {% endif %}
                                        </a>
                                    </h2>
                                    <small>
                                        {{ Post.Date|datel(Lang.Get('Default date format')) | title }}, {{ Post.Date|date("H:ia") }}
                                        {% if Post.Author and Post.Author is not empty %}
                                            | {{ Lang.Get('Posted by') }}
                                            <span class="author">
                                                <span class="author vcard">
                                                    {{Post.Author}}
                                                </span>
                                            </span>
                                        {% endif %}
                                        
                                        {% list Post.Tags %}
                                            {% if loop.length > 0 and loop.index == 1 %}
                                             in 
                                            {% endif %}
                                            <a href="{{Tag.Url}}" title="{{Tag.Title}}">{{Tag.Title}}</a>
                                            {% if not loop.last %}
                                                - 
                                            {% endif %}
                                        {% endlist %}
                                    </small>
                                </div>
                                <div class="entry">
                                    {% if Post.Cover is not empty %}
                                        <a title="{{ Lang.Get('More about') }} {% if Post.Title is not empty %}&quot;{{ Post.Title }}&quot;{% else %}{{ Lang.Get('this post') }}{% endif %}" href="{{ Post.PermaLink }}">
                                            {% if isIndex %}
                                                <img src="{{ Post.Cover|resize(400, 300) }}" alt="" />
                                            {% else %}
                                                <img src="{{ Post.Cover|resize(200, 150) }}" alt="" />
                                            {% endif %}
                                        </a>
                                    {% endif %}
                                    <p>{{ Post.Snippet }}</p>
                                    <div class="clear"></div>
                                </div>
                                <p class="postmetadata">    
                                    <span class="category">
                                        {% list Post.Tags %}
                                            <a href="{{Tag.Url}}" title="{{Tag.Title}}">{{Tag.Title}}</a>
                                        {% endlist %}
                                    </span>
                                </p>
                            </div>
                        {% endlist %}
                        {% if isSearch and Posts is empty %}
                            <!-- No result for search -->
                            <p>{{ Lang.Get('No result found') }}</p>
                        {% endif %}
                    {% endif %}
                    
                    <!-- Pagination -->
                    {% if Navigation.PreviousPage is not empty or Navigation.NextPage is not empty or Navigation.PreviousPost is not empty or Navigation.NextPost is not empty %}
                        <div id="navigation">
                            {% if Navigation.PreviousPage is not empty or Navigation.PreviousPost is not empty %}
                                <p class="alignleft">
                                    {% if Navigation.PreviousPage is not empty %}
                                        <a href="{{ Navigation.PreviousPage }}" class="prev">{{ Lang.Get('Previous page') }}</a>
                                    {% elseif Navigation.PreviousPost is not empty %}
                                        <a href="{{ Navigation.PreviousPost.Permalink }}" class="prev" title="{{ Lang.Get('Go to') }} {% if Navigation.PreviousPost.Title is not empty %}&quot;{{ Navigation.PreviousPost.Title|truncate(30) }}&quot;{% else %}{{ Lang.Get('previous post') }}{% endif %}">{{ Lang.Get('Previous post') }}</a>
                                    {% endif %}
                                </p>
                            {% endif %}
                            {% if Navigation.NextPage is not empty or Navigation.NextPost is not empty %}
                                <p class="alignright">
                                    {% if Navigation.NextPage is not empty %}
                                        <a href="{{ Navigation.NextPage }}" class="next">{{ Lang.Get('Next page') }}</a>
                                    {% elseif Navigation.NextPost is not empty %}
                                        <a href="{{ Navigation.NextPost.Permalink }}" class="next" title="{{ Lang.Get('Go to') }} {% if Navigation.NextPost.Title is not empty %}&quot;{{ Navigation.NextPost.Title|truncate(30) }}&quot;{% else %}{{ Lang.Get('next post') }}{% endif %}">{{ Lang.Get('Next post') }}</a>
                                    {% endif %}
                                </p>
                            {% endif %}
                        </div>
                    {% endif %}
                    
                </div>
                
                <!-- shortcut:[Sidebar content] -->
                <div id="sidebar" class="profile">
                    <ul>
                        <!-- Categories -->
                        {% if Custom('custom_hide_widget_categories') == false %}
                            {% if Blog.PostsTags() and Blog.PostsTags() is not empty %}
                                <li class="widget png_scale">
                                    <h3 class="blocktitle">{{ Lang.Get('Categories') }}</h3>
                                    <ul>
                                        {% list Blog.PostsTags(10) %}
                                            <li><a href="{{ PostsTag.Url }}">{{ PostsTag.Title }} ({{ PostsTag.PostCount }})</a></li>
                                        {% endlist %}
                                    </ul>
                                </li>
                            {% endif %}
                        {% endif %}
                        
                        <!-- Pages -->
                        {% if Custom('custom_hide_widget_pages') == false %}
                            {% if Blog.ListPages and Blog.ListPages is not empty %}
                                <li class="widget png_scale">
                                    <h3 class="blocktitle">{{ Lang.Get('Pages') }}</h3>
                                    <ul>
                                        {% list Blog.ListPages %}
                                            <li>
                                                <a href="{{ListPage.Url}}">{{ListPage.Title}}</a>
                                            </li>
                                        {% endlist %} 
                                    </ul>
                                </li>
                            {% endif %}
                        {% endif %}
                        
                        <!-- Blog owner -->
                        {% if Custom('custom_hide_widget_biography') == false %}
                            <li class="widget png_scale">
                                {% if Blog.Owner.Nickname is not empty and Blog.Owner.Bio is not empty %}
                                    <h3 class="blocktitle">{{ Blog.Owner.Nickname }}</h3>
                                    {% if Blog.Owner.Bio is not empty %}
                                        <p>
                                            {% if Blog.Owner.Avatar is not empty %}
                                                <a title="{{ Blog.Owner.Nickname }}" class="fancybox" href="{{ Blog.Owner.Avatar }}"><img class="ob-media ob-img ob-pull-left ob-media-left" src="{{ Blog.Owner.Avatar|resize(45,45) }}" alt="" /></a>
                                            {% endif %}
                                            {{Blog.Owner.Bio}}
                                            <br/><br/>
                                            {% if Custom('facebook_profile_id') is not empty %}
                                                <a title="Facebook" class="m-network" target="_blank" href="http://www.facebook.com/profile.php?id={{ Custom('facebook_profile_id') }}"><img alt="Facebook" src="http://assets.over-blog-kiwi.com/themes/28/images/facebook.png"></a>
                                            {% endif %}
                                            {% if Custom('twitter_account') is not empty %}
                                                <a title="Twitter" class="m-network" target="_blank" href="https://www.twitter.com/{{ Custom('twitter_account') }}"><img alt="Twitter" src="http://assets.over-blog-kiwi.com/themes/28/images/twitter.png"></a>
                                            {% endif %}
                                            <a title="RSS" class="m-network" target="_blank" href="{{ Blog.RssUrl }}"><img alt="Twitter" src="http://assets.over-blog-kiwi.com/themes/28/images/rss.png"></a>
                                        </p>
                                    {% endif %}
                                {% endif %}
                            </li>
                        {% endif %}
                        
                        <!-- Archives -->
                        {% if Custom('custom_hide_widget_archives') == false %}
                            {% if Blog.Archives is not empty %}
                                <li class="widget png_scale">
                                    <h3 class="blocktitle">{{ Lang.Get('Archives') }}</h3>
                                    <ul class="list" id="archivelist">
                                        {% list Blog.Archives %}
                                            <li>
                                                <a href="#" class"plustext" onclick="javascript: return switch_arch('{{Archive.Year}}')">{{Archive.Year}}</a>
