            .sidebar .widget.archives .plustext {font-size: 20px;}
            .sidebar .widget.archives .arch_month {margin: 10px 0 0 15px;}
            .sidebar .widget.archives .arch_month li {margin-bottom: 20px;}
            .sidebar .widget.archives .arch_month .number {background: {% if Custom('theme_color') == 'Dark'|default('Dark') %}#121212{% else %}#D5D5D5{% endif %}; border-bottom: 1px solid {% if Custom('theme_color') == 'Dark'|default('Dark') %}#2D2D2D{% else %}#FFF{% endif %}; color: #5E5E5E; float: right; line-height: 23px; margin-right: 25px; min-width: 20px; padding: 0 3px; text-align: center;}

            .footer a {color: #FFF;}
