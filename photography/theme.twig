            .ob-section .twt-border {max-width: 100% !important;}


            a, a:hover,
            .content.single a:hover,
            .sub-nav li.active a,
            .sub-nav li a:hover,
            h1, h1 a,
            h2, h2 a,
            h3, h3 a,
            h4, h4 a,
            h5, h5 a,
            .ob-text h3,
            .ob-text h3 a,
            .ob-text h4,
            .ob-text h4 a,
            .ob-text h5,
            .ob-text h5 a,
            .search-title,
            .social h6 {color: {% if Custom('theme_color') == 'Dark'|default('Dark') %}#FFF{% else %}#333{% endif %}; font-family: "{{ Custom('custom_titles_font')|default('Arvo') }}", Arial, Helvetica, sans-serif;}

            .sub-nav li.active a {border-bottom: solid 1px {% if Custom('theme_color') == 'Dark'|default('Dark') %}#777{% else %}#333{% endif %};}
            .logo a,
            .logo a:visited,
            .ob-section-quote,
            .ob-section-link .ob-snippet,
            .content .meta a,
            body {color: {% if Custom('theme_color') == 'Dark'|default('Dark') %}#777{% else %}#333{% endif %};}

            .sidebar {float: right; width: 290px;}
            .sidebar .widget {margin-bottom: 20px; width: 100%;}
            .sidebar .widget h3 {display: block; font-size: 1.1em; font-weight: 400; margin-bottom: 15px; text-transform: uppercase;}
            .sidebar ul {margin-bottom: 10px; width: 100% !important;}
            .sidebar .widget.lasts li {background: {% if Custom('theme_color') == 'Dark'|default('Dark') %}#121212{% else %}#D5D5D5{% endif %}; border-bottom: 1px solid {% if Custom('theme_color') == 'Dark'|default('Dark') %}#2D2D2D{% else %}#FFF{% endif %}; float: left; margin: 0 5px 5px 0; padding: 5px;}
            .sidebar .widget.lasts li h3 {margin: 0 !important;}
            .sidebar .widget.lasts li.left {margin-right: 10px;}
            .sidebar .widget.lasts li img {-webkit-box-shadow: 0 1px 3px #000; -moz-box-shadow: 0 1px 3px #000;  -ms-box-shadow: 0 1px 3px #000;  -o-box-shadow: 0 1px 3px #000;  box-shadow: 0 1px 3px #000;  display: block; }
            .sidebar .widget.lasts:hover li img {-webkit-opacity: 0.5; -moz-opacity: 0.5;  -ms-opacity: 0.5;  -o-opacity: 0.5;  opacity: 0.5; -webkit-transition: opacity 0.4s; -moz-transition: opacity 0.4s; -ms-transition: opacity 0.4s; -o-transition: opacity 0.4s; transition: opacity 0.4s; }
            .sidebar .widget.lasts:hover li:hover img {-webkit-opacity: 1; -moz-opacity: 1;  -ms-opacity: 1;  -o-opacity: 1;  opacity: 1; -webkit-transition: opacity 0.4s; -moz-transition: opacity 0.4s; -ms-transition: opacity 0.4s; -o-transition: opacity 0.4s; transition: opacity 0.4s; }

            .sidebar .widget.archives .archivelist {margin-bottom: 30px;}
