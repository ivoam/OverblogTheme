    </head>
    <body>
        <div id="fb-root"></div>
        <div class="header">
            <div class="wrap">
                <div class="logo">
                    <h1>
                        <a href="{{ Blog.Url }}" title="{{ Blog.Description }}">{{ Blog.Title }}</a>
                    </h1>
                    <p>{{ Blog.Description }}</p>
                </div>
                <div class="nav">
                    <ul>
                        <li>
                            <a {% if isIndex %}class="active"{% endif %} href="{{ Blog.Url }}/">{{ Lang.Get('Home') }}</a>
                                {% if isIndex %}<span class="bg-nav-end"></span>{% endif %}
                            </li>
                        <!-- Pages -->
                        {% if Custom('custom_hide_widget_pages') == false %}
                            {% list Blog.ListPages %}
                                {% if loop.first %}
                                    <li>
                                        <a href="#">{{ Lang.Get('Pages') }}</a>
                                        <ul class="children">
                                {% endif %}
                                            <li>
                                                <a href="{{ListPage.Url}}">{{ ListPage.Title }}</a>
                                            </li>
                                {% if loop.last %}
                                        </ul>
                                    </li>
                                {% endif %}
                            {% endlist %}
                        {% endif %}
                        <li>
                            <a href="{{ Blog.Url }}/contact">{{ Lang.Get('Contact') }}</a>
                        </li>
                        <li class="last">
                            <a href="{{ Blog.Url }}/mail/form">{{ Lang.Get('Subscribe') }}</a>
                        </li>
                    </ul>
                </div>
                <div class="cb"></div>
            </div>
        </div>
        <div class="sub-nav">
            <div class="wrap">
                <div class="categories">
                    <ul>
                        <li {% if isIndex %}class="active"{% endif %}>
                            <a href="{{ Blog.Url }}">{{ Lang.Get('All') }}</a>
                        </li>
                        {% if Custom('tag_number') > 0 %}
                            {% list Blog.PostsTags(Custom('tag_number')) %}
                                <li {% if isTag and Tag.Title == PostsTag.Title %}class="active"{% endif %}>
                                    <a href="{{ PostsTag.Url }}">{{ PostsTag.Title }}</a>
                                </li>
                            {% endlist %}
                        {% endif %}
                    </ul>
                </div>
                {% if Custom('custom_hide_widget_search_form') == false %}
                    <div class="search">
                        <form class="search" method="post" action="/search">
                            <input type="text" name="q" placeholder="{{ Lang.Get('Search') }}..." value="{{ Search }}" />
                            <input type="submit" value="{{ Lang.Get('Search') }}" />
                        </form>
                    </div>
                {% endif %}
            </div>
        </div>

        {% if isSingle or isSpecial %}
            <!-- Single Page -->
            <div class="content single">
                <div class="wrap">
                    <div class="single-post">
                        {% if isSingle %}
                            {% list Posts %}
                                <h2>
                                    <a title="{% if Post.Title is not empty %}&quot;{{ Post.Title }}&quot;{% else %}{{ Lang.Get('this post') }}{% endif %}" href="{{ Post.PermaLink }}">
                                        {% if Post.Title is not empty %}
                                            {{ Post.Title }}
                                        {% else %}
                                            {{ Lang.Get('A post from') }} {% list Post.Sources %}{{ Lang.Get(Source) }} {% if not loop.last %}{{ Lang.Get('and') }}{% endif %} {% endlist %}
                                        {% endif %}
                                    </a>
                                </h2>
                                <p class="meta">
                                    {% if Post.Author and Post.Author is not empty %}
                                        <span class="author">{{ Lang.Get('By') }} {{ Post.Author }}</span>&nbsp;-&nbsp;
                                    {% endif %}
                                    <span class="date">{{ Post.Date|datel(Lang.Get("Default date format"))|title }}, {{ Post.Date|date("H:i") }}</span>
                                    {% list Post.Tags %}
                                        {% if loop.first %}
                                            &nbsp;-&nbsp;<span class="post-tags">{{ Lang.Get('Tags:') }}</span>
                                        {% endif %}
                                        <a href="{{Tag.Url}}" title="{{Tag.Title}}">#{{ Tag.Title }}</a>{% if not loop.last %}, {% endif %}
                                    {% endlist %}
                                </p>
                                <div class="content">
                                    {{ Post.Body }}
                                </div>

                                <div class="social">
                                    <h6>{% if isPage %}{{ Lang.Get('Share this page') }}{% else %}{{ Lang.Get('Share this post') }}{% endif %}</h6>
                                    {% if Custom('share_service') != 'None' %}
                                        {% if Custom('share_service') == 'Google +' %}
                                            <div class="google">
                                                <div class="g-plusone" data-size="medium" data-href="{{ Post.Permalink }}"></div>
                                            </div>                                        
                                        {% elseif Custom('share_service') == 'Twitter' %}
                                            <div class="twitter">
                                                <a href="https://twitter.com/share" class="twitter-share-button"  data-text="{% if Post.Title is not empty %}{{ Post.Title }}{% elseif Post.Snippet is not empty %}{{ Post.Snippet}}{% else %}{{ Blog.Title }}{% endif %}" data-url="{{ Post.Permalink }}" data-lang="{{ Blog.Lang }}"{% if Custom('twitter_username') is not empty %} data-via="{{Custom('twitter_username')}}"{% endif %}></a>
                                            </div>
                                        {% elseif Custom('share_service') == 'Facebook' %}
                                            <div class="facebook">
                                                <div class="fb-like" data-href="{{ Post.Permalink }}" data-send="false" data-layout="button_count" data-width="90" data-show-faces="false"></div>
                                            </div>
                                        {% elseif Custom('share_service') == 'Pinterest' %}
                                            <div class="pinterest">
                                                <a href="http://pinterest.com/pin/create/button/?url={{ Post.Permalink }}&media={{ Post.Cover }}&description={{ Post.Snippet }}" class="pin-it-button" count-layout="horizontal" always-show-count="true">
                                                    <img src="//assets.pinterest.com/images/PinExt.png" title="Pin It" />
                                                </a>
                                            </div>
                                        {% elseif Custom('share_service') == 'All' or Custom('share_service') is empty %}
                                            <div class="google">
                                                <div class="g-plusone" data-size="medium" data-href="{{ Post.Permalink }}"></div>
                                            </div>
                                            <div class="twitter">
                                                <a href="https://twitter.com/share" class="twitter-share-button"  data-text="{% if Post.Title is not empty %}{{ Post.Title }}{% elseif Post.Snippet is not empty %}{{ Post.Snippet}}{% else %}{{ Blog.Title }}{% endif %}" data-url="{{ Post.Permalink }}" data-lang="{{ Blog.Lang }}"{% if Custom('twitter_username') is not empty %} data-via="{{Custom('twitter_username')}}"{% endif %}></a>
                                            </div>
                                            <div class="facebook">
                                                <div class="fb-like" data-href="{{ Post.Permalink }}" data-send="false" data-layout="button_count" data-width="90" data-show-faces="false"></div>
                                            </div>
                                            <div class="pinterest">
                                                <a href="http://pinterest.com/pin/create/button/?url={{ Post.Permalink }}&media={{ Post.Cover }}&description={{ Post.Snippet }}" class="pin-it-button" count-layout="horizontal" always-show-count="true">
                                                    <img src="//assets.pinterest.com/images/PinExt.png" title="Pin It" />
                                                </a>
                                            </div>
                                        {% endif %}
                                    {% endif %}
                                    <div class="ob-share">
                                        {{ Post.RepostButton }}
                                    </div>
                                </div>

                                <!-- Pagination -->
                                {% if  Navigation.PreviousPost is not empty or Navigation.NextPost is not empty%}
                                    <div class="navigation">
                                        {% if Navigation.PreviousPost is not empty %}
                                            <a href="{{ Navigation.PreviousPost.Permalink }}" class="btn prev" title="{{ Lang.Get('Go to') }} {% if Navigation.PreviousPost.Title is not empty %}&quot;{{ Navigation.PreviousPost.Title|truncate(30) }}&quot;{% else %}{{ Lang.Get('previous post') }}{% endif %}">{{ Lang.Get('Previous post') }}</a>
                                        {% endif %}

                                        {% if Navigation.NextPost is not empty %}
                                            <a href="{{ Navigation.NextPost.Permalink }}" class="btn next" title="{{ Lang.Get('Go to') }} {% if Navigation.NextPost.Title is not empty %}&quot;{{ Navigation.NextPost.Title|truncate(30) }}&quot;{% else %}{{ Lang.Get('next post') }}{% endif %}">{{ Lang.Get('Next post') }}</a>
                                        {% endif %}
                                        <div class="cb"></div>
                                    </div>
                                {% endif %}

                                {% if not isPage and Post.ShowComments is not empty %}
                                    <div class="bloc-comment ob-section" id="comments">
                                        <!-- Comments -->
                                        <div class="item-comments">
                                            {% if Custom("comment") == "Facebook" %}
                                                <div class="fb-comments" data-href="{{ Post.Permalink }}" data-num-posts="2" data-width="600" data-colorscheme="{% if Custom('backgroun_color') == 'Dark'|default('Dark') %}dark{% else %}light{% endif %}"></div>
                                            {% elseif Custom("comment") == "Disqus" and Custom('comment_disqus') is not empty %}
                                                <div id="disqus_thread"></div>
                                                <script type="text/javascript">
                                                    var disqus_shortname = '{{ Custom('comment_disqus') }}';
                                                    (function() {
                                                        var dsq = document.createElement('script');
                                                            dsq.type = 'text/javascript';
                                                            dsq.async = true;
                                                            dsq.src = 'http://' + disqus_shortname + '.disqus.com/embed.js';
                                                        (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
                                                    })();
                                                </script>
                                                <noscript>Please enable JavaScript to view the <a href="http://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
                                            {% else %}
                                                {{ Post.Comments }}
                                            {% endif %}
                                        </div>
                                    </div>
                                {% endif %}
                            {% endlist %}
                        {% else %}
                            <div class="content contact">
                                {{ SpecialContent }}
                            </div>
                        {% endif %}
                    </div>

                    <aside class="sidebar">
                        {% if Custom('custom_hide_widget_last') == false %}
                            <div class="widget lasts">
                                <h3>{{ Lang.Get('Last Posts') }}</h3>
                                <ul>
                                    {% list Blog.Posts(6) %}
                                        <li>
                                            <h3>
                                                <a href="{{ Post.Permalink }}" title="{{ Post.Snippet }}">
                                                    <img src="{% if Post.Cover is not empty %}{{ Post.Cover|resize(130,100)|crop }}{% else %}{{ Custom('post_cover')|default('http://assets.over-blog-staging.com/themes/2/images/default-cover.jpg')|resize(130,100)|crop }}{% endif %}" alt="" width="130" height="100" />
                                                </a>
                                            </h3>
                                        </li>
                                        {% if loop.index == 2 or loop.index == 4 %}
                                            <div class="cb"></div>
                                        {% endif %}
                                    {% endlist %}
                                    <div class="cb"></div>
                                </ul>
                                <div class="cb"></div>
                            </div>
                        {% endif %}

                        {% if Custom('custom_hide_widget_blog_archive') == false %}
                             <!-- Follow us -->
                            <div class="widget archives" >
                                <h3>{{ Lang.Get('Blog archives') }}</h3>
                                <ul id="archivelist">
                                    {% list Blog.Archives %}
                                         <li class="arch_year">
                                            <a href="#" class="plustext" onclick="javascript:switch_arch('{{ Archive.Year }}')">{{ Archive.Year }}</a>
                                            <ul id="arch_{{ Archive.Year }}" class="arch_month" {% if loop.index == 1 %}style="display:block;"{% endif %}>
                                                {% list Archive.Months %}
                                                    {% if Month.PostCount > 0 %}
                                                        <li><a href="{{ Month.Url }}">{{ Lang.Get(Month.Name) }}</a><span class="number">{{ Month.PostCount }}</span></li>
                                                    {% endif %}
                                                {% endlist %}
                                            </ul>

                                        </li>
                                    {% endlist %}
                                </ul>
                            </div>
                        {% endif %}
                    </aside>
                    <div class="cb"></div>
                </div>
            </div>
        {% else %}
            <!-- Other Pages -->
            <div class="content list-content">
                <div class="wrap">
                    {% if isSearch %}
                        <p class="search-title">{{ Lang.Get('Search results for') }} “{{ Search.Keyword }}”</p>
                        {% if Posts is empty %}
                            <p class="search-empty">{{ Lang.Get('No posts found for this expression.') }}</p>
                        {% endif %}
                    {% elseif isTag %}
                        {% if Posts is empty %}
                            <p class="tag-empty">{{ Lang.Get('No posts found for this tag.') }}</p>
                        {% endif %}
                    {% else %}
                        {% if Posts is empty %}
                            <p >{{ Lang.Get('Sorry we haven\'t found any post.') }}</p>
                        {% endif %}
                    {% endif %}

                    <ul id="thumbs" class="thumbs">
                        {% list Posts %}
                            <li>
                                <h2>
                                    <a href="{{ Post.PermaLink }}">
                                        {% if Post.Cover is not empty %}
                                            <img alt="{{ Post.Title }}" src="{{ Post.Cover|resize(214, 163)|crop }}" />
                                        {% else %}
                                            <img alt="{{ Post.Title }}" src="{{ Custom('post_cover')|default('http://assets.over-blog-staging.com/themes/2/images/default-cover.jpg')|resize(214, 163)|crop }}" />
                                        {% endif %}
                                        <div>
                                            <span>
                                                {% if Post.Title is not empty %}
                                                    {{ Post.Title }}
                                                {% else %}
                                                    {{ Lang.Get('A post from') }}
                                                    {% list Post.Sources %}
                                                        {{ Lang.Get(Source) }}{% if not loop.last %} {{ Lang.Get('and') }} {% endif %}
                                                    {% endlist %}
                                                {% endif %}
                                            </span>
                                        </div>
                                    </a>
                                </h2>
                            </li>
                            {% if loop.index % 4 == 0 %}
                                <div class="cb"></div>
                            {% endif %}
                        {% endlist %}
                    </ul>
                    <div class="cb"></div>
                </div>
            </div>
        {% endif %}

        <!-- Pagination -->
        {% if Navigation.PreviousPage is not empty or Navigation.NextPage is not empty %}
            <div class="navigation">
                <div class="wrap">
                    {% if Navigation.PreviousPage is not empty %}
                        <a href="{{ Navigation.PreviousPage }}" class="btn prev">{{ Lang.Get('Previous page') }}</a>
                    {% endif %}

                    {% if Navigation.NextPage is not empty %}
                        <a href="{{ Navigation.NextPage }}" class="btn next">{{ Lang.Get('Next page') }}</a>
                    {% endif %}
                    <div class="cb"></div>
                </div>
            </div>
        {% endif %}

        <!-- End Posts -->
        <div class="top-footer"></div>
        <div class="footer">
            <div class="wrap">
                {% if Custom('custom_hide_widget_biography') == false %}
                    {% if Blog.Owner.Bio is not empty %}
                        <div class="about">
                            <h3>{{ Lang.Get('About me') }}</h3>
                            <div class="cb"></div>
                             {% if Blog.Owner.Avatar is not empty %}
                                <div class="avatar">
                                    <a class="blog-avatar" href="{{ Blog.Owner.Avatar }}" title="{{ Blog.Owner.Nickname }}">
                                        <img alt="{{ Blog.Owner.Nickname }}" src="{{ Blog.Owner.Avatar|resize(50,54)|crop }}" />
                                    </a>
                                </div>
                            {% endif %}
                            <p><strong>{{ Blog.Owner.Nickname }}</strong></p>
                            <p>{{ Blog.Owner.Bio }}</p>
                        </div>
                    {% endif %}
                {% endif %}

                {% if Custom('twitter_username') is not empty or Custom('facebook_username') is not empty or Custom('flickr_username') is not empty %}
                    <div class="social">
                        <h3>{{ Lang.Get('Follow me') }}</h3>
                        <div class="cb"></div>
                        <ul>
                            {% if Custom('twitter_username') is not empty %}
                                <li><a class="twitter" href="http://twitter.com/{{ Custom('twitter_username') }}" target="_blank">{{ Lang.Get('Follow me') }}</a></li>
                            {% endif %}
                            {% if Custom('facebook_username') is not empty %}
                                <li><a class="facebook" href="http://facebook.com/{{ Custom('facebook_username') }}" target="_blank">{{ Lang.Get('Like my page') }}</a></li>
                            {% endif %}
                            {% if Custom('flickr_username') is not empty %}
                                <li><a class="flickr" href="http://www.flickr.com/photos/{{ Custom('flickr_username') }}" target="_blank">{{ Lang.Get('See my work') }}</a></li>
                            {% endif %}
                        </ul>
                    </div>
                {% endif %}

                {% if Custom('flickr_user_id') is not empty %}
                    <div class="myflickr">
                        <h3>{{ Lang.Get('Flickr') }}</h3>
                        <div class="cb"></div>
                        <div id="flickr"></div>
                    </div>
                {% endif %}
                <div class="cb"></div>
            </div>
        </div>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
        <script src="http://assets.over-blog-kiwi.com/themes/jquery/fancybox/jquery.fancybox-1.3.4.pack.js"></script>
        {% if not isSingle or not isSpecial %}
            <script src="http://assets.over-blog-kiwi.com/themes/2/js/jquery.hoverdir.js"></script>
        {% endif %}
        {% if Custom('flickr_username') is not empty %}
            <script src="http://assets.over-blog-kiwi.com/themes/2/js/flickr/jquery.zflickrfeed.js"></script>
        {% endif %}
        {% if Custom('share_service') == 'All' or Custom('share_service') is empty or Custom('share_service') == 'Pinterest' %}
            <script type="text/javascript" src="//assets.pinterest.com/js/pinit.js"></script>
        {% endif %}

        <script>
            {% if isSingle %}
                // Social networks : Facebook
                (function(d, s, id) {
                    var js, fjs = d.getElementsByTagName(s)[0];
                    if (d.getElementById(id)) return;
                        js = d.createElement(s);
                        js.id = id;
                        js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId={{ Custom('facebook_appid')|default('284865384904712')}}";
                        fjs.parentNode.insertBefore(js, fjs);
                }(document, 'script', 'facebook-jssdk'));


                {% if Custom('share_service') == 'All' or Custom('share_service') is empty or Custom('share_service') == 'Google +' %}
                    // Google + button
                    window.___gcfg = {lang: '{{ Blog.Lang }}'};
                        (function() {
                        var po = document.createElement('script');
                            po.type = 'text/javascript';
                            po.async = true;
                            po.src = 'https://apis.google.com/js/plusone.js';
                        var s = document.getElementsByTagName('script')[0];
                            s.parentNode.insertBefore(po, s);
                    })();
                {% endif %}

                // Social networks : Twitter
                !function(d, s, id){
                    var js,
                        fjs = d.getElementsByTagName(s)[0];
                    if(!d.getElementById(id)){
                        js=d.createElement(s);
                        js.id=id;
                        js.src="//platform.twitter.com/widgets.js";
                        fjs.parentNode.insertBefore(js,fjs);
                    }
                }(document,"script","twitter-wjs");
            {% endif %}

            {% if isSingle or isSpecial and Custom('custom_hide_widget_blog_archives') == false %}
                // Archives
                function switch_arch(year) {
                    'use strict';
                    var archiveYear,
                        archiveMonths,
                        displayStyle;
                        displayStyle = '';
                        archiveYear = document.getElementById('arch_' + year);
                    if (typeof (archiveYear) !== 'undefined') {
                        if (archiveYear.style.display !== 'none') {

                            // Hide years without months
                                archiveMonths = archiveYear.getElementsByTagName('li');
                            if (!archiveMonths.length) {
                                archiveYear.parentNode.style.display = 'none';
                            }
                            // Onclick management
                            displayStyle = 'none';
                        }
                        archiveYear.style.display = displayStyle;
                    }
                    return false;
                }

                {% list Blog.Archives %}
                    {% if Archive.Year != "now"|date("Y") %}
                        switch_arch('{{ Archive.Year }}');
                    {% endif %}
                {% endlist %}
            {% endif %}

            {% if Custom('google_analytics') is not empty %}
                <!-- Google Analytics -->
                window.google_analytics_uacct = '{{ Custom('google_analytics') }}';
                var _gaq = _gaq || [];
                    _gaq.push(['_setAccount', '{{ Custom('google_analytics') }}']);
                    _gaq.push(['_trackPageview']);
                    _gaq.push(['_trackPageLoadTime']);

                (function() {
                    var ga = document.createElement('script');
                        ga.type = 'text/javascript';
                        ga.async = true;
                        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
                    var s = document.getElementsByTagName('script')[0];
                        s.parentNode.insertBefore(ga, s);
                })();
            {% endif %}

            $(document).ready(function() {
                {% if isSingle or isSpecial %}
                     $('.plustext').click(function() {
                        return false;
                    });
                {% endif %}
                {% if not isSingle %}
                    // Thumbs hover
                    $(function() {
                        $('#thumbs > li').hoverdir();
                    });
                {% endif %}

                {% if Custom('flickr_user_id') is not empty %}
                    $('#flickr').flickrfeed('{{ Custom('flickr_user_id') }}','', {
                        limit: 6
                    });
                {% endif %}

                // Fancybox
                $(".ob-section-images a, .ob-link-img").attr("rel", "fancybox");
                $("a[rel=fancybox], .blog-avatar").fancybox({
                    'overlayColor'  : '#333',
                    'overlayShow'   : true,
                    'titlePosition' : 'over',
                    'transitionIn'  : 'fadin',
                    'transitionOut' : 'fadin',
                    'type'          : 'image'
                });

                // Template script onload
                (function() {
                    {% if Custom('custom_hide_widget_search_form') == false %}
                        // HTML5 placeholder fallback
                        function isPlaceholderSupported() {
                            'use strict';
                            var o = document.createElement('input');
                            return 'placeholder' in o;
                        }
                        function initPlaceHolder(el){
                            if(jQuery.trim(el.val()) === '' || jQuery.trim(el.val()) === el.attr('placeholder')){
                                el.val(el.attr('placeholder'));
                                el.css({'color': '#999'});
                            }
                        }
                        function togglePlaceholder(el, submitMode) {
                            'use strict';
                            var v = jQuery.trim(el.val()), p = el.attr('placeholder');
                            if (v === '' && !submitMode) {
                                el.val(p);
                                el.css({'color': '#999'});
                            } else if (v === p) {
                                el.val('');
                            }
                        }
                        function emulatePlaceholder(form) {
                            'use strict';
                            if (!isPlaceholderSupported()) {
                                jQuery('input[placeholder]', form).each(function () {
                                    initPlaceHolder(jQuery(this));
                                }).focus(function () {
                                    togglePlaceholder(jQuery(this), false);
                                }).blur(function () {
                                    initPlaceHolder(jQuery(this));
                                });
                                form.submit(function () {
                                    jQuery(this).find('input[placeholder]').each(function () {
                                        togglePlaceholder(jQuery(this), true);
                                    });
                                });
                            }
                        }
                    {% endif %}
                })();
            });
        </script>
    </body>
</html>
