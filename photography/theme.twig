            .footer .myflickr ul:hover > li{-moz-opacity: 0.5; -webkit-opacity: 0.5; -ms-opacity: 0.5; -o-opacity: 0.5; opacity: 0.5; -moz-transition: opacity 0.4s; -webkit-transition: opacity 0.4s; -o-transition: opacity 0.4s;}
