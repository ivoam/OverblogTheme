            .footer .flickrBody img {-moz-box-shadow: 0 1px 3px #000; -webkit-box-shadow: 0 1px 3px #000; -ms-box-shadow: 0 1px 3px #000; -o-box-shadow: 0 1px 3px #000; box-shadow: 0 1px 3px #000; display: block; height: 50px; width: 50px;}
