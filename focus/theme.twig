<!DOCTYPE html>
<html lang="{{ Blog.Lang|default('en') }}">
<head>
	<!-- shortcut:[Meta] -->
	<!-- title, description, keyword, robots -->
	{% if isSingle%}
		{% list Posts %}
			<title>{% if Post.Title is not empty %}{{ Post.Title }} - {% elseif Post.Snippet is not empty %}{{ Post.Snippet|truncate(80) }} - {% endif %}{{Blog.Title}}</title>
			<meta name="description" content="{{ Post.Snippet }}" />
			<meta name="keywords" content="{% list Post.Tags %}{{ Tag.Title }}{% if loop.last == false %}, {% endif %}{% endlist %}" />
			<meta name="robots" content="index,follow" />
		{% endlist %}
	{% elseif isTag and Tag is not empty %}
		<title>{{ Tag.Title|title }} - {{Blog.Title}}</title>
		<meta name="description" content="{{ Blog.Description }}" />
		<meta name="keywords" content="{{ Tag.Title|title }}" />
		<meta name="robots" content="index,follow" />
	{% elseif isSearch and Search is not empty %}
		<title>{{ Search|title }} - {{Blog.Title}}</title>
		<meta name="description" content="{{ Blog.Description }}" />
		<meta name="keywords" content="{{ Search|title }}" />
		<meta name="robots" content="noindex" />
	{% else %}
		<title>{{Blog.Title}}</title>
		<meta name="description" content="{{ Blog.Description }}" />
		<meta name="robots" content="index,follow" />
	{% endif %}
	<meta http-equiv="content-type" content="text/html; charset=utf-8" />


	<!-- Social setting : Facebook, G+ .. -->
	<meta property="og:site_name" content="{{ Blog.Title }}" />
	<meta property="og:type" content="blog" />
	<meta property="fb:app_id" content="{% if Custom('facebook_appid') is not empty %}{{ Custom('facebook_appid') }}{% endif %}" />
	<meta itemprop="name" content="{{ Blog.Title }}" />

	{% if isSingle %}
		{% list Posts %}
			<meta property="og:title" content="{% if Post.Title is not empty %}{{ Post.Title }} - {% elseif Post.Snippet is not empty %}{{ Post.Snippet|truncate(80) }}{% endif %}"/>
			<meta property="og:description" content="{{ Post.Snippet }}" />
			<meta itemprop="description" content="{{ Post.Snippet }}" />
			<meta property="og:url" content="{{ Post.Permalink }}" />
			{% if Post.Cover %}
				<meta property="og:image" content="{{ Post.Cover }}"/>
				<meta itemprop="image" content="{{ Post.Cover }}" />
			{% endif %}
		{% endlist %}
		<meta property="og:type" content="overblogkiwi:see" />
	{% else %}
		<meta property="og:title" content="{{ Blog.Title }}"/>
		<meta property="og:description" content="{{ Blog.Description }}" />
		<meta itemprop="description" content="{{ Blog.Description }}" />
		{% if Blog.Avatar %}
		<meta property="og:image" content="{{ Blog.Avatar }}"/>
		<meta itemprop="image" content="{{ Blog.Avatar }}" />
		{% else %}
			{% list Posts %}
			{% if loop.index == 1 and Post.Cover %}
		<meta property="og:image" content="{{ Post.Cover }}"/>
		<meta itemprop="image" content="{{ Post.Cover }}" />
			{% endif %}
			{% endlist %}
		{% endif %}

		{% if isSearch %}
			<meta property="og:type" content="overblogkiwi:search" />
		{% else %}
			<meta property="og:type" content="overblogkiwi:visit" />
		{% endif %}
	{% endif %}

	<!-- shortcut:[Options] -->
	<!-- options -->
		<meta name="constant:displayArticlesLimit" content="12" />

	<meta name="color:primary_color:Primary color" default="#ef8944" />
	<meta name="image:background_image:Custom Background image" default="http://assets.over-blog-kiwi.com/themes/43/images/background.jpg" />
	<meta name="image:post_cover:Default post cover" default="http://assets.over-blog-kiwi.com/themes/43/images/default-cover.jpg" />

	<meta name="list:background_overlay_type:Overlay type" items="None, Default" default="Default" />

	<meta name="string:nav_1_title:Title menu 1" group="Navigation" />
	<meta name="string:nav_1_link:Link menu 1" group="Navigation" />
	<meta name="string:nav_2_title:Title menu 2" group="Navigation" />
	<meta name="string:nav_2_link:Link menu 2" group="Navigation" />

	<meta name="string:wording_tagline_1:Tagline line 1" default="Hi, my name is John Doe" group="Wordings" />
	<meta name="string:wording_tagline_2:Tagline line 2" default="All the amazing pictures you love" group="Wordings" />
	<meta name="string:wording_footer:Footer" default="Focus &copy; 2012" group="Wordings" />

	<meta name="boolean:disable_search:Disable search" group="Widget" />
	<meta name="boolean:disable_page:Disable pages" group="Widget" />
	<meta name="boolean:disable_twitter:Disable Twitter widget" group="Widget" />

	<meta name="string:facebook_account:Enter your Facebook username" default="overblog" group="Social" />
	<meta name="string:twitter_account:Enter your Twitter username" default="overblog" group="Social" />

	<meta name="list:comment:Comment service" items="OverBlog, Facebook, Disqus" default="OverBlog" group="Comment" />
	<meta name="string:comment_disqus:Disqus account (Mandatory)" group="Comment" />

	<meta name="string:facebook_appid:Enter your FaceBook App ID" group="Settings" />
	<meta name="string:google_analytics:Enter your Google Analytics ID" group="Settings" />
	<meta name="string:google_webmaster_id:Enter your Google Webmaster Tools ID" group="Settings" />
	<meta name="string:msn_webmaster_id:Enter your MSN Webmaster Tools ID" group="Settings" />

	<!-- shortcut:[Includes] -->
	<!-- favicon -->
	{% if Blog.FaviconUrl is not empty %}
		<link rel="icon" type="image/png" href="{{ Blog.FaviconUrl }}" />
	{% else %}
		<link rel="shortcut icon" type="image/x-icon" href="http://fdata.over-blog.net/99/00/00/01/img/favicon.ico" />
		<link rel="icon" type="image/png" href="http://fdata.over-blog.net/99/00/00/01/img/favicon.png" />
		<link rel="apple-touch-icon" href="http://fdata.over-blog.net/99/00/00/01/img/mobile-favicon.png" />
	{% endif %}

	<!-- SEO -->
	{% if Custom('google_webmaster_id') is not empty %}
		<meta name="google-site-verification" content="{{ Custom('google_webmaster_id') }}" />
	{% endif %}
	{% if Custom('msn_webmaster_id') is not empty %}
		<meta name="msvalidate.01" content="{{ Custom('msn_webmaster_id') }}" />
	{% endif %}

	<!-- includes -->
	<link rel="alternate" type="application/rss+xml" title="RSS" href="{{ Blog.RssUrl }}" />

	<!-- shortcut:[Styles] -->
	<style type="text/css">
			 /**
			 * Fonts import
			 */
			@import url(http://fonts.googleapis.com/css?family=Josefin+Slab:400,700|Open+Sans:300,400,700);

			/**
			 * YUI 3.4.1 (build 4118)
			 * Copyright 2011 Yahoo! Inc. All rights reserved.
			 * Licensed under the BSD License.
			 * http://yuilibrary.com/license/
			 */
			html {color:#000;background:#FFF}body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td {margin:0;padding:0}table {border-collapse:collapse;border-spacing:0}fieldset,img {border:0}address,caption,cite,code,dfn,em,strong,th,var {font-style:normal;font-weight:normal}ol,ul {list-style:none}caption,th {text-align:left}h1,h2,h3,h4,h5,h6 {font-size:100%;font-weight:normal}q:before,q:after {content:''}abbr,acronym {border:0;font-variant:normal}sup {vertical-align:text-top}sub {vertical-align:text-bottom}input,textarea,select {font-family:inherit;font-size:inherit;font-weight:inherit}input,textarea,select {*font-size:100%}legend {color:#000}

			 /**
			 * Theme
			 */

			/*
			 * Picture under Creative commons
			 * http://www.flickr.com/photos/ericmay/4817484054/
			 */
			html {
				background: url(
				{% if Custom('background_image') is not empty %}
					{{ Custom('background_image') }}
				{% endif %}) no-repeat center center fixed;
				-webkit-background-size: cover;
				   -moz-background-size: cover;
				     -o-background-size: cover;
				        background-size: cover;
			}
			body {
				font-family: 'Open Sans', sans-serif;
				font-size: 12px;
				font-weight: normal;
				-webkit-font-smoothing: subpixel-antialiased;
				text-rendering: geometricPrecision;
				background: none;
			}

			a, a:focus, a:visited, a:hover {
				text-decoration: none;
				-webkit-transition: color 300ms ease-in;
				   -moz-transition: color 300ms ease-in;
						-ms-transition: color 300ms ease-in;
						 -o-transition: color 300ms ease-in;
								transition: color 300ms ease-in;
			}

			.clearboth {
				clear: both;
			}

			.header ,
			.main,
			.footer
			 {
				width: 960px;
				margin: auto;
				position: relative;
				z-index: 2;
			}

			.mask {
				background-color: transparent;
				position: fixed;
				top: 0;
				left: 0;
				bottom: 0;
				right: 0;
				z-index: -1;

				{% if Custom('background_overlay_type') == 'Default' or Custom('background_overlay_type') is empty %}
				background-image: url('http://assets.over-blog-kiwi.com/themes/43/images/mask.png');
				{% endif %}
			}

			/* Header */
			.header {
				background: #000; /* IE < 9 */
				background: rgba(0, 0, 0, 0.7);
				margin-top: 20px;
				margin-bottom: 20px;
				border-top-right-radius: 10px;
				border-top-left-radius: 10px;

				height: 40px;
				padding-top: 15px;
			}

			.header .logo {
				float: left;
				margin-left: 15px;
				width: 650px;
				height: 28px;
				overflow: hidden;
				line-height: 28px;
			}
			.header .logo a {
				color: white;
				font-size: 25px;
				font-family: 'Josefin Slab', serif;
				font-weight: 700;
				font-style: italic;
			}
			.header .menu {
				float: right;
				margin-right: 15px;
				margin-top: 3px;
			}
			.header .menu ul {
				list-style: none;
			}
			.header .menu ul li {
				display: inline-block;
				margin-left: 10px;
			}
			.header .menu ul li a {
				color: #999999;
				font-family: 'Open Sans', sans-serif;
				font-size: 14px;
			}

			.header .menu ul li:hover a,
			.header .menu ul li a :hover {
				color: #FFFFFF;
			}

			/* Main */
				/* Tagline */
				.main .tagline {
					text-align: left;
					position: relative;
				}
				.main .tagline h2 {
					font-family: 'Josefin Slab', serif;
					font-size: 80px;
					color: #FFFFFF;
					font-weight: 700;
					letter-spacing: -2px;
					position: relative;
					text-shadow: 2px 2px rgba(0, 0, 0, 0.5);
				}
				.main .tagline h3 {
					font-family: 'Open Sans', sans-serif;
					font-size: 32px;
					font-style: normal;
					font-weight: 300;
					color: #D8D8D8;
					position: relative;
				}
				.main .tagline span {
					color: #ef8944;
				}

				/* Blog*/
				.main .blog {
					position: relative;
				}

				/* Tags */
				.main .blog .posts-tags,
				.main .blog .index {
					list-style: none;
					margin-top: 20px;
				}
				.main .blog .posts-tags li {
					display: inline-block;
					padding-left: 5px;
					padding-right: 5px;
					background: rgba(0,0,0,0.5);
					border-radius: 4px;

					-webkit-transition: background 300ms ease-in;
					   -moz-transition: background 300ms ease-in;
							-ms-transition: background 300ms ease-in;
							 -o-transition: background 300ms ease-in;
									transition: background 300ms ease-in;
				}

				.main .blog .posts-tags li:hover {
					background: #ef8944;
				}

				.main .blog .posts-tags li a {
					color: #FFFFFF;
					font-size: 12px;
					font-style: normal;
					font-weight: bold;
				}

				/* Index content */
				.main .blog .index li.error {
					width: 940px;
					padding: 10px;
					background: #FEFEFE;
				}

				.main .blog .index li.error h4{
					font-size: 1.3em;
				}

				.main .blog .index li.post {
					float:left;
					margin-left: 10px;
					margin-right: 10px;
					margin-bottom: 10px;
					margin-top: 10px;
					width: 220px;
					height: 220px;
					overflow: hidden;
					background: #FEFEFE;
					position: relative;
					-webkit-box-shadow: 0 0 10px rgba(0, 0, 0, 0.5);
					   -moz-box-shadow: 0 0 10px rgba(0, 0, 0, 0.5);
									 box-shadow: 0 0 10px rgba(0, 0, 0, 0.5);

				}

				.main .blog .index li.post:hover {
					-webkit-box-shadow: 0 0 10px rgba(0, 0, 0, 0.8);
					   -moz-box-shadow: 0 0 10px rgba(0, 0, 0, 0.8);
									box-shadow: 0 0 10px rgba(0, 0, 0, 0.8);
				}

				.main .blog .index li .thumb img{
					cursor: pointer;
				}

				.main .blog .index li .thumb img{
					cursor: pointer;
					position: relative;
					z-index: 2;

					-webkit-transition: all 500ms cubic-bezier(0.420, 0.000, 1.000, 1.000);
					   -moz-transition: all 500ms cubic-bezier(0.420, 0.000, 1.000, 1.000);
							-ms-transition: all 500ms cubic-bezier(0.420, 0.000, 1.000, 1.000);
							 -o-transition: all 500ms cubic-bezier(0.420, 0.000, 1.000, 1.000);
									transition: all 500ms cubic-bezier(0.420, 0.000, 1.000, 1.000); /* ease-in */

					-webkit-transition-timing-function: cubic-bezier(0.420, 0.000, 1.000, 1.000);
					   -moz-transition-timing-function: cubic-bezier(0.420, 0.000, 1.000, 1.000);
							-ms-transition-timing-function: cubic-bezier(0.420, 0.000, 1.000, 1.000);
							 -o-transition-timing-function: cubic-bezier(0.420, 0.000, 1.000, 1.000);
									transition-timing-function: cubic-bezier(0.420, 0.000, 1.000, 1.000); /* ease-in */
				}

				.main .blog .index li:hover .thumb img{
				-webkit-transform: translateY(100px);
				   -moz-transform: translateY(100px);
					-ms-transform: translateY(100px);
					 -o-transform: translateY(100px);
						transform: translateY(100px);

				-webkit-transform-origin: 0% 0%;
				   -moz-transform-origin: 0% 0%;
					-ms-transform-origin: 0% 0%;
					 -o-transform-origin: 0% 0%;
						transform-origin: 0% 0%;

				-webkit-transition: all 700ms cubic-bezier(0.735, 0, 0.310, 1); /* older webkit */
				-webkit-transition: all 700ms cubic-bezier(0.735, -0.025, 0.310, 1.605);
				   -moz-transition: all 700ms cubic-bezier(0.735, -0.025, 0.310, 1.605);
					-ms-transition: all 700ms cubic-bezier(0.735, -0.025, 0.310, 1.605);
					 -o-transition: all 700ms cubic-bezier(0.735, -0.025, 0.310, 1.605);
						transition: all 700ms cubic-bezier(0.735, -0.025, 0.310, 1.605); /* custom */

				-webkit-transition-timing-function: cubic-bezier(0.735, 0, 0.310, 1); /* older webkit */
				-webkit-transition-timing-function: cubic-bezier(0.735, -0.025, 0.310, 1.605);
				   -moz-transition-timing-function: cubic-bezier(0.735, -0.025, 0.310, 1.605);
					-ms-transition-timing-function: cubic-bezier(0.735, -0.025, 0.310, 1.605);
					 -o-transition-timing-function: cubic-bezier(0.735, -0.025, 0.310, 1.605);
						transition-timing-function: cubic-bezier(0.735, -0.025, 0.310, 1.605); /* custom */
				}

				.main .blog .index li .meta {
					height: 150px;
					overflow: hidden;
					padding: 5px;
					position: absolute;
					top: 0;
					width: 210px;
					z-index: 1;
				}
				.main .blog .index li .meta .title {
					font-family: 'Open Sans',sans-serif;
					font-size: 16px;
					font-weight: 300;
					height: 25px;
					line-height: 25px;
					overflow: hidden;
					width: 220px;
				}

				.main .blog .index li .meta .title a {
					color: #000000;
				}
				.main .blog .index li .meta .title a:hover {
					color: #ef8944;
				}

				.main .blog .index li .meta .tags {
					margin-bottom: 14px;
				}

				.main .blog .index li .meta .tags li {
					display: inline-block;
				}

				.main .blog .index li .meta .tags li,
				.main .blog .index li .meta .date,
				.main .blog .index li .meta .readmore {
					font-family: 'Open Sans',sans-serif;
					font-size: 14px;
					font-weight: 300;
					color: #AAA;
				}

				.main .blog .index li .meta .tags li {
					font-size: 12px;
				}

				.main .blog .index li .meta .date {
					float: left;
				}

				.main .blog .index li .meta .readmore {
					float: right;
				}
				.main .blog .index li .meta .readmore a {
					color: #ef8944;
				}

				/* Pagination */
				.main .navigation {
					display: block;
					margin: 20px auto 50px;
					clear: both;
				}
				.main .navigation a {
					display: block;
					padding: 4px 6px;
					background: #000; /* IE < 9 */
					background: rgba(0, 0, 0, 0.7);
					color: #FFF;

					-webkit-transition: all 700ms linear; /* older webkit */
					   -moz-transition: all 700ms linear;
						-ms-transition: all 700ms linear;
						 -o-transition: all 700ms linear;
							transition: all 700ms linear; /* custom */
				}

				.main .navigation a:hover {
					color: #ef8944;
				}
				.main .navigation .prev {
					float: left;
				}
				.main .navigation .next {
					float: right;
				}

				/* Sidebar */
				.main .blog .sidebar {
					width: 260px;
					background: #000; /* IE < 9 */
					background: rgba(0, 0, 0, 0.7);
					float: right;
					font-family: 'Open Sans', sans-serif;
					font-weight: 300;
					font-size: 12px;
				}

				.main .blog .sidebar a {
					color: #999999;
				}

				.main .blog .sidebar a:hover {
					color: #FFFFFF;
				}

				.main .blog .sidebar .item.search form {
					background: #AAA; /* IE < 9 */
					background: rgba(255, 255, 255, 0.2);
				}

				.main .blog .sidebar .item.search form input[type='text'] {
					background: none;
					width: 189px;
					color: #FFFFFF;
					border: none;
					padding-left: 4px;
					padding-right: 4px;
				}

				.main .blog .sidebar .item.search form input[type='submit'] {
					background: white;
					border: none;
					border-radius: 5px;
					cursor: pointer;
					padding: 5px;
					width: 43px;

					-webkit-transition: all 700ms linear; /* older webkit */
					   -moz-transition: all 700ms linear;
						-ms-transition: all 700ms linear;
						 -o-transition: all 700ms linear;
							transition: all 700ms linear; /* custom */
				}
				.main .blog .sidebar .item.search form input[type='submit']:hover {
					color: {{ Custom('primary_color') }};
				}

				.main .blog .sidebar .item {
					clear: both;
					padding: 10px;
					margin-bottom: 20px;
				}
				.main .blog .sidebar h3 {
					color: #FFFFFF;
					font-weight: bold;
					font-size: 14px;
					margin-bottom: 10px;
				}

				.main .blog .sidebar .posts ul li {
					float: left;
					width: 100px;
					height: 100px;
					overflow: hidden;
					margin: 10px;

					-webkit-transition: box-shadow 500ms ease-in;
					   -moz-transition: box-shadow 500ms ease-in;
						-ms-transition: box-shadow 500ms ease-in;
						 -o-transition: box-shadow 500ms ease-in;
							transition: box-shadow 500ms ease-in;
				}

				.main .blog .sidebar .posts ul li:hover {
					-webkit-box-shadow: 0 0 10px rgba(255, 255, 255, 0.7);
					   -moz-box-shadow: 0 0 10px rgba(255, 255, 255, 0.7);
						    box-shadow: 0 0 10px rgba(255, 255, 255, 0.7);
				}

				.main .blog .sidebar .item.tags ul li a,
				.main .blog .sidebar .item .follow,
				.ob-input-submit {
					display: block;
					float: left;
					padding: 0 10px;
					margin: 0 10px 10px 0;
					-webkit-border-radius: 2px;
					   -moz-border-radius: 2px;
					     -o-border-radius: 2px;
					        border-radius: 2px;
					background-color: #111111;
					color: #FFFFFF;
					line-height: 20px;

					-webkit-box-shadow: 0 1px 0 0 rgba(0, 0, 0, 0.3);
					   -moz-box-shadow: 0 1px 0 0 rgba(0, 0, 0, 0.3);
					   	 -o-box-shadow: 0 1px 0 0 rgba(0, 0, 0, 0.3);
							box-shadow: 0 1px 0 0 rgba(0, 0, 0, 0.3);
					-webkit-transition: background-color 0.1s ease-in;
					   -moz-transition: background-color 0.1s ease-in;
						 -o-transition: background-color 0.1s ease-in;
							transition: background-color 0.1s ease-in;
				}
				.main .blog .sidebar .item.tags ul li a:hover {
					background-color: #ef8944;
				}
				.main .blog .sidebar .item.tags {
					margin-bottom: 10px;
				}
				.main .blog .sidebar .item .follow {
					padding: 2px 10px 2px 25px;
					background-image: url('http://assets.over-blog-kiwi.com/themes/43/images/focus-social-sprite.png');
					background-repeat: no-repeat;
				}
				.main .blog .sidebar .item .facebook {
					width: 50px;
					margin-right: 14px;
					background-position: 5px 4px;
				}
				.main .blog .sidebar .item .twitter {
					width: 32px;
					margin-right: 14px;
					background-position: 5px -17px;
				}
				.main .blog .sidebar .item .rss {
					width: 15px;
					background-position: 5px -37px;
				}

				/* Item single */
				.main .blog div.single {
					float: left;
					width: 690px;
					border-top: 4px solid #ef8944;
					background: #FFFFFF;
					position: relative;
					top: 0px;
					left: 0px;
				}

				.main .blog div.single a {
					color: #ef8944;
				}

				.main .blog div.single a:hover {
					text-decoration: underline;
				}

				.main .blog div.single .item-cover {
					left: 0px;
					position: relative;
					top: 0px;
				}

				.main .blog div.single .item-content,
				.main .blog div.single .item-comments {
					padding-left: 40px;
					padding-right: 40px;
