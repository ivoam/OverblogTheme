</head>
<body id="top">
	<div id="fb-root"></div>
	<!-- shortcut:[Header] -->
	<div class="mask"></div>
	<header class="header">
		<!--Logo-->
		<h{% if isSingle %}2{% else %}1{% endif %} class="logo">
			<a href="{{ Blog.Url }}" title="{{ Blog.Description }}">
				{% if Blog.Title is not empty %}
					{{ Blog.Title }}
				{% else %}
					Focus
				{% endif %}
			</a>
		</h{% if isSingle %}2{% else %}1{% endif %}>

		<!-- shortcut:[Menu] -->
		<nav class="menu">
			<ul>
				<li>
					<a href="{{ Blog.Url }}">
						{{ Lang.Get('Home') }}
					</a>
				</li>
				<li>
					<a href="{{ Custom('nav_1_link') }}">
						{{ Custom('nav_1_title') }}
					</a>
				</li>
				<li>
					<a href="{{ Custom('nav_2_link') }}">
						{{ Custom('nav_2_title') }}
					</a>
				</li>
				<li>
					<a href="{{ Blog.Url }}/contact">
						{{ Lang.Get('Contact') }}
					</a>
				</li>
			</ul>
		</nav>
	</header>
	<!-- End Header -->

	<!-- shortcut:[Main] -->
	<div class="main">
		{% if isIndex and Custom('wording_tagline_1') is not empty %}
		<!-- shortcut:[Tagline] -->
		<aside class="tagline">
			<h2>
				{{ Custom('wording_tagline_1')}}
			</h2>
			<h3>
				{{ Custom('wording_tagline_2') }}
			</h3>
		</aside>
		{% elseif isSearch %}
		<aside class="tagline">
			<h3>
				{{ Lang.Get('Search for') }} "{{ Search.Keyword }} "
			</h3>
		</aside>
		{% endif %}

		<section class="blog">
			{% if isSingle or isSpecial %}
				<!-- shortcut:[Sidebar] -->
				<div class="sidebar">
					{% if Custom('disable_search') == false %}
					<div class="item search">
						<h3>{{ Lang.Get('Search') }}</h3>
						<form method="get" action="/search/">
							<input type="text" name="q" placeholder="{{ Lang.Get('Search') }}..." value="{{ Lang.Get('Search') }}..." /><input type="submit" value="{{ Lang.Get('Ok') }}" />
						</form>
					</div>
					{% endif %}

					{% if Custom('disable_page') == false %}
					<div class="item pages">
						<h3>{{ Lang.Get('Pages') }}</h3>
						<ul>
							{% list Blog.ListPages %}
							<li>
								<a href="{{ ListPage.Url }}">
									{{ ListPage.Title }}
								</a>
							</li>
							{% endlist %}
						</ul>
					</div>
					{% endif %}

					<div class="item posts">
						<h3>{{ Lang.Get('Last shoots') }}</h3>
						<ul>
							{% list Blog.Posts %}
								{% if Post.cover is not empty and loop.index <= 4 %}
								<li>
									<a href="{{ Post.PermaLink }}" class="thumb">
										<img title="{{ Post.Title }}" alt="{{ Post.Title }}" src="{{ Post.Cover | resize(100) | crop }}">
									</a>
								</li>
								{% endif %}
							{% endlist %}
						</ul>
					</div>

					{% if Custom('twitter_account') is not empty and Custom('disable_twitter') == false %}
					<div class="item">
						<h3>{{ Lang.Get('My lastest Tweets') }}</h3>
						<script charset="utf-8" src="http://widgets.twimg.com/j/2/widget.js"></script>
						<script>
						new TWTR.Widget({
						  version: 2,
						  type: 'profile',
						  rpp: 4,
						  interval: 30000,
						  width: 'auto',
						  height: 300,
						  theme: {
								shell: {
								  background: '#000000',
								  color: '#ffffff'
								},
								tweets: {
								  background: '#000000',
								  color: '#ffffff',
								  links: '{{ Custom('primary_color')|default('#ef8944') }}'
								}
						  },
						  features: {
								scrollbar: false,
								loop: true,
								live: true,
								behavior: 'default'
						  }
						}).render().setUser('{{ Custom('twitter_account') }}').start();
						</script>
					</div>
					{% endif %}

					<div class="item tags">
						<h3>{{ Lang.Get('Tags') }}</h3>
						<ul>
							{% list Blog.PostsTags(15, SORT_POSTS_NUMBER_DESC) %}
							<li>
								<a href="{{ PostsTag.Url }}"  alt="{{PostsTag.Title}}">
									{{ PostsTag.Title }}
								</a>
							</li>
							{% endlist %}
						</ul>
					</div>

					{% if Custom('display_widget_follow') == false %}
	                    <div class='item'>
	                        <h3>{{ Lang.Get('Follow us') }}</h3>
                            <ul>
                              	{% if Custom('facebook_account') is not empty %}
                                <li>
                                	<a class="follow facebook" href="http://facebook.com/{{ Custom('facebook_account') }}" title="{{ Lang.Get('Follow us on') }} Facebook" target="_blank">Facebook</a>
                                </li>
                                {% endif %}
                                {% if Custom('twitter_account') is not empty %}
                                <li>
                                	<a class="follow twitter" href="http://twitter.com/{{ Custom('twitter_account') }}" title="{{ Lang.Get('Follow us on') }} Twitter" target="_blank">Twitter</a>
                                </li>
                                {% endif %}
                                <li>
                                	<a class="follow rss" href="{{ Blog.RssUrl }}" title="{{ Lang.Get('Follow us with') }} RSS" target="_blank">RSS</a>
                                </li>
                            </ul>
	                    </div>
	                {% endif %}
				</div>
				<!-- End Sidebar -->

				{% list Posts  %}
					<!-- Post item -->
					<div class="single">
						<div class="item-cover">
							{% if Post.Cover is not empty %}
							<img src="{{ Post.Cover|resize(690,200)|crop }}" alt="{{Post.Title}}" title="{{Post.Title}}" />
							{% endif %}
						</div>
						<div class="item-content">
							<header>
								<hgroup>
									<h1 class="title">{{Post.Title}}</h1>
									<div class="meta">{{ Lang.Get('Published on') }} <time pubdate>{{ Post.Date|date("M d, Y") }}</time> {% if Post.Author is not empty %}{{ Lang.Get('Written by') }} <span class="author" rel="author">{{ Post.Author }}</span>{% endif %}</div>
									<div class="tags">{% list Post.Tags %}{% if loop.index == 1 %}<strong>{{ Lang.Get('Tags:')}}</strong> {% endif %}{% if loop.index != 1 %},&nbsp;{% endif %}<a href="{{ Tag.Url }}"  alt="{{ Tag.Title }}">{{ Tag.Title }}</a>{% endlist %}</div>
								</hgroup>
							</header>
							<div class="body">
								{{Post.Body}}
							</div>
							<div class="share">
								<div class="fb-like" data-href="{{ Post.Permalink }}" data-send="true" data-layout="button_count" data-width="150" data-show-faces="false"></div>
								<a href="https://twitter.com/share" class="twitter-share-button" data-text="{{ Post.Title }}" data-url="{{ Post.Permalink }}" data-lang="{{ Blog.Lang }}" {% if Custom('twitter_account') is not empty %}data-via="{{Custom('twitter_account')}}{% endif %}">Tweet</a>
								<g:plusone href="{{ Post.Permalink }}" size="medium"></g:plusone>
							</div>
						</div>
						{% if isSingle and isPage == false %}
				         	 <div class="item-comments">
							    {% if Custom("comment") == "Facebook" %}
							    	<div class="fb-comments" data-href="{{ Post.Permalink }}" data-num-posts="2" data-width="610"></div>
							    {% elseif Custom("comment") == "Disqus" and Custom('comment_disqus') is not empty %}
							        <div id="disqus_thread"></div>
							        <script type="text/javascript">
							        var disqus_shortname = '{{ Custom('comment_disqus') }}';

							        (function() {
							        var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
							        dsq.src = 'http://' + disqus_shortname + '.disqus.com/embed.js';
							        (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
							        })();
							        </script>
							        <noscript>Please enable JavaScript to view the <a href="http://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
							    {% else %}
							        {{Post.Comments}}
							    {% endif %}
							</div>
                        {% endif %}
						<div class="clearboth"></div>
					</div>
					<!-- End Post item -->
				{% endlist %}
				{% if isSpecial %}
					<div class="single">
						<div class="item-content">
							<header>
								<hgroup>
									<h1 class="title">{{ Lang.Get('Contact us') }}</h1>
								</hgroup>
							</header>
							<div class="body">
								{{ SpecialContent }}
							</div>
						</div>
					</div>
				{% endif %}
			{% else %}
			{# Index page #}
			<ul class="posts-tags">
				<li class="active"><a href="/">{{ Lang.Get('All') }}</a></li>
				{% list Blog.PostsTags(10, "count") %}
					<li>
						<a href="{{ PostsTag.Url }}">
							{{ PostsTag.Title|title }}
						</a>
					</li>
				{% endlist %}
			</ul>

			<!-- List posts -->
			<ul class="index">
				{% list Posts %}
					<li class="post">
						<a href="{{Post.PermaLink}}" class="thumb">
							<img title="" src="{% if Post.Cover is not empty %}{{ Post.Cover|resize(220)|crop }}{% elseif Custom('post_cover') is not empty %}{{ Custom('post_cover')|resize(220)|crop }}{% endif %}">
						</a>

						<div class="meta">
							<h3 class="title">
								<a title="{{Post.Title}}" href="{{ Post.PermaLink }}" title="{{ Lang.Get('More about') }} {% if Post.Title is not empty %}&quot;{{ Post.Title }}&quot;{% else %}{{ Lang.Get('this post') }}{% endif %}" alt="{{Post.Title}}">
								{% if Post.Title is not empty %}
									{{ Post.Title|truncate(25) }}
								{% else %}
									{{ Lang.Get('A post from') }}
									{% list Post.Sources %}
										{{ Lang.Get(Source) }}
										{% if not loop.last %}
											{{ Lang.Get(' and ') }}
										{% endif %}
									{% endlist %}
								{% endif %}
								</a>
							</h3>
							<ul class="tags">
								{% list Post.Tags %}
									{% if loop.index <= 2 %}
									<li>{% if loop.index != 1 %}, {% endif %}{{ Tag.Title }}</li>
									{% endif %}
								{% endlist %}
							</ul>
							<span class="date">{{ Post.Date|date("M d Y") }}</span>
							<span class="readmore">
								<a title="{{ Lang.Get('Read more') }}" href="{{Post.PermaLink}}">
									{{ Lang.Get('Read more') }}
								</a>
							</span>
						</div>
					</li>
					{% else %}
					<li class="error">
						{% if isSearch %}
							<h4>{{ Lang.Get('No result') }}</h4>
							{{ Lang.Get('No posts found for this expression.') }}
						{% elseif isTag %}
							<h4>{{ Lang.Get('No result') }}</h4>
							{{ Lang.Get('No posts found for this tag.') }}
						{% else %}
							<h4>{{ Lang.Get('No post') }}</h4>
							{{ Lang.Get('Sorry we haven\'t found any post.') }}
						{% endif %}
					</li>
				{% endlist %}
			</ul>
			<!-- End List posts -->

			{% endif %}
			<!-- End Content -->
			<div class="clearboth"></div>
		</section>

	<!-- Pagination -->
	{% if Navigation.PreviousPage is not empty or Navigation.NextPage is not empty or Navigation.PreviousPost is not empty or Navigation.NextPost is not empty %}
		<nav class="navigation">
		{% if Navigation.PreviousPage is not empty %}
			<a href="{{Navigation.PreviousPage}}" class="prev">&laquo; {{ Lang.Get('Previous page') }}</a>
		{% elseif Navigation.PreviousPost is not empty %}
			<a href="{{ Navigation.PreviousPost.Permalink }}" class="prev">&laquo; {% if Navigation.PreviousPost.Title is not empty %}{{ Navigation.PreviousPost.Title | truncate(25) }}{% else %}{{ Lang.Get('Previous post') }}{% endif %} </a>
		{% endif %}

		{% if Navigation.NextPage is not empty %}
			<a href="{{Navigation.NextPage}}" class="next">{{ Lang.Get('Next page') }} &raquo;</a>
		{% elseif Navigation.NextPost is not empty %}
            <a href="{{ Navigation.NextPost.Permalink }}" class="next">{% if Navigation.NextPost.Title is not empty %}{{ Navigation.NextPost.Title | truncate(25) }}{% else %}{{ Lang.Get('Next post') }}{% endif %} &raquo;</a>
        {% endif %}
		</nav>
	{% endif %}
	<!-- End Pagination -->
	</div>

	<!-- shortcut:[Footer] -->
	<footer class="footer clearboth">
		<p>{{Custom('wording_footer')}}</p>
	{% if isSingle %}
		<a class="scroll-top" href="#top">
			{{ Lang.Get('Top') }}
			<span>â–²</span>
		</a>
	{% endif %}
	</footer>
	<!--End Footer-->

	<!-- Scripts -->
	<script type="text/javascript">
		// Update month into the top bar
		(function()
		{
			$(document).ready(function(){
				$.backstretch(
					$('html').css('background-image').replace('url(','').replace(')','')
				);

				if(result = document.location.href.match(/(\/tag\/.*)(#.+)?$/gi))
				{
					$('.posts-tags li').removeClass('active');
					$('.posts-tags li').each(function(){
						if( $(this).children('a').attr('href') == result )
						{
							$(this).addClass('active');
						}
					});
				}

				{% if isSingle %}
					$(".ob-section-images a, .ob-link-img").attr("rel", "group1");
					$("a[rel=group1]").fancybox({
						'overlayShow'	: true,
						'transitionIn'	: 'elastic',
						'transitionOut'	: 'elastic',
						'type'			: 'image'
					});
				{% endif%}
			});
		})();
	</script>

